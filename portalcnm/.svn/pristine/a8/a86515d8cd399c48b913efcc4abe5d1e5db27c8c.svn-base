    font-size: 11px;
    display: block;
    position: absolute;
    transform: translate(-50%, 0);
    border-radius: 6px;
}
.rangeValue span:before{
    content: "";
    position: absolute;
    width: 0;
    height: 0;
    border-top: 5px solid rgba(3,169,244,0.7);
    border-left: 5px solid transparent;
    border-right: 5px solid transparent;
    top: 100%;
    left: 50%;

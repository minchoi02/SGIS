.popReport .pheader .sideBtn a.t01{background:#f1f1f1;}
.popReport .pheader .sideBtn a.t02{background:#3d62e8;color:#fff;border:1px solid #3d62e8;}
.popReport .pcont{width:calc( 100% - 40px );height:400px;overflow:hidden;overflow-y:auto;margin:20px;}
.popReport .pcont .tit{font-weight:bold;width:100%;height:auto;overflow:hidden;font-size:15px;}
.popReport .pcont .reportCont{width:100%;height:auto;overflow:hidden;margin:0 auto;}
.popReport .pcont .reportCont dt{width:calc( 100% - 10px );padding-left:10px;height:auto;overflow:hidden;margin:20px auto 10px auto;position:relative;}
.popReport .pcont .reportCont dt:before{content:"";width:2px;height:2px;overflow:hidden;background:#555;display:block;position:absolute;left:0;top:7px;}
.popReport .pcont .reportCont dd{width:100%;height:auto;overflow:hidden;}
.popReport .pcont .reportMap{width:100%;height:300px;overflow:hidden;}
.popReport .pcont .reportDesc{display:block;border:1px solid #ddd;font-size:12px;padding:10px;line-height:1.5em;color:#555;}
.popReport .pcont .reportChart{width:100%;height:300px;overflow:hidden;}


.dataSearchResultScrolls{width:calc( 100% - 40px );margin:10px auto 0 auto;height:auto;max-height:200px;overflow:hidden;overflow-y:auto;}
.dataSearchResult{width:100%;margin:0 auto;height:auto;overflow:hidden;}
.dataSearchResult li{width:100%;height:auto;overflow:hidden;margin-bottom:5px;}
.dataSearchResult li a{float:left;}
.dataSearchResult li span{float:left;height:25px;line-height:25px;margin-left:10px;}

.sasaList{width:100%;height:auto;overflow:hidden;}
.sasaList li{margin:20px 0 5px 0;}
.sasaList li .s01{margin-bottom:5px;font-weight:bold;font-size:13px;}
.sasaList li .s02{color:#666;font-size:13px;}
 
 .jq-selectbox .jq-selectbox__search{display:none !important;}

.uploadList li .uploadResultList{margin-top:170px;width:100%;height:auto;overflow:hidden;}
.uploadList li .uploadResultList li{border:0;float:none;color:#fff;background:url(../img/bg_uploadline.png) no-repeat left bottom;height:30px;line-height:30px;width:215px;overflow:hidden;margin:0 auto;}
.btnBox{margin:50px auto;width:100%;height:auto;overflow:hidden;text-align:center;}
.btnBox a{font-size:12px;display:inline-block;width:132px;height:36px;line-height:36px;text-align:center;border-radius:18px;background:#3d62e8;color:#fff;margin:0 2px;}

.uploadList{width:1140px;height:auto;overflow:hidden;margin:50px auto;}
.uploadList li{float:left;width:379px;height:291px;overflow:hidden;border:1px solid #e5e5e5;border-left:0;text-align:center;position:relative;}
.uploadList li:first-child{border-left:1px solid #e5e5e5;width:378px;}
.uploadList li .ico{margin-top:50px;}
.uploadList li .btnUploadLink{display:block;width:228px;height:44px;border:1px solid #d5d5d5;line-height:44px;text-align:center;border-radius:22px;font-size:15px;color:#000;margin:10px auto;}

@import url(common.css); 

select{   -webkit-appearance: none;    -moz-appearance: none;    -ms-appearance: none;    -o-appearance: none;    appearance: none;}


.wrap{height:auto; background:#fff; width:100%; min-width:1200px; position:relative; }

/*login */
.loginWrap{width:100%; background:url(../img/login_bg.png)repeat-x 0 0; }
.login{width:1200px; margin:0 auto; }
.loginArea{width:100%; height:298px;  box-sizing:border-box; overflow:hidden;}
.loginArea ul{margin:30px 0 0 65px;}
.loginArea li{margin-top:6px;}
.loginArea li:first-child{margin-top:0;}
.loginArea li select{width:420px; height:45px; border-radius:30px; border:1px solid #d5d5d5;  background:url(../img/select_bg.png)no-repeat top right; background-color:#fff;font-size:12px; color:#000; box-sizing:border-box;  padding-left:40px;  line-height:45px; }
.loginArea li input{width:420px; height:45px; border-radius:30px; border:1px solid #d5d5d5; font-size:12px; color:#000; box-sizing:border-box;  padding-left:40px;  line-height:45px;}
.loginArea li button{width:420px; height:45px; border-radius:30px; background:#3d62e8; font-size:17px; color:#fff; text-align:center; border:0;}
.loginArea li button:hover{opacity:0.5}
.loginBember{ margin:0 0 0 65px ; width:420px; }
.loginBember li {float:left; width:105px; background:url(../img/li_bg.png)no-repeat top right; font-size:12px; text-align:center; margin-top:0;}
.loginBember li:last-child{background:none;}
.loginBember li a{font-size:12px; text-align:center; color:#525252;}
.loginBember li a:hover{opacity:0.5}


/*main */

.topWrap{height:90px; border-bottom:1px solid #e5e5e5; width:100%; overflow:hidden; z-index:200;position:relative;background:#fff; overflow:hidden;}
.topBox{ width:100%; position:absolute;;}
.top{width:1200px; margin:0 auto; height:90px; min-width:1200px; display: flex; flex-direction: row; justify-content: space-between; }
.toplogo{vertical-align:middle; line-height:90px;}
.toplogo img{vertical-align:middle;}

.topAdmin{padding-top:27px;}
.topAdmin ul{width:240px; height:35px; border-radius:30px;  box-sizing:border-box; border:1px solid #e5e5e5;  padding:7px 15px; float:left; overflow:hidden;}
.topAdmin li{float:left; margin-right:6px; height:20px; line-height:20px;}
.topAdmin li:last-child{ margin-left:10px; }
.topAdmin li.admin{background:url(../img/icon_admin01_n.png)no-repeat 0 0; width:20px; height:20px; }
.topAdmin li.admin.on{background:url(../img/icon_admin01_s.png)no-repeat 0 0; width:20px; height:20px; }
.topAdmin li.group{background:url(../img/icon_admin02_n.png)no-repeat 0 0; width:20px; height:20px; }
.topAdmin li.group.on{background:url(../img/icon_admin02_s.png)no-repeat 0 0; width:20px; height:20px; }
.topAdmin button{width:82px; height:35px; border:1px solid #e5e5e5; box-sizing:border-box; font-size:12px; text-align:center; border-radius:30px; float:left; margin-left:10px;}
.topAdmin button:hover{opacity:0.5;}

.topMenu li{line-height:90px; width:126px; box-sizing:border-box;  float:left; height:90px;}
.topMenu li:hover , .topMenu li.on{border-bottom:3px solid #3d62e8; }
.topMenu li a{ font-size:18px; color:#000; text-align:center;  width:126px; display: inline-block; font-weight:600;}

.subMenuWrap{width:100%; height:214px; position:absolute; left:0; top:91px;  z-index:1; background:#fff; border-bottom:1px  solid #e5e5e5; box-sizing:border-box; display:none;}
.subMenu{width:1200px; margin:0 auto; box-sizing:border-box; padding:20px 289px;}
.subMenu ul{float:left; width:126px;}
.subMenu li{line-height:30px;  }
.subMenu li a{font-size:12px; color:#6e6e6e; }
.subMenu li a:hover{opacity:0.5;}


.dashbordWrap{width:100%; height:57px; border-bottom:1px solid #e5e5e5; line-height:57px; position:relative;}

.dashbordTile{width:1200px; min-width:1200px; margin:0 auto; display: flex; flex-direction:row; justify-content:space-between; position:relative;}
.dashbordTile h1{font-size:15px; font-weight:500; color:#000;}
.dashbordTile button{width:22px; height:22px; background:url(../img/icon_setting_n.png)no-repeat 0 0; margin-top:20px;}
.dashbordTile button:hover{background:url(../img/icon_setting_s.png)no-repeat 0 0; }

.dashbordSet{position:absolute; top:-1px; right:40px; border:1px solid #e5e5e5; box-sizing:border-box; width:260px; height:296px; background:#fff; display:none; z-index:2}
.dashbordSet h1{width:100%; border-bottom:1px solid #e5e5e5; height:57px; line-height:57px; font-size:15px; color:#000; text-align:center;}
.dashbordSet ul{margin-top:22px;}
.dashbordSet li{font-size:12px; color:#000; line-height:25px; margin-top:8px; margin-left:20px;}
.dashbordSet li input{margin-right:20px;}
.dashbordSet li input[type=checkbox]:not(old){  margin:0; padding:0; opacity:0; background:url(../img/checkbox_n.png) no-repeat 0 0; width:0; }
.dashbordSet li input[type=checkbox]:not(old) + label{display: inline-block; text-align: left; height:25px; padding-left: 40px; background: url(../img/checkbox_n.png) no-repeat 0 0; }
.dashbordSet li input[type=checkbox]:not(old):checked + label {background: url(../img/checkbox_s.png) no-repeat 0 0;}


.dashbordArea{width:100%; height:268px; background:#f1f1f1; border-bottom:1px solid #e5e5e5; }
.dashbord{width:1200px; margin:0 auto; display: flex; flex-direction:row; justify-content:space-between; box-sizing:border-box; padding-top:42px; }
.dashbordBox{width:calc( 100% - 20px ); height:182px; box-sizing:border-box; border:1px solid #e5e5e5; padding:0 12px; background:#fff; margin:0 10px;}
.dashbordBox h1{width:100%; border-bottom:1px solid #e5e5e5; font-size:15px; color:#000; text-align:center; line-height:53px; font-weight:100;}
.boxInfo{width:100%; margin-top:15px; font-size:80px; text-align:right;}
.boxInfo.green{color:#92d050}
.boxInfo.sky{color:#00bbd3}
.boxInfo.yellow{color:#fec006}
.boxInfo.pink{color:#ff7fff}
.boxInfo.blue{color:#00aeff}
.boxInfo.red{color:#f96868}
.boxInfo span{font-size:15px; color:#a3a3a3; margin-right:10px; }


.contentWrap{width:100%; padding:50px 0; border-bottom:1px solid #e5e5e5; overflow:hidden; height:360px;}
.content{width:1200px; min-width:1200px;  margin:0 auto;  display: flex; flex-direction:row; justify-content:space-between;}
.box01,.box02,.box03{width:364px; height:360px; position:relative;}

.contentBox{width:364px; height:360px; border:1px solid #e5e5e5; box-sizing:border-box; position:absolute; text-align:center; border-radius:5px; z-index:1; left:0; top:0;}
.contentBox img{vertical-align:middle; text-align:center; margin-top:10px;}
.contentBox h1{font-size:18px; color:#000; font-weight:500; margin-top:50px; text-align:center;}
.contentBox p{text-align:center; font-size:17px; color:#000; margin-top:20px;}
.contentBox.sky{background:#e5f8fb}
.contentBox.sky p span{font-weight:500; color:#085fd1;}
.contentBox.yellow{background:#fff9e5}
.contentBox.yellow p span{font-weight:500; color:#fe5621;}
.contentBox.green{background:#f4faed}
.contentBox.green p span{font-weight:500; color:#2e7f31;}

.contentBox_over{width:364px; height:360px; border:1px solid #e5e5e5; box-sizing:border-box; position:absolute; text-align:center; border-radius:5px;  z-index:0; left:0; top:0;}
.contentBox_over img{vertical-align:middle; text-align:center; margin-top:10px;}
.contentBox_over h1{font-size:18px; color:#fff; font-weight:500; margin-top:50px; text-align:center;}

.contentBox_over ul{width:290px; margin:0 auto; margin-top:40px;}
.contentBox_over li{width:100%; border-radius:30px; background:rgba(255,255,255,0.2); height:40px; margin-top:10px; line-height:40px; }
.contentBox_over li:first-child{margin-top:0;}
.contentBox_over li a{text-align:center; width:100%; display:inline-block; font-size:15px; color:#fff;}
.contentBox_over li a:hover{opacity:0.5;}

.contentBox_over.sky{background:url(../img/cont01_bg.png)no-repeat 0 0;}
.contentBox_over.yellow{background:url(../img/cont02_bg.png)no-repeat 0 0;}
.contentBox_over.green{background:url(../img/cont03_bg.png)no-repeat 0 0;}

.noticeWrap{width:1200px; min-width:1200px; display: flex; flex-direction:row; justify-content:space-between; margin:0 auto; padding:50px 0; overflow:hidden;}
.notice{width:585px; overflow:hidden;}
.notice h1{font-weight:500; font-size:18px; color:#000; padding-left:12px; float:left; line-height:35px;overflow:hidden; }
.notice button{ height:35px; border:1px solid #e5e5e5; box-sizing:border-box; padding:0 20px;  font-size:12px; color:#333333; float:right; border-radius:35px; }
.notice button img{margin-right:10px;}
.notice button:hover{opacity:0.5;}

.notice table{width:100%; border-top:2px solid #393c4a; margin-top:13px; clear:both; float:left;}
.notice table td{height:53px; border-bottom:1px solid #e5e5e5; font-size:15px; background:url(../img/table_list.png)no-repeat 20px 25px; padding-left:30px; }
.notice table td:last-child{background:none;}


.footerWrap{width:100%; height:100px; background:#4e5258; }
.footer{width:1200px; min-width:1200px; margin:0 auto; box-sizing:border-box; padding:20px 0; }
.footer p{color:rgba(255,255,255,0.8); font-size:12px;}


/*sub */
.subConentWrap{width:1200px; margin:0 auto; height:auto; padding-bottom:50px; min-height:700px;}
.subTitleWrap{height:170px; width:100%; overflow:hidden;}
.subTitle{width:100%;height:auto;overflow:hidden;float:left; margin-top:50px; position:relative;}
.subTitle h1{margin-top:10px;font-size:30px; font-weight:100; color:#000; }
.subTitle h2{font-size:12px; font-weight:100; color:#333333; margin-top:5px;}
.subTitle .home{background:url(../img/ico_home01.png) no-repeat left center;padding-left:18px;color:#adadad;font-family:'dotum';}
.subTitle .btnUpload{position:absolute;right:0;top:40px;float:left;height:40px;padding:0 20px 0 40px ;background:#3d62e8 url(../img/ico_download.png) no-repeat 10px center;color:#fff;border-radius:20px;line-height:40px;}

.see{float:right; margin-top:110px;}
.see select{width:109px; height:35px; border-radius:30px; border:1px solid #d5d5d5;  background:url(../img/select_bg.png)no-repeat 70px -6px; background-color:#fff;font-size:12px; color:#000; box-sizing:border-box;  padding-left:15px;  line-height:35px; }


.tabWrap{height:104px; border-bottom:1px solid #e5e5e5; border-top:1px solid #e5e5e5; box-sizing:border-box; padding:24px 0;}
.tab ul{display: flex; flex-direction:row; justify-content:space-between;}
.tab li{width:400px; height:56px;box-sizing:border-box; border:1px solid #e5e5e5;}
.tab li a{font-size:15px; color:#000; text-align:center; display:inline-block; width:100%; line-height:56px;}
.tab li:hover, .tab li.on{background:#3d62e8; border:none}
.tab li a:hover, .tab li.on a{color:#fff; }

.info{margin:80px 0;}
.info h1{font-weight:500; font-size:25px; color:#000; height:50px; padding-left:10px;}
.info ul{}
.info li{background:url(../img/table_list.png)no-repeat 15px 15px; font-size:15px; line-height:35px;  color:#000; padding-left:30px;}
.info dt{background:url(../img/table_list.png)no-repeat 15px 15px; font-size:15px; line-height:35px;  color:#000; padding-left:30px;}
.info dd{font-size:15px; color:#000; line-height:25px; padding-left:30px;}

.siteMap{width:100%; display: flex; flex-direction:row; justify-content:space-between;  }
.site{width:185px; height: auto; }
.site h1{font-size:20px; font-weight:500;  width:100%; height:45px; border-bottom:2px solid #3d62e8; text-align:center; color:#000;}
.site dt ,.site li{background:url(../img/table_list.png)no-repeat 15px 20px; font-size:15px; line-height:45px;  color:#000; padding-left:30px; box-sizing:border-box; width:100%; border:1px solid #e5e5e5; background-color:#f1f1f1; margin-top:20px;}
.site dd{font-size:12px; color:rgba(0,0,0,0.8); line-height:25px; padding-left:30px; }

.QA{ width:100%; }
.QA button{background:#3d62e8; width:130px; height:35px; border-radius:35px; text-align:center; color:#fff; font-size:15px;}
.QA button:hover{opacity:0.5;}
.QABtn{width:100%; display:flex; flex-direction:row; justify-content:center; margin-top:50px;}

.QA table{width:100%; border-top:2px solid #3d62e8; }
.QA table th{border-right:1px solid #e5e5e5; border-bottom:1px solid #e5e5e5; font-size:15px; font-weight:500; text-align:center; height:58px; color:#000;}
.QA table th:last-child{border-right:0}
.QA table td{border-right:1px solid #e5e5e5; border-bottom:1px solid #e5e5e5; font-size:15px; ; text-align:center; height:58px;color:#000}
.QA table td:last-child{border-right:0}
.QA table td.list{text-align:left; padding:0 30px;}

.QA table td input[type="text"]{border:1px solid #e5e5e5; height:35px; box-sizing:border-box; color:#000;  padding:0 20px; width:100%;}
.QA table td input::-ms-input-placeholder { color: #e5e5e5; }
.QA table td input::-webkit-input-placeholder { color: #e5e5e5; } 
.QA table td input::-moz-placeholder { color: #e5e5e5; }
.QA table td textarea{width:100%; box-sizing:border-box; padding:30px; font-size:15px; color:#000; height:100px; border:1px solid  #e5e5e5; margin:20px 0; text-align:left; vertical-align:top;}

.QA table td div{width:100%; display:flex; flex-direction:row; justify-content:space-between;  box-sizing:border-box; padding:10px 30px; box-sizing:border-box;}
.QA table td div button.btn{width:150px; height:35px; border:1px solid #e5e5e5; box-sizing:border-box; text-align:center; font-size:12px; color:#000; background:#fff;}
.QA table td div.list{width:84%; border:1px solid #e5e5e5; }
.QA table td div.list ul{}
.QA table td div.list li{font-size:12px; color:#000;  padding-top:5px;}
.QA table td div.list li span{width:25px; height:25px; margin-right:10px; display:inline-block; vertical-align:middle;}
.QA table td div.list li span:hover{opacity:0.5;}

.QA table.reading{}
.QA table.reading th{text-align:left; border-right:0; padding-left:20px; font-weight:550;}
.QA table.reading td{ text-align:left; height:auto; padding: 40px;}
.QA table.reading td.bg{background:#f8f8f8;}









.page{overflow:hidden; width:530px; margin:0 auto; margin-top:30px;}
.page li{float:left; height:35px; width:35px; border:1px solid #e5e5e5; box-sizing:border-box; }
.page li a{text-align:center; line-height:35px; display:inline-block; width:100%;}
.page li:hover, .page li.on{border-bottom:2px solid #3d62e8; }
.page li a:hover, .page li.on a{color:#3d62e8; font-weight:600}


.faq{width:100%; border-top:2px solid #3d62e8; overflow:hidden;}
.faqBox{display: flex; flex-direction:row; justify-content:space-between; box-sizing:border-box; padding:0 30px; border-bottom:1px solid #e5e5e5; width:100%; height:56px; }
.faqBox h1{font-weight:100; font-size:15px; color:#000;  line-height:35px; padding-top:10px; }
.faqBox h1 span{width:35px; height:35px; background:#879099; border-radius:20px; font-size:15px; text-align:center; color:#fff; margin-right:30px; display:inline-block;}
.faqBox button{width:35px; height:35px; background:url(../img/btn_down.png)no-repeat center center; margin-top:10px;}
.box{box-sizing:border-box; padding:30px 100px; border-bottom:1px solid #e5e5e5; height:auto; font-size:13px; color:#000;line-height:30px; display:none;}


.container{width:100%;height:auto;overflow:hidden;}
.dataStepBox{width:100%;height:102px;overflow:hidden;border-top:1px solid #e5e5e5;border-bottom:1px solid #e5e5e5;}
/*
.dataStepBox.step01{background:url(../img/bg_step01.jpg) no-repeat center center;}
.dataStepBox.step02{background:url(../img/bg_step02.jpg) no-repeat center center;}
.dataStepBox.step03{background:url(../img/bg_step03.jpg) no-repeat center center;} 
*/
.dataStepBox .stepList{margin:0 auto;margin-top:25px;position:relative;width:1150px;height:50px;overflow:hidden;}
.dataStepBox .stepList li{position:relative;z-index:2;float:left;width:382px;height:48px;border-radius:24px;border-top-left-radius:0;border-bottom-left-radius:0;border:1px solid #d6d4d5;background:#f2f2f2;border-left:0;}
.dataStepBox .stepList li:first-child{border-left:1px solid #d6d4d5;border-top-left-radius:24px;border-bottom-left-radius:24px;}
.dataStepBox .stepList li.etc01{position:absolute;left:300px;width:500px;top:0;z-index:1;display:none;}
.dataStepBox .stepList li.etc02{position:absolute;left:300px;width:500px;top:0;z-index:1;}
.dataStepBox .stepList.step02 li.etc01{width:500px;left:200px;background:#3d62e8;display:block;}
.dataStepBox .stepList.step02 li.etc02{width:500px;left:500px;}
.dataStepBox .stepList.step03 li.etc01{width:500px;left:200px;background:#3d62e8;display:block;}
.dataStepBox .stepList.step03 li.etc02{width:500px;left:500px;background:#3d62e8;}
.dataStepBox .stepList li .t01{margin-top:7px;float:left;width:126px;height:33px;border-radius:17px;border:1px solid #d5d5d5;background:#fff;line-height:33px;text-align:center;font-size:12px;color:#3d62e8;margin-left:7px;}
.dataStepBox .stepList li .t02{margin-top:7px;float:left;height:35px;line-height:35px;color:#000;font-weight:bold;font-size:18px;margin-left:20px;}
.dataStepBox .stepList li .ico{margin-right:7px;margin-top:7px;float:right;width:35px;height:35px;overflow:hidden;background:url(../img/bg_stepico.png);}
 
.dataStepBox.type01 .stepList li{width:286px;}
.dataStepBox.type01 .stepList li .t01{width:64px;}
.dataStepBox.type01 .stepList li.etc01{width:630px;}
.dataStepBox.type01 .stepList li.etc02{width:630px;left:260px;}
.dataStepBox.type01 .stepList.step02 li.etc01{display:block;background:#3d62e8;left:260px;}
.dataStepBox.type01 .stepList.step02 li.etc02{left:310px;}
.dataStepBox.type01 .stepList.step03 li.etc01{display:block;background:#3d62e8;left:260px;}
.dataStepBox.type01 .stepList.step03 li.etc02{left:790px;background:#f2f2f2;width:100px;}
.dataStepBox.type01 .stepList.step04 li.etc01{display:block;background:#3d62e8;left:260px;}
.dataStepBox.type01 .stepList.step04 li.etc02{left:790px;background:#3d62e8;width:100px;}


.dataStepBox .stepList li.on{background:#3d62e8;}
.dataStepBox .stepList li.on .t01{}
.dataStepBox .stepList li.on .t02{color:#fff;}
.dataStepBox .stepList li.on .ico{background:url(../img/bg_stepico_on.png)}

.uploadDlist{width:1133px;height:auto;overflow:hidden;margin:20px auto;}

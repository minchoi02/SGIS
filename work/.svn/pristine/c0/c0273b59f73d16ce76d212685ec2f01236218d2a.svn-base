.dataBoardBox{width:600px;height:100%;overflow:hidden;background:#fff;position:absolute;right:0;top:58px;border-left: 1px solid #e5e5e5;z-index:1000;}
.dataBoardBox .dbHeader{width:100%;height:33px;overflow:hidden;background:#3d62e8;z-index:1000;}
.dataBoardBox .dbHeader span{float:left;height:33px;line-height:33px;margin-left:20px;color:#fff;font-weight:bold;}
.dataBoardBox .dbHeader a{float:right;margin:8px 10px 0 0;}
.dataBoardBox .dbChkCensus{width:100%;height:auto;overflow:hidden;z-index:990;}
.dataBoardBox .dbChkCensus .chkCensus{font-weight:bold;font-size:15px;color:#E01D1D;margin-top:15px;margin-left:20px;}
.dataBoardBox .dbChkCensus .chkCensus a{color:#E01D1D;}
.dataBoardBox .dbChkCensus .chkCensusImg{display:none;}
.dataBoardBox .dbCont .dbInfo .tit.fn{font-weight: normal;}
.dataBoardBox .dbCont .dbInfo{width:100%;/* height:82px; */overflow:hidden;border-bottom:1px solid #e5e5e5;}
.dataBoardBox .dbCont .dbInfo .tit{/* float: left;  */font-weight: bold;font-size: 15px;color: #333;padding: 5px; padding-left: 0px;text-align:left;}
.dataBoardBox .dbCont .dbInfo .tit.fn{font-weight: normal;}
.dataBoardBox .dbCont .dbInfo .tit.w80{width:80px;}
.dataBoardBox .dbCont .dbInfo .tit2{width:80px;font-weight:bold;font-size:15px;color:#333;float:left;}
.dataBoardBox .dbCont .dbInfo ul{float: right; margin: 0 20px 0 -30px;position: relative;}
.dataBoardBox .dbCont .dbInfo ul li{font-size:12px;margin-bottom:5px;color:#adadad;} 
.dataBoardBox .dbCont .scrolls{/* width:calc( 100% - 40px );padding:0 20px; */height:560px;overflow:hidden;overflow-y:scroll;}
.dataBoardBox .dbCont .dbe01{width:calc( 100% - 40px );border-top:1px solid #d5d5d5;padding-top:20px;color:#d71826;margin:0 auto;}

.dbItemList{width:calc( 100% - 40px );height:auto;overflow:hidden;;padding:0 20px;}
.dbItemList dt{width:100%;height:auto;overflow:hidden;padding:10px 0 10px 0;}
.dbItemList dt a{font-weight:bold;display:block;padding-left:20px;width:calc( 100% - 22px );height:33px;line-height:33px;overflow:hidden;border:1px solid #d5d5d5;border-radius:18px;background:#f8f8f8 url(../../img/common/ico_up01.png) no-repeat right center;}
.dbItemList dt a.on{background:#f8f8f8 url(../../img/common/ico_down01.png) no-repeat right center;}
.dbItemList dd{width:calc( 100% - 20px );height:auto;overflow:hidden;padding:10px 10px;}
.clist01{width:100%;height:auto;overflow:hidden;}
.clist01 tr{width:100%;height:auto;overflow:hidden;padding:5px 0;}
.clist01 tr .t01{width:90px;font-size:12px;color:#000;font-weight:bold;padding: 5px 0 5px 0;}
.clist01 tr .t02{font-size:12px;color:#000;padding: 5px 20px 5px 0;}
.dbItemList dd .chart{width:100%;height:300px;overflow:hidden;}
.dbItemList dd .chart.off{position:absolute;left:-2000000px;}
.dbItemList dd .e01{margin:2px 0 5px 0;}
.dbItemList dd .e02{margin:20px 0 5px 0;padding-left:10px;width:calc( 100% - 10px );height:auto;overflow:hidden;position:relative;}
.dbItemList dd .e02:before{line-height:1.5em;content:"";width:2px;height:2px;overflow:hidden;background:#555;position:absolute;left:0;top:7px;}

.analysisDataBoard{text-indent:25px;color:#fff;font-size:11px;position:absolute;z-index:1300;right:0px;top:58px;width:125px;line-height:33px;height:33px;background:#3d62e8 url(../../img/common/bg_databoard.png) no-repeat right center;}
.analysisDataBoard:hover{background:#00bcd4 url(../../img/common/bg_databoard.png) no-repeat right center;}
.analysisDataBoard.on, .analysisDataBoard.on:hover{background:transparent url(../../img/common/bg_databoard.png) no-repeat right center;}
#analysisType:hover{opacity:1;}

.listTable01{width:1140px;margin:0 auto;table-layout:fixed;border-collapse:collapse;}
.listTable01 th{height:45px;border:1px solid #c9c9c9;border-right:0;border-top:2px solid #879099;font-size:15px;color:#000;}
.listTable01 td{height:55px;border:1px solid #c9c9c9;border-right:0;font-size:12px;color:#4c4c4c;text-align:center;}
.listTable01 tr>th:first-child,
.listTable01 tr>td:first-child{border-left:0;}

#dataViewTable {display: block; overflow-x: auto; } 
.previewTd {    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    line-height: 20px;
    overflow: hidden;
    text-overflow: ellipsis;}

.listTable01.t01 th{height:52px;border-top:2px solid #879099;background:#f8f8f8;}
.listTable01.t01 td{height:47px;}

.listTable02{width:1200px;margin:0 auto;table-layout:fixed;border-collapse:collapse;}
.listTable02 th{height:45px;border:1px solid #c9c9c9;border-right:0;border-top:2px solid #879099;font-size:15px;color:#000;}
.listTable02 td{height:55px;border:1px solid #c9c9c9;border-right:0;font-size:12px;color:#4c4c4c;text-align:center;}
.listTable02 tr>th:first-child,
.listTable02 tr>td:first-child{border-left:0;}
.listTable02 td.al{text-align:left;padding:0 20px;}
.listTable02.t01{margin-top:20px;}
.listTable02.t01 td{border:1px solid #000;}
.listTable02.t01 tr>th:first-child,
.listTable02.t01 tr>td:first-child{border:1px solid #000;}
.listTable02 .inp{width:calc( 100% - 20px );padding:0 10px;height:55px;overflow:hidden;border:0;}

.listTable03{width:500px;margin:0 auto;table-layout:fixed;border-collapse:collapse;}
.listTable03 th{height:30px;border:1px solid #c9c9c9;border-top:2px solid #879099;font-size:12px;color:#000;}
.listTable03 td{height:30px;border:1px solid #c9c9c9;font-size:12px;color:#4c4c4c;text-align:center;}
.listTable03 td.al{text-align:left;padding:0 10px;}
.listTable03 td.ar{text-align:right;padding:0 10px;}
.listTable02 td .btnColumAdd{display:block;text-align:center;}
.listTable02 td .btnColumAdd span{display:block;color:#000;}
.listTable02 td .btnColumAdd:hover{opacity:.7;}
.listTable03 td .cr{color:#cb4218;}
.listTable03 td .cb{color:#1896cb;}
.listTable03 .selected{background:#f1f1f1;}

.listTable04{width:100%;margin:0 auto;table-layout:fixed;border-collapse:collapse;}
.listTable04 th{height:30px;border:1px solid #c9c9c9;background:#f1f1f1;font-size:12px;color:#000;font-weight:normal;}
.listTable04 td{height:30px;border:1px solid #c9c9c9;font-size:12px;color:#4c4c4c;text-align:left;padding:0 20px;}

.markerTooltip {margin:10px;}
.markerTooltip > .mTitle {font-size:14px;font-weight:bold;color:#3792de;}
.markerTooltip > .sTitle {font-size:12px;padding-left:5px;}
.markerTooltip > .deleteBtn{width:100%;height: 30px;background: #3d62e8;font-size: 12px;color: #fff;text-align: center;border-radius:30px;margin:10px 0 0 0;}
.markerTooltip > .deleteBtn:hover {opacity:0.7;}

.cr {color:#ff0000 !important; font-weight:bold !important;}
.cb {color:#0000ff !important; font-weight:bold !important;}

.dataTypeBtn.t03 {margin-right:295px;}
.dataTypeBtn.t04 {margin-left:295px;}
.scrollWrapper {max-height:400px;margin-bottom:20px;}

.select05 {width:160px !important;}
.downFormat, .inp {height:33px;width:280px;border:1px solid #e5e5e5;padding:0 10px;overflow:hidden;}

.preViewbtn {margin-right: 10px;height: 33px;width: 100px;overflow: hidden;border-radius: 17px;line-height: 33px;text-align: center;border: 1px solid #d5d5d5;font-size: 12px;color: #000;float: right;cursor:pointer;}
.preViewbtn:hover {opacity:0.7}
@CHARSET "UTF-8";

/* noto_sans */
@font-face{font-family:noto_sans;src:url('../fonts/NotoSansKR-DemiLight-Hestia.woff') format('woff');}

/* ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
 $ 레이아웃 스타일
~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ */
*:focus { outline:0 !important; }

html{
  height: 100%;
}
body {
  font-family:  "NanumGothic", "NanumGothicOTF","나눔고딕"; 
  min-height: 100%;
  color:#333333;
  padding-top:109px;
}
header.topbar {
  z-index: 2;
  position: relative;
  width: 100%;
  min-height: 60px;
  padding: 19px 22px;
  background:#F4F8FB;background-color:#F4F8FB;
  border-bottom: 1px solid #DDDDDD;
  font-size:1.2em;
}
header.topbar .icon { color:#C2C2C2;margin-right:10px; }
header.topbar .exp { font-size:0.7em;color:#727373;margin-left:5px; }
section.content { padding:10px 15px; }
.center-block {
    float: none;
    display: block;
    margin-left: auto;
    margin-right: auto;
    /* padding:0 10px 0 10px;
    border-left:1px solid #D2D2D2; */
}
.relative { position:relative; }
.autoheight { height:100%; }
body.login-body { padding:0;margin:0; }

/* 그리드 레이아웃*/
.lay-container { display:table;width:100%;table-layout:fixed;height:600px; }
.lay-container .box-item {
    float: none;
    display: table-cell;
    vertical-align:top;
}
.lay-container .panel-heading { padding:3px 15px; }
.lay-container .panel-heading h5 { font-size:13px; }
a:hover { text-decoration:normal !important; }
a:focus { text-decoration:normal !important;outline:0 !important; }
/* ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
 $ 공통
~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ */
.inline-block { display:inline-block; }
.col-pad { float:left;vertical-align:middle; }
.col-pad-i { float:left;vertical-align:middle;margin-left:15px; }
/* 로딩 메시지 스타일 */
.ld-msg { font-size:0.8em;color:#aaaaaa;margin-left:8px; }
fieldset {
    border: 1px solid #D2D2D2 !important;
    padding: 0 1.4em 1.4em 1.4em !important;
    margin: 0 0 1.5em 0 !important;
    /* -webkit-box-shadow:  0px 0px 0px 0px #D2D2D2;
            box-shadow:  0px 0px 0px 0px #D2D2D2; */
}
legend {
	font-size : 1em !important;
    font-weight: bold !important;
    text-align: left !important;
    width:inherit;
    border-bottom:none;
}
textarea {
    width:100%;border:none;font-size:12px;
}
.inline-block {
    display:inline-block;vertical-align:middle;
}
.clearboth { 
	clear:both;display:block; 
}
.no-border { 
	border:none; 
}
.vcenter {
    display: inline-block;
    vertical-align: middle;
}
.vbottom  {
    vertical-align: bottom;
}
.underline { text-decoration:underline; }
/* 검색형 테이블 스타일 */
table.tb-search01 {
	width:100%;border-spacing:0;font-size:0.9em;
}
table.tb-search01 td { padding:3px;height:30px;vertical-align:middle; }
table.tb-search01 .sp-lbl { font-weight:bold;display:block;margin-bottom:3px; }

table.bg-grey tr, 
table.bg-grey td,
table.bg-grey th { background:#F7F7F7; }

table.tb-layout { 
	width:100%; 
}
table.spacing {
	border-spacing:3px;border-collapse:separate;	
}
table.spacing td { border:1px solid #7F7F7F !important; }

/* 항목안내 테이블 스타일 */
.bps-tb { padding-bottom:10px; border-bottom:1px solid #EAE7DA;margin-bottom:5px; }
.bps-tb h4 { font-size:18px;padding-bottom:5px;margin-bottom:5px;border-bottom:1px solid #D2D2D2; }
.bps-tb table { width:100%;border-spacing:0; }
.bps-tb th, .bps-tb td { padding:13px;height:30px;vertical-align:middle;margin-bottom:10px; }
.bps-tb th { background:#D0CECE;width:150px;text-align:center; }
.bps-tb tr { border:1px solid #EAE7DA; }

.noborder { border:none; }

.tbs2 {
  table {
    width: 100%;
    border-bottom: 1px solid #c9c9c9;

    th {
      position: relative;
      border-top: 1px solid #c9c9c9;
      //height:55px;
      line-height: 150%;
      padding: 10px 10px 10px 30px;
      background: #f9f9f9;

      &.require {
        &:before {
          content: '*';
          font-size: 15px;
          position: absolute;
          top: 50%;
          left: 20px;
          -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
        }
      }
    }

    td {
      border-top: 1px solid #c9c9c9;
      min-height: 35px;
      padding: 10px;

      .tips {
        line-height: 130%;
        margin-top: 5px;
      }

      &.tb-content {
        padding-top: 15px;
        padding-bottom: 15px;
      }

      &.tb-files {
        line-height: 25px;

        .file {
          position: relative;
          font-weight: 700;
          display: inline-block;
          vertical-align: top;
          margin-right: 5px;
          padding-left: 20px;

          &:before {
            content: '';
            width: 15px;
            height: 15px;
            display: inline-block;
            position: absolute;
            top: 50%;
            left: 0;
            margin-top: -9px;
            background: url('#{$imgPath}ico_file.svg') center center no-repeat;
            background-size: cover;
          }
        }

        em {
          display: inline-block;
          vertical-align: top;

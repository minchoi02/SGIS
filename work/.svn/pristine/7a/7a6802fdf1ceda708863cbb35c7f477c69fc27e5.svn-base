.switch {
  position: relative;
  display: inline-block;
  vertical-align: top;
  width: 95px;
  height: 35px;
  cursor: pointer;
}

.switch-txt {
  font-size: 17px;
  height: 35px;
  line-height: 35px;
  display: inline-block;
  vertical-align: top;
  margin-left: 10px;
}

.switch-input {
  position: absolute;
  top: 0;
  left: 0;
  opacity: 0;
}

.switch-label {
  position: relative;
  display: block;
  height: inherit;
  font-size: 15px;
  text-color: #565656;
  background: #a5a5a5;
  border-radius: 35px;
  font-weight: 700;
  line-height: 35px;
  border: 1px solid #ddd;
  box-sizing: border-box;
}

.switch-label:before, .switch-label:after {
  position: absolute;
  top: 0;
  line-height: 1;
  -webkit-transition: inherit;
  -moz-transition: inherit;
  -o-transition: inherit;
  transition: inherit;
  font-size: 15px;
  line-height: 35px;
  box-sizing: border-box;
}

.switch-label:before {
  content: attr(data-off);
  right: 11px;
  color: #fff;
}

.switch-label:after {
  content: attr(data-on);
  left: 11px;
  color: white;
  opacity: 0;
}

.switch-input:checked ~ .switch-label {
  background: #cc0000;
}

.switch-input:checked ~ .switch-label:before {
  opacity: 0;
}

.switch-input:checked ~ .switch-label:after {
  opacity: 1;
}

.switch-handle {
  position: absolute;
  top: 3px;
  left: 3px;
  width: 29px;
  height: 29px;
  border-radius: 29px;
  background: linear-gradient(to bottom, #ffffff 0%, #e5e5e5 100%);
  -webkit-transition: left 0.15s ease-out;
  -moz-transition: left 0.15s ease-out;
  -o-transition: left 0.15s ease-out;
  transition: left 0.15s ease-out;
  box-sizing: border-box;
}

.switch-input:checked ~ .switch-handle {
  left: 62px;
}


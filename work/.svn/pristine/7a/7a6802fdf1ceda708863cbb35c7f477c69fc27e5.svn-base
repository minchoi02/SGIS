.row {
  position: relative;
  box-sizing: border-box;
  font-size: 0;
  margin-bottom: 30px;
  text-align:left;

  .line-txt {
    line-height: 100%;
  }

  .form-btns {
    text-align: right;
  }
  .cols {
    box-sizing: border-box;
    margin-top: 20px;

    &:first-child {
      margin-top: 0;
    }
  }

  .col {
    font-size: 12px;
    box-sizing: border-box;
    display: inline-block;
    vertical-align: top;
    //padding: 0 10px;
    line-height:35px;

    &.col-txt{
      text-align:center;
      line-height:35px;
      font-weight:700;
      font-size: 14px;
    }

    &.col-5 {
      width: 20%;
    }

    &.col-4 {
      width: 25%;
    }

    &.col-3 {
      width: 33.33%;
    }

    &.col-2 {
      width: 50%;
    }

    &.col-1 {
      width: 100%;
    }

    &.col-sm1 {
      width: 8.33333%;
      &>.col{
        padding:0;
      }
    }

    &.col-sm2 {
      width: 16.66666%;
      &>.col{
        padding:0;
      }
    }

    &.col-sm3 {
      width: 25%;
      &>.col{
        padding:0;
      }
    }

    &.col-sm4 {
      width: 33.33333%;
      &>.col{
        padding:0;
      }
    }

    &.col-sm5 {
      width: 41.66666%;
      &>.col{
        padding:0;
      }
    }

    &.col-sm6 {
      width: 50%;
      &>.col{
        padding:0;
      }
    }

    &.col-sm7 {
      width: 58.33333%;
      &>.col{
        padding:0;
      }
    }

    &.col-sm8 {
      width: 66.66666%;
      &>.col{
        padding:0;
      }
    }

    &.col-sm9 {
      width: 75%;
      &>.col{
        padding:0;
      }
    }

    &.col-sm10 {
      width: 83.33333%;
      &>.col{
        padding:0;
      }
    }

    &.col-sm11 {
      width: 91.66666%;

      & > .col {
        padding: 0;
      }
    }

    &.col-sm12 {
      width: 100%;

      & > .col {
        padding: 0;
      }
    }

    .picker-group {
      position: relative;
      box-sizing: border-box;
      font-size: 0;
      //margin-right: 5px;
      overflow: hidden;

      .picker-item {
        position: relative;
        width: 50%;
        display: inline-block;
        vertical-align: top;

        &:first-child {
          float: left;

          .inputs {
            margin-right: 10px;
          }

          &:after {
            content: '~';
            width: 20px;
            font-size: 14px;
            position: absolute;
            top: 0;
            right: -10px;
            line-height: 35px;
            text-align: center;
          }
        }

        &:last-child {
          float: right;

          .inputs {
            margin-left: 10px;
          }
        }
      }
    }

    .srch-group {
      position: relative;

      .inputs {
        margin-right: 110px;
      }

      button {
        position: absolute;
        top: 0;
        right: 0;
        width: 100px;
      }
    }
  }

  .in-box {
    padding: 0 10px;

    &:first-child {
      margin-bottom: 20px;
    }

    &:last-child {
      margin-top: 0px;
    }

    &.line {
      border: 1px solid #ddd;
      padding: 0 10px 15px 10px;
      margin: 0 10px;

      .tb-tit {
        font-size: 16px;
        padding-top: 15px;
        margin-bottom: 10px;
        border-bottom: 1px solid #ddd;

        .total {
          font-size: 12px;
        }
      }

      table {
        margin-top: 10px;

        thead {
          th {
            background: #F1F1F1;
            height: 35px;
          }
        }

        tbody {
          td {
            height: 35px;
            cursor: pointer;

            &.maps {
              height: 300px;
            }

            &.maps, &.opts {
              &:hover {
                background: #fff;
              }
            }
          }
        }
      }

      .paging {
        margin-top: 15px;
      }
    }
  }

  .tits {
    position: relative;
    overflow: hidden;
    padding: 0 10px 5px 10px;
    margin-bottom: 10px;

    h4 {
      font-size: 18px;
      font-weight: 700;
    }
  }
}

.label {
  display:inline-block;
  color: #fff;
  padding: 5px 8px;
  font-size: 13px;
  min-width:40px;
  line-height: 100%;
  box-sizing: border-box;

  &.c1 {
    background: #5cb85c;
  }

  &.c2 {
    background: #5bc0de;
  }

  &.c3 {
    background: #de5b5b;
  }
  &.c4 {
    background: #dece5b;
  }

  &.c5 {
    background: #777;
  }

  &.c6 {
    background: #3d62eb;
  }

  &.c7 {
    background: #995bde;
  }

  &.c8 {
    background: #3498db;
  }
}

.inputs{
  position:relative;
  display:block;
  box-sizing: border-box;
  vertical-align: top;
  height: 35px;
  overflow: hidden;
  border: 1px solid #d5d5d5;
  &.inline{
    display:inline-block;
  }

  &.half {
    width: 49.5%;
    display: inline-block;
    vertical-align: top;

    &:first-child {
      float: left;
    }

    &:last-child {
      float: right;
    }
  }
  input{
    font-size:12px;
    width:100%;
    height: 100%;
    text-indent:10px;
    margin:0;
    padding:0;
    border:0;
    box-sizing: border-box;
    display: inline-block;
    vertical-align: top;

    &:disabled {
      background: #eaeaea;
    }
  }
  &.datepicker{
    .ui-datepicker-trigger{
      position:absolute;
      top: 5px;
      right: 10px;
      width: 25px;
    }
  }

  &.timepicker {
    background: transparent;

    input {
      z-index: 2;
      position: relative;
      background: transparent;
    }

    &:after {
      content: '';
      position: absolute;
      top: 5px;
      right: 10px;
      width: 25px;
      height: 22px;
      background: url('#{$imgPath}ico_time.svg') center center no-repeat;
      background-size: cover;
      z-index: 1;
    }

  }

  &.datepicker-month {
    .cal {
      position: absolute;
      top: 5px;
      right: 10px;
      width: 25px;
      height: 22px;
      background: url('#{$imgPath}ico_cal.svg') center center no-repeat;
      background-size: cover;
    }
  }
}

.textarea {
  position: relative;
  display: block;
  box-sizing: border-box;
  vertical-align: top;
  overflow: hidden;
  border: 1px solid #d5d5d5;

  &.inline {
    display: inline-block;
  }

  textarea {
    font-size: 12px;
    width: 100%;
    height: 100%;
    padding: 10px;
    margin: 0;
    border: 0;
    box-sizing: border-box;

    &:disabled {
      background: #eaeaea !important;
    }
  }
}

.check-items {
  display: inline-block;
  vertical-align: top;
  margin-right: 15px;

  .checkbox {
    margin-left: 10px;
  }
}

.checkbox {
  display: inline-block !important;
  position: relative !important;
  top: 0 !important;
  left: 0 !important;
  -webkit-transform: translate(0, 0) !important;
  transform: translate(0, 0) !important;

  &.inline {
    display: inline-block;
  }

  input {
    position: absolute;
    top: -9999px;
    left: -9999px;

    & + label {
      position: relative;
      display: block;
      height: 17px;
      line-height: 17px;
      padding-left: 24px;
      font-size: 12px;
      color: #000;
      cursor: pointer;

      &:before {
        content: '';
        position: absolute;
        top: 0;
        left: 0;
        display: inline-block;
        vertical-align: top;
        width: 17px;
        height: 17px;
        border: 1px solid #c9c9c9;
      }

      &:after {
        content: '';
        position: absolute;
        top: 0;
        left: 0;
        display: inline-block;
        vertical-align: top;
        width: 17px;
        height: 17px;
      }
    }

    &:checked {
      & + label {

        &:before {
          border: 1px solid #3d62e7;
        }

        &:after {
          content: '';
          background: url('#{$imgPath}ico_check.svg') center center no-repeat;
          background-size: 12px 10px;
        }
      }
    }
  }
  &.solo {
    input {
      position: absolute;

      & + label {
        position: relative;
        display: block;
        //border: 1px solid #c9c9c9;
        width: 17px;
        height: 17px;
        cursor: pointer;
        padding-left: 0px;
      }

      &:checked {
        & + label {
          border: 1px solid #3d62e7;
          background: url('#{$imgPath}ico_check.svg') center center no-repeat;
          background-size: 12px 10px;
        }
      }
    }
  }
}

.radios {
  display: inline-block;
  margin-right: 10px;

  input {
    position: absolute;
    top: -9999px;
    left: -9999px;

    & + label {
      position: relative;
      display: block;
      height: 17px;
      line-height: 17px;
      padding: 0 0 0 23px;
      font-size: 12px;
      color: #000;
      cursor: pointer;

      &:before {
        content: '';
        position: absolute;
        top: 0;
        left: 0;
        display: inline-block;
        vertical-align: top;
        box-sizing: border-box;
        width: 17px;
        height: 17px;
        border-radius: 17px;
        border: 1px solid #c9c9c9;
      }
    }

    &:checked {
      & + label {
        &:before {
          border: 1px solid #3d62e8;
        }

        &:after {
          content: '';
          position: absolute;
          top: 3px;
          left: 3px;
          display: inline-block;
          vertical-align: top;
          width: 11px;
          height: 11px;
          border-radius: 11px;
          background: #3d62e8;
        }
      }
    }
  }

  &.line {
    input {
      position: absolute;
      top: -9999px;
      left: -9999px;

      & + label {
        position: relative;
        display: block;
        width: 80px;
        text-align: center;
        border: 1px solid #c9c9c9;
        height: 35px;
        line-height: 35px;
        padding: 0;
        font-size: 12px;
        color: #000;
        cursor: pointer;

        &:before {
          display: none;
        }
      }

      &:checked {
        & + label {
          border: 1px solid #3d62e7;

          &:before {
            display: none;
          }

          &:after {
            display: none;
          }
        }
      }
    }
  }
}

.select{
  position:relative;
  display:block;
  border: 1px solid #d5d5d5;
  box-sizing: border-box;
  height: 35px;
  overflow: hidden;
  &:after{
    content:'';
    position:absolute;
    top:15px;
    right:10px;
    border-right: 5px solid transparent;
    border-left: 5px solid transparent;
    border-top: 5px solid #000;
  }

  &.inline{
    display:inline-block;
    min-width: 200px;
  }

  select{
    font-size:12px;
    width:100%;
    height: 100%;
    background:none;
    border:0;
    text-indent:10px;
    box-sizing: border-box;
  }
}

.user-txt {
  font-size: 18px;
  display: inline-block;
  padding: 5px 10px;
  margin-bottom: 10px;
}


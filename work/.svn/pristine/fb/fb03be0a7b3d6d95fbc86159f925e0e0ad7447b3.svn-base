

.useBox{width:100%; }
.useBox li{float:left; width:380px; height:383px; margin-left:30px; position:relative; margin-top:40px}
.useBox li:first-child{margin-left:0;}
.useBox li:nth-child(1){background:url(../../img/analysis/use/use_li_bg01.png)no-repeat left bottom;}
.useBox li:nth-child(2){background:url(../../img/analysis/use/use_li_bg02.png)no-repeat left bottom;}
.useBox li:nth-child(3){background:url(../../img/analysis/use/use_li_bg03.png)no-repeat left bottom;}
.useBox li:nth-child(4){background:url(../../img/analysis/use/use_li_bg04.png)no-repeat left bottom; margin-left:0;}
.useBox li:nth-child(5){background:url(../../img/analysis/use/use_li_bg05.png)no-repeat left bottom;}
.useBox li:nth-child(6){background:url(../../img/analysis/use/use_li_bg06.png)no-repeat left bottom;}
.useBox li:nth-child(2) h1,.useBox li:nth-child(4) h1,.useBox li:nth-child(6) h1{background:#085fd1}
.useBox li h1{z-index:10; text-align:center; background:#009587; color:#fff; line-height:60px; width:67px; height:67px; box-sizing:border-box; border:4px solid #fff; border-radius:35px; position:absolute; left:160px; margin:0 auto;}
.normal{z-index:1; position:absolute; left:0; bottom:0; cursor:pointer; width:100%; height:355px}
.normal_tilte{width:100%; font-size:18px; color:#000; text-align:center; margin-top:50px; font-weight:500; }
.normal_cont{font-size:14px; color:rgba(0,0,0,0.8); margin-left:190px; margin-top:100px; }
.hover{position:absolute; left:0; bottom:0; z-index:2; width:100%; display:none }
.hover_tilte{width:100%; height:157px; background:#009587; font-size:18px; color:#fff; text-align:center; box-sizing:border-box; font-weight:100; padding-top:50px;}
.hover_tilte.blue{background:#085fd1;}
.hover_cont{width:100%; height:198px; background:rgba(0,0,0,0.85);  box-sizing:border-box; padding:20px 25px 30px 25px; }
.hover_cont p{height:105px; width:100%; font-size:13px; color:#fff;}
.hover_cont button{width:100%; border-radius:35px; height:45px; background:#fff; color:#000; font-size:15px;}
.hover_cont button:hover{background:#009587; color:#fff;}
.hover_cont button.blue:hover{background:#085fd1;}


.usePopupWrap{width:100%; background:rgba(0,0,0,0.8); padding-bottom:50px; position:absolute; left:0; top:0; z-index:300; padding-top:95px; display:none;}
.usePopup{width:1100px; margin:0 auto; background:#fff; display:none;}
.usePopupTitle{width:100%; height:322px; background:#009587; overflow:hidden; padding:26px 32px; box-sizing:border-box; position:relative; }
.usePopupTitle.blue{background:#085fd1;}
.usePopupTitle button{position:absolute; top:26px; right:32px;}
.usePopupTitle button:hover{opacity:0.5}
.usePopupTitle div{float:left;}
.usePopupTitle h1{font-size:30px; font-weight:600; color:#fff; letter-spacing:-1px; margin-top:60px; }
.usePopupTitle h2{margin-top:30px; font-size:15px; color:rgba(255,255,255,0.5); }
.usePopupCont{width:100%; background:#fff; padding:70px 78px; box-sizing:border-box;}
.redbox{overflow:hidden; }
.redbox h1{border-radius:30px; background:#ed304d url(../../img/analysis/use/popup_title_bg.png)no-repeat 9px 8px; height:58px; padding-top:8px; color:#fff; box-sizing: border-box; padding-left:78px; font-size:17px; font-weight:500; }
.redbox h1 span{font-size:27px; }
.redbox dl{padding-left:65px; }
.redbox dt{height:30px; margin-top:43px; background:url(../../img/analysis/use/popup_dt_bg.png)no-repeat left center; font-size:18px; font-weight:600; color:#000; padding-left:48px; }
.redbox dd{padding-left:47px; font-size:15px; color:rgba(0,0,0,0.8);}
.redbox dd.font{font-size:12px;}
.greenbox{margin-top:50px; overflow:hidden;}
.greenbox h1{border-radius:30px; background:#2cc36b url(../../img/analysis/use/popup_title_bg.png)no-repeat 9px 8px; height:58px; padding-top:8px;  box-sizing: border-box; color:#fff; padding-left:78px; font-size:17px; font-weight:500; }
.greenbox h1 span{font-size:27px; }
.stepBox{width:100%; padding:40px 50px 0 66px; box-sizing:border-box; overflow:hidden;}
.stepBox h1{height:32px; width:230px; border-radius:20px; border:2px solid #fe5621; font-size:16px; color:#fe5621; background:#fff; line-height:28px; padding-left:24px; font-weight:500; padding-top:0}
.stepBox img{float:right; margin-top: 15px; overflow:hidden;}
.stepBox dl{float:left; }
.stepBox dt{font-size:18px; font-weight:600; color:#000; margin-top:15px; }
.stepBox dd{background:url(../../img/analysis/use/popup_dd_bg.png)no-repeat left 5px; padding-left: 30px; font-size:15px; margin-top:10px;}
.stepBox dd span{color:#009944; font-weight:600}
.stepBox p{font-size:12px; color:rgba(0,0,0,0.5); text-align:center;}
.stepBox div.step{float:left; width:450px; overflow:hidden;}
.stepBox ul{margin-left:30px;}
.stepBox li{float:left; width:112px; margin-right:10px; }
.stepBox li:last-child{margin-right:0;}
.stepBox li span{width:100%; display:inline-block; text-align:left; background:url(../../img/analysis/use/step_li_bg.png)no-repeat 10px 5px; padding-left:30px; font-size:12px; color:rgba(0,0,0,0.8);}
.usePopupB{width:100%; height:342px; box-sizing:border-box; background:#f2f2f2; padding-top:35px; overflow:hidden; text-align:center;}
.usePopupB button{width:550px; height:35px; border-radius:20px; text-align:center; font-size:12px; color:#fff; background:#3d62e8; margin:0 auto; margin-top:25px;}
.usePopupB button span{font-size:15px; font-weight:600;}
































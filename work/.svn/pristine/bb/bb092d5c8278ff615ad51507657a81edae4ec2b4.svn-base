.dashbordWrap{width:100%; height:57px; border-bottom:1px solid #e5e5e5; line-height:57px; position:relative;}

.dashbordTile{width:1200px; min-width:1200px; margin:0 auto; display: flex; flex-direction:row; justify-content:space-between; position:relative;}
.dashbordTile h1{font-size:15px; font-weight:500; color:#000;}
.dashbordTile button{width:22px; height:22px; background:url(../../img/main/icon_setting_n.png)no-repeat 0 0; margin-top:20px;}
.dashbordTile button:hover{background:url(../../img/main/icon_setting_s.png)no-repeat 0 0; }

.dashbordSet{position:absolute; top:-1px; right:40px; border:1px solid #e5e5e5; box-sizing:border-box; width:260px; height:340px; background:#fff; display:none; z-index:2}
.dashbordSet h1{width:100%; border-bottom:1px solid #e5e5e5; height:57px; line-height:57px; font-size:15px; color:#000; text-align:center;}
.dashbordSet ul{margin-top:22px;}
.dashbordSet li{font-size:12px; color:#000; line-height:25px; margin-top:8px; margin-left:20px;}
.dashbordSet li input{margin-right:20px;}
.dashbordSet li input[type=checkbox]:not(old){  margin:0; padding:0; opacity:0; background:url(../../img/common/checkbox_n.png) no-repeat 0 0; width:0; }
.dashbordSet li input[type=checkbox]:not(old) + label{display: inline-block; text-align: left; height:25px; padding-left: 40px; background: url(../../img/common/checkbox_n.png) no-repeat 0 0; }
.dashbordSet li input[type=checkbox]:not(old):checked + label {background: url(../../img/common/checkbox_s.png) no-repeat 0 0;}

#dashSaveBtn{background:#3d62e8; width:130px; height:35px; border-radius:35px; text-align:center; color:#fff; font-size:15px; margin-top: 10px; margin-left: 66px;}
#dashSaveBtn:hover{opacity:0.5;}

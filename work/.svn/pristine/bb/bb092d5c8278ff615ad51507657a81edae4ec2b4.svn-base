.wTableBox{width:1140px;margin:0 auto;height:auto;border-top:2px solid #879099;border-bottom:1px solid #e5e5e5;padding:30px 0;}
.wTableBox.t01{margin-top:30px;padding:20px 0;}
.wTableBox.t02{width:1200px;margin-top:30px;padding:20px 0;border-bottom:0;}
.wTableBox.t03{border-top:0;}
.wTable{width:100%;margin:0 auto;table-layout:fixed;border-collapse:collapse;}
.wTable th{text-align:left;vertical-align:top;width:240px;padding-left:30px;padding-top:16px;}
.wTable td{padding:10px 20px;}
.wTable td .inp{height:33px;width:calc( 100% - 22px );border:1px solid #e5e5e5;padding:0 10px;overflow:hidden;}
.wTable td .inp01{height:33px;width:calc( 100% - 22px );border:1px solid #e5e5e5;padding:0 10px;overflow:hidden;border-radius:17px;}

.wTable td .sel01{height:33px;width:calc( 100% - 0px );border:1px solid #e5e5e5;padding:0 10px;overflow:hidden;border-radius:17px;}

.wTable td .textarea{height:100px;width:calc( 100% - 22px );resize:none;border:1px solid #e5e5e5;padding:10px;overflow:hidden;overflow-y:auto;}
.wTable td .textarea ul{width:100%;height:auto;overflow:hidden;margin:0 auto;}
.wTable td .textarea ul li{float:left;clear:both;width:100%;height:auto;margin:5px auto;}
.wTable td .textarea ul li>.chkEtc{float:left;}
.wTable td .textarea ul li label{float:left;margin-left:10px}

.listTable02{width:100%;margin:0 auto;table-layout:fixed;border-collapse:collapse;overflow : auto ; display:inline-table}
.listTable02 tr{min-width:1200px;}
.listTable02 th{height:45px;border:1px solid #c9c9c9;border-right:0;border-top:2px solid #879099;font-size:15px;color:#000;min-width:120px}
.listTable02 td{height:55px;border:1px solid #c9c9c9;border-right:0;font-size:12px;color:#4c4c4c;text-align:center;}
.listTable02 tr>th:first-child,
.listTable02 tr>td:first-child{border-left:0;}
.listTable02 td.al{text-align:left;padding:0 20px;}

.listTable02.t01{width:100%;margin:0 auto;table-layout:fixed;border-collapse:collapse;overflow : auto ;  display:block }
.listTable02.t01 tr{min-width:1200px;}
.listTable02.t01 th{height:45px;border:1px solid #c9c9c9;border-right:0;border-top:2px solid #879099;font-size:15px;color:#000;min-width:120px}
.listTable02.t01 td{height:55px;border:1px solid #c9c9c9;border-right:0;font-size:12px;color:#4c4c4c;text-align:center;}
.listTable02.t01 tr>th:first-child,
.listTable02.t01 tr>td:first-child{border-left:0;}

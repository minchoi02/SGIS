.dataSearchResultScrolls{width:calc( 100% - 40px );margin:10px auto 0 auto;height:auto;max-height:200px;overflow:hidden;overflow-y:auto;}
/* .dataSearchResult{width:100%;margin:0 auto;height:auto;overflow:hidden;} */
/* .dataSearchResult{width:100%;margin:0 auto;height:70px;overflow-y:scroll;overflow-x:hidden;} */
.dataSearchResult{width:100%;margin:0 auto;height:100px;overflow-y:auto;}
.dataSearchResult li{width:100%;height:auto;overflow:hidden;margin-bottom:5px;}
.dataSearchResult li a{float:left;}
.dataSearchResult li span{float:left;height:25px;line-height:25px;margin-left:10px;}

.clist01{width:340px;height:auto;overflow:hidden;margin: 0px 20px 0 20px;position: absolute;top: 100px;}
.clist01 tr{width:100%;height:auto;overflow:hidden;padding:5px 0;}
.clist01 tr .t01{width:90px;font-size:12px;color:#000;font-weight:bold;padding: 5px 0 5px 0;}
.clist01 tr .t02{font-size:12px;color:#000;padding: 5px 0 5px 0;}

.clist01 td{text-align:left;width:10px;padding-left:5px;}
.clist01 .td01 {font-size: 20px;font-weight: bold; color: #000;text-align: left;width: 280px;padding-left: 40px;float: left;}
.clist01 .td02 {font-size: 16px; color: #000;text-align: left;width: 280px;padding: 20px 0 0 40px;float: left;}
.clist01 .td03 {font-size: 16px; color: #000;text-align: left;width: 280px;padding: 10px 0 0 40px;float: left;}
.clist01 .ellipsis {display: inline-block;width: 270px;white-space: nowrap;overflow: hidden;text-overflow: ellipsis; height: 54px;text-align: left;word-wrap: break-word;display: -webkit-box;-webkit-line-clamp: 2;-webkit-box-orient: vertical; white-space: normal;}

.dataEmpty span  {font-size: 20px;font-weight: bold; color: #000;text-align: center;width: 100%;padding-left: 0px;float: left;margin-top:130px; }


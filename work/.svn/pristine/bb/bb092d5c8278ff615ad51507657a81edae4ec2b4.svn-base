.topBox{ width:100%; position:absolute;;}
.top{width:1200px; margin:0 auto; height:90px; min-width:1200px; display: flex; flex-direction: row; justify-content: space-between; }
.toplogo{vertical-align:middle; line-height:90px;}
.toplogo img{vertical-align:middle;}

.topAdmin{padding-top:27px;}
.topAdmin ul{width:240px; height:35px; border-radius:30px;  box-sizing:border-box; border:1px solid #e5e5e5;  padding:7px 15px; float:left; overflow:hidden;}
.topAdmin li{float:left; margin-right:6px; height:20px; line-height:20px;}
.topAdmin li:last-child{ margin-left:10px; }
.topAdmin li.admin{background:url(../../img/common/icon_admin01_n.png)no-repeat 0 0; width:20px; height:20px; }
.topAdmin li.admin:hover{background:url(../../img/common/icon_admin01_s.png)no-repeat 0 0; width:20px; height:20px; }
/* .topAdmin li.admin.on{background:url(../../img/common/icon_admin01_s.png)no-repeat 0 0; width:20px; height:20px; } */
.topAdmin li.group{background:url(../../img/common/icon_admin02_n.png)no-repeat 0 0; width:20px; height:20px; }
/* .topAdmin li.group.on{background:url(../../img/common/icon_admin02_s.png)no-repeat 0 0; width:20px; height:20px; } */
.topAdmin li.group:hover{background:url(../../img/common/icon_admin02_s.png)no-repeat 0 0; width:20px; height:20px; }

.topAdmin button{width:82px; height:35px; border:1px solid #e5e5e5; box-sizing:border-box; font-size:12px; text-align:center; border-radius:30px; float:left; margin-left:10px;}
.topAdmin button:hover{opacity:0.5;}

.topMenu li{line-height:90px; width:126px; box-sizing:border-box;  float:left; height:90px;}
.topMenu li:hover , .topMenu li.on{border-bottom:3px solid #3d62e8; }
.topMenu li a{ font-size:18px; color:#000; text-align:center;  width:126px; display: inline-block; font-weight:600;}


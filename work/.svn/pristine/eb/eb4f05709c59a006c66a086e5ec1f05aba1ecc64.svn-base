.subConentWrap{width:1200px; margin:0 auto; height:auto; padding-bottom:50px; }
.subTitleWrap{height:170px; width:100%; overflow:hidden;border-bottom: 1px solid #d5d5d5;}
.subTitle{float:left; margin-top:80px; }
.subTitle h1{font-size:30px; font-weight:100; color:#000; }
.subTitle h2{font-size:12px; font-weight:100; color:#333333; margin-top:5px;}

.see{float:right; margin-top:110px;}
.see select{width:109px; height:35px; border-radius:30px; border:1px solid #d5d5d5;  background:url(../../img/common/select_bg.png)no-repeat 70px -6px; background-color:#fff;font-size:12px; color:#000; box-sizing:border-box;  padding-left:15px;  line-height:35px; }


.tabWrap{height:104px; border-bottom:1px solid #e5e5e5; border-top:1px solid #e5e5e5; box-sizing:border-box; padding:24px 0;}
.tab ul{display: flex; flex-direction:row; justify-content:space-between; width:900px;margin:0 auto;}
.tab li{width:100%; height:56px;box-sizing:border-box; border:1px solid #e5e5e5;}
.tab li a{font-size:15px; color:#000; text-align:center; display:inline-block; width:100%; line-height:56px;}
.tab li:hover {cursor:pointer;}
.tab li.on{background:#3d62e8; border:none;}
.tab li.on a{color:#fff; }

.info{margin:80px 0;}
.info h1{font-weight:500; font-size:25px; color:#000; height:50px; padding-left:10px;}
.info ul{}
.info li{background:url(../../img/common/table_list.png)no-repeat 15px 15px; font-size:16px; line-height:35px;  color:#000; padding-left:30px;}
.info dt{background:url(../../img/common/table_list.png)no-repeat 15px 15px; font-size:15px; line-height:35px;  color:#000; padding-left:30px;}
.info dd{font-size:15px; color:#000; line-height:25px; padding-left:30px;}

.siteMap{width:100%; display: flex; flex-direction:row; justify-content:space-between;  }
.site{width:185px; height: auto; }
.site h1{font-size:20px; font-weight:500;  width:100%; height:45px; border-bottom:2px solid #3d62e8; text-align:center; color:#000;padding:0px;}
.site dt ,.site li{cursor:pointer;background:url(../../img/common/table_list.png)no-repeat 15px 20px; font-size:15px; line-height:45px;  color:#000; padding-left:30px; box-sizing:border-box; width:100%; border:1px solid #e5e5e5; background-color:#f1f1f1; margin-top:20px;}
.site dd{font-size:12px; color:rgba(0,0,0,0.8); line-height:25px; padding-left:30px; cursor:pointer;}
.site dd:hover, .site li:hover, .site dt:hover{font-weight:bold;}

.QA{ width:100%; }
.QA button{background:#3d62e8; width:130px; height:35px; border-radius:35px; text-align:center; color:#fff; font-size:15px;}
.QA button:hover{opacity:0.5;}
.QABtn{width:100%; display:flex; flex-direction:row; justify-content:center; margin-top:50px;}

.QA table{width:100%; border-top:2px solid #3d62e8; }
.QA table th{border-right:1px solid #e5e5e5; border-bottom:1px solid #e5e5e5; font-size:15px; font-weight:500; text-align:center; height:58px; color:#000;}
.QA table th:last-child{border-right:0}
.QA table td{border-right:1px solid #e5e5e5; border-bottom:1px solid #e5e5e5; font-size:15px; ; text-align:center; height:58px;color:#000}
.QA table td:last-child{border-right:0}
.QA table td.list{text-align:left; padding:0 30px;}

.QA table td input{border:1px solid #e5e5e5; height:35px; box-sizing:border-box; color:#000;  padding:0 20px; width:100%;font-size:15px;}
.QA table td input::-ms-input-placeholder { color: #e5e5e5; }
.QA table td input::-webkit-input-placeholder { color: #e5e5e5; } 
.QA table td input::-moz-placeholder { color: #e5e5e5; }
.QA table td textarea{width:100%; box-sizing:border-box; padding:20px; font-size:15px; color:#000; height:300px; border:1px solid  #e5e5e5; margin:20px 0; text-align:left; vertical-align:top;resize:none;font-family:" NSQR","나눔고딕", "NanumGothic",ng,'Apple SD Gothic Neo','Microsoft NeoGothic';}

.QA table td div{width:100%; display:flex; flex-direction:row; justify-content:space-between;  box-sizing:border-box; padding:10px 30px; box-sizing:border-box;}
.QA table td div button.btn{width:150px; height:35px; border:1px solid #e5e5e5; box-sizing:border-box; text-align:center; font-size:12px; color:#000; background:#fff;}
.QA table td div.list{width:84%; border:1px solid #e5e5e5; }
.QA table td div.list ul{}
.QA table td div.list li{font-size:12px; color:#000;  padding-top:5px;}
.QA table td div.list li span{width:25px; height:25px; display:inline-block; vertical-align:middle;}
.QA table td div.list li span:hover{opacity:0.5;}

.QA table.reading{}
.QA table.reading th{text-align:left; border-right:0; padding-left:20px; font-weight:550;}
.QA table.reading td{ text-align:left; height:300px; padding: 40px;min-height:300px;}
.QA table.reading td.bg{background:#f8f8f8;}

.QA table .left {text-align:left; padding:0 20px 0 20px;}

.faq{width:100%; border-top:2px solid #3d62e8; overflow:hidden;height:90px;}
.faqBox{display: flex; flex-direction:row; justify-content:space-between; box-sizing:border-box; padding:0 30px; border-bottom:1px solid #e5e5e5; width:100%; height:70px; }
.faqBox h1{font-weight:100; font-size:15px; color:#000;  line-height:35px; padding-top:10px; }
.faqBox .noArea {margin-top:16px;width:50px;}
.faqBox .no {width:35px; height:35px; background:#879099; border-radius:20px; font-size:15px; text-align:center; color:#fff; margin-right:30px; display:inline-block;line-height:35px;}
.faqBox .title { display: table-cell;vertical-align: middle;height: 70px;font-size: 15px;width:1200px;padding: 0 10px 0 10px;color:#000;font-weight:bold;}
.faqBox .arrow {margin-top:5px;}
.faqBox button{width:35px; height:35px; background:url(../../img/common/btn_down.png)no-repeat center center; margin-top:10px;}
.box{box-sizing:border-box; padding:30px 100px; border-bottom:1px solid #e5e5e5; height:auto; font-size:15px; color:#000;line-height:30px; display:none;}

.wd80 {width:80px;}
.wd120 {width:120px;}

.attachFileArea {}
.attachFileArea  a {cursor:pointer;display: inline-block;min-width: 35px;height: 35px;border: solid 1px #e5e5e5;font-size: 12px;line-height: 35px; text-align: center;vertical-align: top; padding: 0 10px 0 10px; border-radius: 20px;margin-right:10px;background:#e5e5e5;}

.searchArea {height:60px;text-align:center;margin-top: 15px;}
.searchArea select{width:200px; height:45px;border:1px solid #d5d5d5;  background:url(../../img/common/select_bg.png)no-repeat top right; background-color:#fff;font-size:14px; color:#000; box-sizing:border-box;  padding-left:40px;  line-height:45px; }
.searchArea input{width:300px; height:45px;border:1px solid #d5d5d5; font-size:14px; color:#000; box-sizing:border-box;  padding:0px 40px 0px 40px;  line-height:45px;}
.searchArea button{width:200px; height:45px;background:#3d62e8; font-size:17px; color:#fff; text-align:center;margin-top:-1px;}
.searchArea button:hover{opacity:0.5}
.writeBtn {border-radius:30px;float:right;background:#fff !important;color:#000 !important;border: 1px solid #e5e5e5 !important;}

.contentArea {width: 100%;height: 441px;}
.srvImgArea {width: 600px;/* height: 400px; */border: 1px solid #e5e5e5;float:left;}
.srvContentArea {width: 580px;height: 400px;float:left;margin-left:10px;}
.linkBtn {width: 200px;height:35px;box-sizing:border-box;font-size: 12px;text-align: center;border-radius: 30px;float: right;margin-left: 10px;background: #3d62e8;color: #fff;}
.linkBtn:hover {opacity:0.7;}
.subContent {padding-left:50px;}
.srvContentArea > .linkArea {width:100%;height:50px;}
.subContent li {background:none; font-size: 14px;}
.subContent .title {background:none; font-size: 15px; font-weight: bold;}
.bt50 {border-bottom: 1px solid #e5e5e5;height:50px;}
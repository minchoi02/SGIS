package kostat.lbdms.ServiceAPI.common.web.exception;

public class NotFoundFileException extends FileSystemException{

	private static final long serialVersionUID = 6253370123341495481L;
	
	@SuppressWarnings("unused")
	private String path;
	
	public NotFoundFileException( Throwable throwable ) {
		super( throwable );
	}
	
	public NotFoundFileException( String message ) {
		super( message );
	}
	
	public NotFoundFileException( String message, String path ) {
		super( message );
		this.path = path;
	}

	@Override
	public String getMessage() {
		
		return super.getMessage();
	}
	
}

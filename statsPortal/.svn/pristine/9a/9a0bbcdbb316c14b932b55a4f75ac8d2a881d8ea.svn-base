*{margin:0;padding:0;box-sizing:border-box;}
body{}
table{width:100%;border-collapse: collapse;}
img{vertical-align:bottom;max-width:100%;}
textarea{resize:none;border:1px solid #ddd;padding:10px;max-width:100%;}
a{text-decoration:none;}
.wrap{width:700px;margin:0 auto;}
.cont_wrap{padding:0 30px 30px;background:#39a1ff;}
.cont{background:#fff;padding:30px;position:relative;}
.cont .bg01{position:absolute;top:10px;left:10px;}
.cont h2{font-size:18px;background:#39a1ff url('./img/bg02.png') 99% 80% no-repeat;padding:6px 20px;color:#fff;margin:20px 0;}
.cont h5{margin-top:40px;display:flex;font-size:15px;}
.cont h5:first-child{margin-top:0;}
.cont h5 span:first-child{margin-right:4px;}
.cont p.hint{font-size: 13px;margin: 20px;color:#39a1ff;position:relative;padding-left:58px;}
.cont p.hint::before{content:'HINT';padding:2px 10px;background:#39a1ff;border-radius:12px;color:#fff;margin-right:4px;position:absolute;left:0;top:0;}
.answer_wrap{margin:16px;}
.answer_wrap.layout_style01{display:flex;flex-wrap:wrap;}
.answer_wrap.layout_style01 p{width:calc(50% - 20px);}
.answer_wrap.layout_style02{display:flex;flex-wrap:wrap;}
.answer_wrap.layout_style02 p{width:calc(33.333% - 10px);}
.answer_wrap.layout_style03{display:flex;flex-wrap:wrap;}
.answer_wrap.layout_style03 p{width:calc(25% - 10px);}
table .answer_wrap{margin:10px;}
.answer{display:flex;align-items:center;margin-right:14px;margin-bottom:6px;font-size:14px;}
span.num{display:block;width:16px;height:16px;border-radius:50%;border:1px solid #555;font-size:12px;display:flex;align-items:center;justify-content:center;margin-right:6px;}
span.num.select {background: #0398fb;color: #fff;border: 1px solid #1fb9f9;}
.answer .sub_txt{font-size:12px;color:#ff0000;margin-left:10px;font-weight:600;}
.agree_wrap{margin:20px;padding:20px;background:#f1f1f1;}
.agree_txt{text-align:left;font-size:15px;padding:20px 0;}
.agree_chk_wrap{text-align:center;}
.user_info_wrap{margin:40px 20px 20px;} 
.user_info_tit{font-size:18px;text-align:center;padding:4px;}
.user_info_txt{font-size:14px;border:1px solid #ddd;border-top:none;padding:10px;}
.btn_wrap{display:flex;align-items:center;justify-content: center;margin:40px 0;}
.btn_wrap .btn_submit{width:120px;height:40px;background:#39a1ff;color:#fff;display:flex;align-items:center;justify-content:center;border-radius:20px;}
.btn_wrap .btn_submit:hover{background:#103f5f;}
.btn_wrap .btn_cancel{width:120px;height:40px;background:#ccc;margin-left:20px;color:#555;display:flex;align-items:center;justify-content:center;border-radius:20px;}
.btn_wrap .btn_cancel:hover{background:#bbb;}
.line{height:2px;background:#ddd;margin:30px 0;}

.table_style01{margin:20px 0;}
.table_style01 th, .table_style01 td{text-align: -webkit-center;border:1px solid #ddd;padding:8px;}
.table_style01 th{background:#f1f1f1;white-space:nowrap;}
.table_style01.tdc td span.num{margin:0 auto;}

/* 모바일 */
.m_wrap{max-width:500px;width:100%;margin:0 auto;}
.m_wrap textarea{width:100%;}
.m_wrap .cont_wrap{padding:0 20px 20px;}
.m_wrap .cont{padding:20px;}
.m_wrap .cont h2{font-size:16px;margin:10px 0;}
.m_wrap .cont h5{margin-top:20px;}
.m_wrap .answer_wrap{margin:20px 0;}
.m_wrap .answer .sub_txt{margin:10px 0;}
.m_wrap .answer{display:block;margin-bottom:6px;}
.m_wrap span.num{width:16px;height:16px;border-radius:50%;border:1px solid #555;font-size:12px;display:inline-block;line-height:15px;text-align:center;margin-right:6px;}
.m_wrap span.num.select {
    background: #0398fb;
    color: #fff;
    border: 1px solid #1fb9f9;}
.m_wrap .user_info_wrap{margin:0;}
.user_info_tit{text-align:left;}
.m_wrap .table_style01 th, .table_style01 td{text-align:left;padding:4px 12px;}
.m_wrap .footer {position:relative;margin:20px 0;height:20px;}
.m_wrap .footer .btn {position:absolute;background:#fff;border-radius:30px;padding: 8px 16px;color: #0398fb;font-size: 14px;font-weight: 700;text-decoration: none;text-transform: uppercase;transition: 0.5s ease;}
.m_wrap .footer .btn#prev{top:0;left:0;}
.m_wrap .footer .btn#next{top:0;right:0;}
.m_wrap .agree_wrap{margin:0;}
.m_cont {background:#fff;border-radius:30px;padding:20px;position: relative;  overflow: hidden;transition: 0.5s ease;}
.product {width:100%;position: absolute;  top: 0;  left: 0;opacity: 0;visibility: hidden;transition: 0.5s ease;}
.product.active {opacity: 1;visibility: visible;}


/* 유지관리팀 추가 */
button.num{width:16px;height:16px;border-radius:50%;border:1px solid #555;font-size:12px;align-items:center;justify-content:center;margin-right:6px;background-color:#FFF;cursor: pointer;}
button.num.select{background: #0398fb;color:#fff;border: 1px solid #1fb9f9;cursor: pointer;}
label{cursor: pointer;}
.f14{font-size:14px;}
.f15{font-size:15px;}
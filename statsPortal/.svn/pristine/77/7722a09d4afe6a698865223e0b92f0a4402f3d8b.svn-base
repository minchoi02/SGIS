@import url(https://cdn.jsdelivr.net/gh/moonspam/NanumSquare@1.0/nanumsquare.css);
body { padding:0; margin:0; font-family: 'NanumSquare', sans-serif; letter-spacing:-1px; }
a { outline:none; text-decoration:none; cursor:pointer; }
img { max-width:100%; }
ul { padding:0; margin:0; }
li { list-style:none;}

select { font-family: 'NanumSquare', sans-serif;}
#wrapper2 { width:100%; margin:0 auto; position:relative; overflow:hidden; }
#wrapper2 .container { width:100%; height:calc(100vh - 70px); margin:0 auto; position:relative; overflow:hidden; background-color:#F6F8FC; padding:15px; }

#wrapper2 .container .searchlist { width:350px; height:100%; position:relative; float:left; box-sizing:border-box; border-right:1px solid #E3E3EA; background-color:#fff;  }
#wrapper2 .container .searchlist .searchbox { position:relative; width:100%; float:left; box-sizing:border-box; border-bottom:1px solid #E3E3EA; padding:15px 20px;}
#wrapper2 .container .searchlist .searchbox .search { width:100%; margin:0 auto; box-sizing:border-box; background-color:#F8F8F8;  border-radius:30px; padding:5px 20px; border:1px solid #1F295A;}
#wrapper2 .container .searchlist .searchbox .search .searchLayer { display:none; width:309px; height:320px; box-sizing:border-box; position:absolute; z-index:100; left:20px; top:60px; border-radius:10px; background-color:#fff;  border:1px solid #E8E8E8; filter:drop-shadow(2px 2px 2px  #E2E2E2); padding:15px;}


#wrapper2 .container .searchlist .searchbox .search .searchLayer table th { text-align:left; font-size:14px; color:#1F295A; padding-bottom:5px; }
#wrapper2 .container .searchlist .searchbox .search .searchLayer table th > img { width:15px; margin-top:-3px; vertical-align:middle; margin-right:5px; }
#wrapper2 .container .searchlist .searchbox .search .searchLayer table th span { display:inline; box-sizing:border-box; padding:3px 10px;  font-size:12px; font-weight:bold; background-color:#E9ECFF; border-radius:10px; margin-left:10px; }
#wrapper2 .container .searchlist .searchbox .search .searchLayer .searchClose { cursor:pointer; float:right;}
#wrapper2 .container .searchlist .searchbox .search .searchLayer table td { font-size:14px; color:#494949; text-indent:22px; padding-bottom:5px;}
#wrapper2 .container .searchlist .searchbox .search .searchLayer table td img { vertical-align:middle; }
#wrapper2 .container .searchlist .searchbox .search .searchLayer .scrollBox { width:100%; height:150px; overflow-y:auto;}
#wrapper2 .container .searchlist .searchbox .search .searchLayer .scrollBox::-webkit-scrollbar { width: 15px; }
#wrapper2 .container .searchlist .searchbox .search .searchLayer .scrollBox::-webkit-scrollbar-thumb { background-color: #E9ECFF; border-radius:30px; }
#wrapper2 .container .searchlist .searchbox .search .searchLayer .scrollBox::-webkit-scrollbar-track { background-color: #F8F8F8; border-radius:30px;}

.btnTagHidden { cursor:pointer;}



#wrapper2 .container .searchlist .searchbox .search input[type='text'] { border:none; background-color:#F8F8F8; font-size:14px; color:#616161; text-indent:10px; width:80%; height:25px; }

#wrapper2 .container .searchlist .searchbox .search button { border:none; cursor:pointer; background-color:#F8F8F8; font-size:19px; float:right;}

#wrapper2 .container .searchlist .listbox { width:96%; height:500px; margin:15px 2%; float:left;  box-sizing:border-box; padding:20px; overflow-y:auto;}
#wrapper2 .container .searchlist .listbox li { width:100%; float:left; padding:8px 10px; box-sizing:border-box; border-radius:5px;}
#wrapper2 .container .searchlist .listbox li:nth-child(even) { background-color:#FBFBFB;}
#wrapper2 .container .searchlist .listbox li a { font-size:13px; color:#494949; }
#wrapper2 .container .searchlist .listbox li a:last-child { display:inline; vertical-align:middle; float:right;}

#wrapper2 .container .searchlist .listbox::-webkit-scrollbar { width: 15px; }
#wrapper2 .container .searchlist .listbox::-webkit-scrollbar-thumb { background-color: #E9ECFF; border-radius:30px; }
#wrapper2 .container .searchlist .listbox::-webkit-scrollbar-track { background-color: #F8F8F8; border-radius:30px;}

#wrapper2 .container .searchlist .searchbox2 { width:100%; position:absolute; z-index:10; bottom:28px; left:0px;  border-top:1px solid #E3E3EA; box-sizing:border-box; padding:20px; background-color:#fff;}
#wrapper2 .container .searchlist .searchbox2 table { width:100%; }

@charset "utf-8";
/* CSS Document */

/*NanumSquare*/
@font-face {
font-family:"NanumSquare";
font-style:normal;
font-weight:400;
src: url(./font/NanumSquareR.eot);
src:url(./font/NanumSquareR.woff) format('woff'), url(./font/NanumSquareR.ttf) format('truetype');
}
/* font-style:bold; */
/* @font-face {
font-family:"NanumSquareB"; 
font-weight:700;
src: url(./font/NanumSquareB.eot);
src:url(./font/NanumSquareB.woff) format('woff'), url(./font/NanumSquareB.ttf) format('truetype');
} */
/*NanumBarunGothic*/
@font-face {
font-family:"NanumBarunGothic";
font-style:normal;
font-weight:400;
src: url(./font/NanumBarunGothic.eot);
src:url(./font/NanumBarunGothic.woff) format('woff'), url(./font/NanumBarunGothic.ttf) format('truetype');
}

/*reset*/
* {margin: 0; padding: 0; text-decoration: none; font-size: 1em; outline: none;}
code, kbd, samp, pre, tt, var, textarea, input, select, isindex, listing, xmp, plaintext {font: inherit; font-size: 1em; white-space: normal;}
dfn, i, cite, var, address, em {font-style: normal;}
th, b, strong, h1, h2, h3, h4, h5, h6 {font-weight: normal;}
a, img, a img, iframe, form, fieldset, abbr, acronym, object, applet, table {border: none;}
table {border-collapse: collapse; border-spacing: 0;}
caption, th, td, center {text-align: left; vertical-align: top;}
body { font-family:"NanumSquare";	font-size:14px;	color:#666;line-height: 1; background: white; height:100%; width:100%; }
q {quotes: "" "";}
ul, ol, dir, menu {list-style: none;}
sub, sup {vertical-align: baseline;}
a {color: inherit;}
hr {display: none;} 
font {color: inherit !important; font: inherit !important; color: inherit !important;} /* disables some nasty font attributes in standard browsers */
marquee {overflow: inherit !important; -moz-binding: none;}
blink {text-decoration: none;}
nobr {white-space: normal;}

/*layout*/
.wrap { position:absolute; height:100%; width:100%;}
/* 2020-02-17 [김남민] My통계로 상단 화면과, SGIS 포털 상단 화면 탭이 포함된 부분의 크기가 일치하도록 수정 START */
.top_wrap { position:fixed; width:100%; height:28px; text-align:center; background-color:#32bed7; z-index:30; }
.top_box { margin:0 auto; height:35px; text-align:left;  }
.content_wrap { margin:28px auto 30px auto;; padding:0; /*overflow:auto;*/}
.side_box { float:left; position:fixed; width:230px; padding:0 20px; height:calc(100% - 58px); background-color:#fff; z-index:10; overflow:auto; /*box-shadow:3px 3px 3px rgba(0, 0, 0, .2);*/ }
/* 2020-02-17 [김남민] My통계로 상단 화면과, SGIS 포털 상단 화면 탭이 포함된 부분의 크기가 일치하도록 수정 END */
.content_box { float:right; position:relative; width:780px;  height:100%; padding-top:0px; padding-bottom:20px; text-align:center; }
.map_box { position:absolute; width:calc(100% - 270px); height:calc(100% - 29px); padding-left:270px; right:0; background-color:#f5f5f5; }
.footer_wrap { position:fixed; bottom:0; width:100%; height:30px; text-align:center; background-color:#4b4b4b; z-index:20;}
.footer_box { margin:0 auto; padding:10px 0 0 0;  text-align:center; color:#999; font-size: 12px;  }
.narrow { width:1100px; }
.wide { width:100%; }

/*common*/
.global_nav { width:270px; }
.global_nav ul { margin-left:20px; }
.global_nav ul li { display:inline-block; position:relative; height:35px; bottom:0;  }
/* 2020-02-17 [김남민] My통계로 상단 화면과, SGIS 포털 상단 화면 탭이 포함된 부분의 크기가 일치하도록 수정 */
.global_nav ul li a { position:absolute; display:block; padding:10px 0 0px 0; height:14px; margin-top:4px; width:100px; text-align:center; background-color:#fff; border-radius:30px 30px 0 0; color:#3399cc; font-size:12px; } 
.global_nav ul li a:hover { font-weight:bold; }
/* 2020-02-17 [김남민] 변경 디자인안 반영. START */
.tm_my { background:url(/images/statsMe/tm_my.png) no-repeat center; background-size:50px; }
.tm_my:hover { background-size:53px; }
.tm_sgis { background:url(/images/statsMe/tm_sgis.png) no-repeat center; background-size:50px; }
.tm_sgis:hover { background-size:53px; }
/* 2020-02-17 [김남민] 변경 디자인안 반영. END */

/*side nav*/
.logo_sgis { margin-top:30px; height:22px; }
.logo_sgis span { display:none; }
.logo_me_main { height:65px; }
.logo_me_main h1 { display:none; }
.logo_me_sub { height:100px; text-align:center; }
.logo_me_sub h1 { display:none; }
.me_about {font-size:13px; height:35px; margin-top:10px; margin-bottom:48px; line-height:150%; }
.me_about strong { font-weight:bold;}
.sidenav_box { margin-left:10px; border-left:1px solid #d7d7d7; }
.sidenav_box ul { margin-top:-5px; }
.sidenav_box ul li { margin-bottom:20px; margin-left:-9px; }
.sidenav_box ul li>a { display:block; padding:0 0 10px 25px; background:url(/images/statsMe/i_01.png) no-repeat left 0; font-size:15px; } /* 삭제 처리 letter-spacing:-1px; */
.sidenav_box ul li>a strong { font-weight:bold; color:#333; }
.sidenav_box ul li>a:hover { background:url(/images/statsMe/i_02.png) no-repeat left 0; }
.sidenav_box ul li>a:hover, .sidenav_box ul li a:hover strong { transition: color .6s ease; color:#ff5c5e;}
.sidenav_box ul li.current a { background:url(/images/statsMe/i_02.png) no-repeat left 0; color:#ff5c5e; }
.sidenav_box ul li.current a strong { color:#ff5c5e;}
.sidenav_box ul li ul { margin-top:10px; margin-left:40px; }
.sidenav_box ul li ul li {  margin-bottom:5px; font-size:16px; color:#666; }
.sidenav_box ul li ul li span {  color:#CCC; }
.sidenav_box ul li ul li a, .sidenav_box ul li.current ul li a { display:inline; background-image:none; padding:0; vertical-align:middle; }
.sidenav_box ul li ul li a:hover, .sidenav_box ul li.current ul li a:hover { display:inline; background-image:none; padding:0; vertical-align:middle; }
.view_type_box { position:relative; width:calc(100% - 2px); background-color:#fff; border:1px solid #22bed7; border-radius:15px; min-height:75px; overflow:hidden; }
.view_type_box .disable { border-top:none; border-bottom: 1px solid #d2d2d2; }
.view_type_box .view_map.disable a { background-color:#f5f5f5 !important; color:#8D8D8D; border:none; } 	/* 2019.11.29[한광희] 웹접근성(명도대비)에 따른 변경. color:#ccc; → color:#8D8D8D */ 
.view_type_box .view_text.disable a { background-color:#f5f5f5 !important; color:#8D8D8D;  border-bottom: 1px solid #d2d2d2; border-right:none; } /* 2019.11.29[한광희] 웹접근성(명도대비)에 따른 변경. color:#ccc; → color:#8D8D8D */
.view_type_box .disable a strong { color:#8D8D8D;  }	/* 2019.11.29[한광희] 웹접근성(명도대비)에 따른 변경. color:#ccc; → color:#8D8D8D */
.sidenav_box ul li .view_type_box a { display:block; background:none; padding:15px 0; }
.sidenav_box ul li .view_type_box a:hover {transition: color .6s ease; color:#ff5c5e; }
/* 2020-02-14 [김남민] 지도로 확인해요, 상세정보를 확인해요 화면을 볼때 메뉴 글씨색을 빨간색으로 수정 START */
.view_map { color:#ff5c5e; position:absolute; left:0; top:0; width:96px; height:77px;  font-size:23px; text-align:center; border-right:1px solid #d2d2d2; }
.view_text {  color:#ff5c5e; position:absolute; right:0; top:0; width:128px; height:62px;  font-size:23px; text-align:center; letter-spacing:-2px;}
.view_map strong, .view_text strong { font-weight:bold; /* color:#333; */}
/* 2020-02-14 [김남민] 지도로 확인해요, 상세정보를 확인해요 화면을 볼때 메뉴 글씨색을 빨간색으로 수정 END */
.view_map span, .view_text span { font-size:18px; }
.disable {  }
.disable div a { transition:none; color:#d2d2d2; cursor:default; }
.disable div a:hover { transition:none; color:#d2d2d2  !important; cursor:default;  }
.disable div a strong, .disable div a:hover strong  {  color:#d2d2d2 !important;  }
.sidenav_box ul li .view_list { margin-top:77px; /*border-top:1px solid #d2d2d2;*/ }
.sidenav_box ul li .view_list ul { margin:0; padding: 20px 30px 20px 30px; }
.sidenav_box ul li .view_list ul li { height:30px; }
.sidenav_box ul li .view_list ul li a { display:block; font-size:16px; padding:5px 10px; white-space: nowrap; overflow: hidden;	text-overflow: ellipsis;}
.sidenav_box ul li .view_list ul li a:hover { color:#333; font-weight:bold; }
.sidenav_box ul li .view_list ul li a.current { padding:6px 10px; background-color:#ff5c5e; color:#fff; border-radius:8px; letter-spacing:0; }

/* 테스트용 */
.view_type_box2 { position:relative; width:calc(100% - 2px); background-color:#fff; border:1px solid #22bed7; border-radius:15px; min-height:75px; overflow:hidden; }

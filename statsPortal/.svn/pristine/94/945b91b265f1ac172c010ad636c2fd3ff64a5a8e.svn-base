.workRoad #dataSlider-item .ui-slider-horizontal .ui-slider-range{top:0;height:100%;}
.workRoad #dataSlider-item .ui-slider-horizontal .ui-slider-range-min{left:0;}
.workRoad #dataSlider-item .ui-slider-horizontal .ui-slider-range-max{right:0;}
.workRoad #dataSlider-item .ui-slider-vertical{width:.8em;height:100px;}
.workRoad #dataSlider-item .ui-slider-vertical .ui-slider-handle{left:-.3em;margin-bottom:-.6em;margin-left:0;}
.workRoad #dataSlider-item .ui-slider-vertical .ui-slider-range{left:0;width:100%;}
.workRoad #dataSlider-item .ui-slider-vertical .ui-slider-range-min{bottom:0;}

<style>
#wrap { width:100%; min-width: 1600px; position:relative; height: 100vh; background-color: #ddd;  }
.mapWrapper { width:100%; min-width: 1600px; position:relative; height: 100vh;background: url( "/images/lvs/img/map01.png") center center no-repeat; }
.header { width:320px; height:100vh; position:fixed; left:0px; top:0px; z-index:10; }
.container{ width:calc(100% - 320px); height:100vh; position:fixed; right:0px; top:0px; z-index:10;padding: 10px;}
/* -----------------------------------header------------------------------------------- */
.logo_box{width: 100%; height: 200px; background: url("/images/lvs/img/logo-bcakground.png") center center no-repeat; padding:10px 20px 20px 20px;}
.logo_box .logo{ display:block;width:100%;height: 100px;}

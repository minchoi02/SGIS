/* ----------------------pop-up----------------------------------------- */
.popup_layer{width:100%; min-width: 1600px;height: 100vh; position:fixed;top:0px;right:0;z-index:999;;background:rgba(0,0,0,0.5);}
.pop_cont{width:920px;height: 620px;background: #fff;position:fixed;top:50%;right:50%;transform:translate(50%, -50%);}
.pop_cont .heading{ background: #fff;height: 45px; padding: 0 20px;font-size: 16px;}
.pop_cont .heading button{float: right;height: 100%;padding:0 12px;background: url("/images/lvs/icon/icon-close.png") center center no-repeat;}
.pop_cont .content{ padding: 0 20px; color: #666;}
/* --------------------------------------------------------------- */
.br{border: 1px solid #ccc;}
/* ---------------------------------팝업----------------------------- */
.popup_layer.active {display: block;}
.popup_layer{position: fixed;width: 100%;height: 100%;top: 0;left: 0;right: 0;z-index: 1100;display: none;}
.popup_layer .layerBox {position: relative;top: 50%;left: 50%;transform: translate(-50%, -50%);z-index: 10;}
.popup_layer .popup_bg {width:100%; min-width: 1600px;height: 100vh; position:fixed;top:0px;right:0;z-index:9;;background:rgba(0,0,0,0.5);}

.popup_home {width:400px;height: 280px;background: #fff; border-radius: 2px;}
.popup_home .info_text{width: 300px;height: 220px;margin: 0 auto;display: flex;flex-direction: column;align-items: center;    justify-content: center;}
.popup_home .info_text p{color:#444;font-size: 15px; margin-top: 10px;}
.popup_home .info_text strong{color:#204EE3;font-weight: bold;}
.popup_home .popup_btn a{width: 200px;float: left;border-top: 1px solid #ccc;line-height: 58px;text-align: center;font-weight: bold;color:#444;font-size: 15px;}
.popup_home .popup_btn a:nth-child(1){border-right:1px solid #ccc;}

.popup_extend{width:960px;height: 620px;background: #fff; border-radius: 2px;}
.popup_extend .heading{width: 100%;line-height: 50px;background: #EDF1F4; padding-left:20px;font-size: 16px;font-weight: bold;color:#444}
.popup_extend .heading button{float: right;height: 50px;padding:25px;background: url("/images/lvs/icon/icon-close.png") center center no-repeat;}

.popup_extend .content{padding:20px; color: #666;}
.popup_extend h4{padding-left: 10px; border-left: 3px solid #2A62FF;font-size: 17px;color: #0B0E11;font-weight: bold;margin-bottom: 15px;}
.tabs5{height: 18px;margin-bottom: 15px;}
.tabs5 li {padding-right: 10px;border-right: 1px solid #ccc;line-height: 14px;text-align:center;float: left;color: #444;}
.tabs5 li:not(:nth-child(1)) {padding-left: 10px;}
.tabs5 li:last-child {border-right: 0;}
.tabs5 li.active a {color:#2A62FF;font-weight:bold;}

.tab_content5:nth-child(2) .area_map{width: 715px; height: 100%;background:url("/images/lvs/img/popup-map.png") center center no-repeat; }
.tab_content5{width: 100%;height: 470px;position: relative;}
.tab_content5 .sop_control{width: 196px; height: 217px;background:url("/images/lvs/img/sop_control.png") center center no-repeat;position: absolute;z-index: 999; bottom: 0; right: 0;}

.tab_content5 ul{width: 715px; height: 100%;}

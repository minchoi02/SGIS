.extend_panel{display: flex;align-items: center;}
.extend_panel a{margin:0 10px;}
.extend_panel a.active{color: #2A62FF;}
.extend_panel button{background: #fff;}
.extend_panel .btn_download{padding-right: 15px;}
.extend_panel > span{width: 1px;height: 13px;background: #ccc;}

.location_more_box{min-width: 400px; height: 135px;background: #EDF1F4;position: absolute;top:50px;right: 30px;border: 1px solid #666666;color: #444;box-shadow: 2px 5px 10px #dedede;display: none; z-index: 99;}
.location_more_box .heading{ background: #fff;height: 45px; padding: 0 20px;font-size: 16px;}

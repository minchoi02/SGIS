.extend_data_01 .main .main_wrap{height: 100%;display: flex;color:#666;}
.extend_data_01 .main .main_wrap > li:nth-child(1){flex: 1;background: #fff;margin-right: 10px;padding: 15px 20px;}
.extend_data_01 .main .main_wrap > li:nth-child(2){flex: 2;background: #fff;padding: 15px 20px;}

.extend_data_01 .main dl h5{font-weight:bold; margin-bottom: 15px;color:#666;}
.extend_data_01 .main dl h4{font-size: 30px; font-weight:bold; color:#2A62FF; margin-bottom: 15px;}
.extend_data_01 .main dl h4::before{background: url("/images/lvs/icon/icon-main-img.png") center center no-repeat;content: "";width: 30px;height: 30px;display: inline-block;margin-right: 10px;}

.main dl div{margin-bottom: 10px;}
.main dl div > span:nth-child(2), .main dl div > span:nth-child(4){font-weight:bold; margin-right: 20px;color: #444;}
.main dl dd:nth-child(2){width: 100%;line-height: 40px;background: #EDF1F4;text-align: center;margin-top: 24px;}
.main dl dd:nth-child(2) span:nth-child(2){font-size: 18px;font-weight:bold;}

.extend_data_02{width: 100%;height: 903px;padding: 10px; display: block;background: #EDF1F4;}
.extend_data_02 .main{width: 780px; height: 100%;}

/* -----------------------tab2------------------- */
.tab_container2{height: 100%;}
.tab_container2 > span{float:left;}
.tab_container2 .tab_content2 {width: 432px;height:140px;box-sizing:border-box;clear: both;}
.tabs2 {width: auto;height: auto;float: right;margin-bottom: 10px;}
.tabs2 li {width: 26px;height: 26px;border:1px solid #E2E2E2;float:left;border-radius: 2px;}
.tabs2 li:first-child {margin-right:5px;background: url("/images/lvs/icon/icon-reports-grey.png") center center no-repeat; }
.tabs2 li:last-child { background: url("/images/lvs/icon/icon-page-grey.png") center center no-repeat;}
.tabs2 li a {display: block;width: 100%; height: 100%;display: block;}
.tabs2 li:first-child.active{background: url("/images/lvs/icon/icon-reports.png") center center no-repeat;border: 1px solid #00B271;}
.tabs2 li:last-child.active{background: url("/images/lvs/icon/icon-page.png") center center no-repeat;border: 1px solid #00B271;}
.tabs2 li.active a {color: #000;}

.tab_content input[type="radio"] {vertical-align: middle;appearance: none;border: 1px solid #ccc;border-radius: 0;width: 14px;height: 14px;}
.tab_content input[type="radio"]:checked {background: url("/images/lvs/icon/icon-check.png") center center no-repeat;border: 0;}
.tab_content input[type="radio"]:checked+label {font-weight:bold;}
.tab_content input[type="radio"]:checked+label::before {content: ''; position: fixed;background: url("/images/lvs/icon/icon-check.png") center center no-repeat;}

.tab_content2:last-child{background: #EDF1F4;padding: 15px; word-break:break-all;overflow: scroll;overflow-x: hidden;}
.tab_content2:last-child::-webkit-scrollbar {width: 5px;}
.tab_content2:last-child::-webkit-scrollbar-thumb{height: 30%;background: rgba(102, 102, 102, 0.5);border-radius: 10px;}
.tab_content2:last-child::-webkit-scrollbar-track {background: #ccc;}
.tab_content2:last-child p{margin-bottom: 20px;}
.tab_content2:last-child span{font-weight:bold;color:#FF5F5F;}

/* --------------------slider----------------- */
.slick_wrap{ width: 798px;height: 650px;padding: 0 30px;}
.arrows > div{width: 20px; height: 10px;position: absolute;z-index: 999999px;}
.arrows > .prev_arrow{top:60%;left: 5px;}
.arrows > .next_arrow{top:60%;right: 0px;}
.slick_slider{height: 622px; position: relative;}
.slick_slider .slider{width: 100%;height: 622px;background: #fff; }
.slick_slider .slider > ul{display:flex;flex-wrap: wrap;}

.slider_item{width: 367px;height: 208px;padding: 20px;position: relative;background: #fff;}
.slider_item .item_heading {margin-bottom: 40px;}
.slider_item .item_heading h5{float: left;font-weight: bold;color: #666;color: #444;}
.slider_item .item_heading img{display: inline;margin-left: 5px;cursor:pointer;}
.slider_item .item_heading span{float: right;}

.slider_item .tab_item h5{float: left;font-weight: bold;color: #666;color: #444;}
.slider_item .tab_item img{display: inline;margin-left: 5px;cursor:pointer;}
.slider_item .tab_item .new{display:inline;font-size: 10px;padding: 2px;background:#FC821C;border: 1px solid #FFA822;border-radius: 3px;color: #fff;}

.slider_item:nth-child(2n){border-left: 1px solid rgba(204, 204, 204, 0.3);border-bottom: 1px solid rgba(204, 204, 204, 0.3);color: #666;}
.slider_item:nth-child(2n-1){border-bottom: 1px solid rgba(204, 204, 204, 0.3);color: #666;}

.slider_item .item_content li:first-child{width: 120px;float: left;border-right: 1.5px dashed #ccc;}
.slider_item .item_content li > div p{font-size: 17px; font-weight: bold;margin-top: 7px;color: #0B0E11;}
.slider_item .item_content li .last_year{margin-top: 32px;}
.slider_item .item_content li .last_year p{color:#204EE3;}
.slider_item .item_content li:nth-child(2){width: 160px;float: right;}
.slider_item .item_content li:nth-child(2) div{margin-bottom: 5px;}
.slider_item .item_content li:nth-child(2) strong{color:#444;margin-left: 36px;}
.slider_item .item_content li:nth-child(2) div .cr01::before{content: "";width: 10px;height: 10px;display: inline-block;background: #05A8F5;margin-right:5px;}
.slider_item .item_content li:nth-child(2) div .cr02::before{content: "";width: 10px;height: 10px;display: inline-block;background: #FF5F5F;margin-right:5px;}
/* -----------------------tab3--------------------- */

.localinfo div:not(:last-child){margin-right:5px;}
.localinfo div:not(.selected_data){background:#fff;color:#444;}
.selected_data{background:#FC821C;font-weight:bold; color:#fff;}
.btn_data_open{padding:12px 11px;border:1px solid #444;background: #FC821C;color:#fff;font-weight:bold;border-radius: 2px;}
.btn_data_open:hover{background:#FF7A00;}


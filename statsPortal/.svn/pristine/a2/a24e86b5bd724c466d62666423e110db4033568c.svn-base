/* 함께하는 지도 */
.legend{margin-right: 50px;}
.legend .level~.level{margin-left: 14px;}
.legend .level::before{display:inline-flex; justify-content: center; align-items: center; width:18px; height:18px; text-align: center; border-radius: 100%; border:2px solid; font-size: 1.1rem; font-weight: 600; vertical-align: middle;}
.legend .level.level01::before{content:"A"; background-color: #BBF4E6; border-color:#8BDDBA; }
.legend .level.level02::before{content:"P"; background-color: #DBD1FF; border-color:#B7A2FF; }
.legend .level.level03::before{content:"L"; background-color: #D8E5FF; border-color:#9fbfff; }
.legend .level.level04::before{content:"C";background-color: #FED9F8; border-color:#EFAAFF;}
.legend .level span{margin-left: 5px; font-size:1.3rem;}

/* 해상도 ~ 1680px */ 
@media all and (max-width:1680px){

    .card li a{padding:20px 40px;}
}

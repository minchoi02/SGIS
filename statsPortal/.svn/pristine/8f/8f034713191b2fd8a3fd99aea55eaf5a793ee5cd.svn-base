
.wrap{ width:100%; height:100vh;margin:0 auto; background:#000; min-width: 1600px; }
.active{background:#204EE3;}

/* --------------------------------LNB 작성자:유지혜(2022/10/05)---------------------------------------- */
.aside{width: 320px;height:100%;float:left;position: fixed;left: 0;top:0px;bottom: 0;z-index: 2;}
.aside h1{width: 100%; height:  200px; background:#999;}
.aside h1 a{}
.aside h1 p{}

.aside .content{ background: #fff;height:calc(100% - 200px); padding: 20px 20px 10px 20px;}
.gnb_wrap{width: 100%;height:auto;margin-bottom: 25px;}
.gnb_wrap .btn_extend_open{width: 100%;background: #0F3C9A;padding:14px 20px;text-align:left;margin-bottom:20px;}

.gnb{width:100%;height: 35px;margin-bottom:20px;}
.gnb li{height:auto;float: left;}
.gnb li a{display:block; text-align:center;width:100%;}
.gnb li a img{width: 100%; height: 20px; background: #555;display:block;}

.menu{width:100%;height:auto;background: #fff;}
.tab{clear:both; display:flex; margin-bottom:15px; border:1px solid #204EE3;}
.tab li{padding: 12px 14px;}

.menu_list {width: 100%; height: 18px;margin-bottom:14px;}
.menu_list_wrap{float:left;}
.menu_list .tooltip{width: 18px; height: 18px; background: #666;float:right;}


}

.listArea{
  background: #f3f5f7;padding:30px 0;
  .inner{
    overflow:hidden;
    &>ul{
      margin:-19px 0 0 -24px;
      &>li{
        position:relative;float:left;width:312px;height:154px;background:#fff;margin:19px 0 0 24px;padding:25px 16px 16px;
        &>strong{display:block;margin-bottom:16px;font-weight: normal;font-family: 'Nanum Square Bold';color:#222222;font-size:19px;text-indent:5px;}
      }
    }
  }
  li li{
     margin-top:11px;
     &:first-child{margin-top:0;}
     &:before{content:"·";display:inline-block;margin-right:3px;}
     a{
       display:inline-block;width:265px;color:#666;white-space: nowrap;overflow: hidden;text-overflow: ellipsis;
       &:hover{color:#3680c7;text-decoration: underline;}
     }
     span{color:#a5a5a5;font-size:13px;}
  }
  .imgArticle{
    border:1px solid #f1f2f4;box-sizing:border-box;background: #f1f2f4;
    &>div{
      position:relative;float:left;width:130px;height:74px;padding:25px 15px 15px;
      &:first-child{width:150px;padding:0;}
    }
    strong,span{display: block;}
    strong{display:block;height:57px;font-size:17px;color:#555555;font-family: 'Nanum Square Bold';}
    span{position:absolute;bottom:15px;left:15px;font-size:13px;color:#a6a6a6;}
  }
  .sgs{
    padding-top:7px;
    li{
      position:relative;float:left;background:url("../images/main/img_gear.png") no-repeat center 0;font-family: 'Nanum Square Bold';color:#777;font-size:17px;text-align: center;width:156px;margin:0;
      &:first-child{background:url("../images/main/img_thinking.png") no-repeat center 0 ;}
      &:before{content:"";position:absolute;top:10px;left:0;width:1px;height:110px;background:#e3e6ea;}
      &:first-child:before{display:none;}
    }
    a{
      display:block;width:100%;height:100%;padding-top:110px;color:#222222;
      &:hover{color:#222222;text-decoration:none;}
    }
  }
  .btn_more{position:absolute;top:28px;right:25px;width:14px;height:14px;background:url("../images/main/btn_more.png") no-repeat;}
}


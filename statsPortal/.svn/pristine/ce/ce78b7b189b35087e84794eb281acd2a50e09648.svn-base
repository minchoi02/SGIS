body {font-size:14px; font-family:'Pretendard'; letter-spacing:-0.5px;}

.logo_box {width: 100%;height: 160px;background: url("/images/lvs/img/logo-bcakground.png") center center no-repeat;background-position: 0 -40px;padding: 10px 20px 20px 20px;}
.logo_box .logo {display: block;width: 100%;height: 100px;}
.logo_box h1 {display: block;width: 100%;padding-top: 20px;font-size: 20px;font-weight: bold;color: #fff;}

.header {width: 320px;position: fixed;left: 0px;top: 0px;z-index: 10;}
.container {width: calc(100% - 320px);height: 100vh;position: fixed;right: 0px;top: 0px;z-index: 10;padding: 10px;}



/* 상단 헤더 로케이션 + 데이터보드 버튼 */
.control_top{height:auto; position: absolute; left:330px; top:10px}
.localinfo div{float:left;  border-radius: 2px;padding:12px 11px;border:1px solid #444;}
.localinfo .county::before{content: ""; width: 10px;height: 13px;background: url("/images/lvs/icon/icon-location.png") center left no-repeat;padding-right: 15px;}
.localinfo div:not(:last-child){margin-right:5px;}
.localinfo div:not(.selected_data){background:#fff;color:#444;}
.selected_data{background:#FC821C;font-weight:bold; color:#fff;}
.btn_data_open{position:absolute; right:10px; top:10px; padding:12px 11px;border:1px solid #444;background: #FC821C;color:#fff;font-weight:bold;border-radius: 2px;}
.btn_data_open:hover{background:#FF7A00;}

/* 데이터 보드 에니메이션 */
#extendBoard {
    width:250px; 
    position: absolute; right:0; top:50px;
    background:#EDF1F4;border:1px solid #444;
    z-index:10;margin:10px;
    -webkit-transition: all 0.3s ease-out; -o-transition: all 0.3s ease-out; transition: all 0.3s ease-out;
}
#extendBoard.active {
    width: 800px; height:calc(100% - 20px);
    top:0;
    -webkit-transition: all 0.3s ease-out; -o-transition: all 0.3s ease-out; transition: all 0.3s ease-out; }
#extendBoard .extend_data_header {height: 44px;background: #fff;padding: 0 15px;line-height: 44px;display: -webkit-box;display: -ms-flexbox;display: flex;-webkit-box-pack: justify;-ms-flex-pack: justify;justify-content: space-between;}

#extendBoard .extend_data_header h2 {font-size: 18px; font-weight: bold;color:#2A62FF;}

#extendBoard .extend_panel {display: -webkit-box;display: -ms-flexbox;display: flex; -webkit-box-align: center; -ms-flex-align: center; align-items: center;}


#extendBoard .extend_panel a{display: none;}
#extendBoard.active .extend_panel a{margin:0 10px; display: block;}
#extendBoard.active .extend_panel a.active{color: #2A62FF;}

#extendBoard .extend_panel button {display: none;}
#extendBoard.active .extend_panel button{background: #fff; display: block;}
#extendBoard .extend_panel button.data_player {display: -webkit-box;display: -ms-flexbox;display: flex; -webkit-box-align: center; -ms-flex-align: center; align-items: center; width:24px; height:24px; -webkit-box-pack: center; -ms-flex-pack: center; justify-content: center;}
#extendBoard .extend_panel button.data_player:hover {border:1px solid #2A62FF}
#extendBoard .extend_panel button.data_player i.controller {background: url(/images/lvs/icon/icon-play-btn.png); width:12px; height:12px; display: block;}
#extendBoard .extend_panel button.data_player[data-status='play'] i.controller {background-position: -1px 0;}
#extendBoard .extend_panel button.data_player[data-status='pause'] i.controller {background-position: 9px 0;}

#extendBoard.active .extend_panel .btn_download{padding-right: 15px;}

#extendBoard.active .extend_panel > span{width: 1px;height: 13px;background: #ccc;}

#extendBoard .extend_panel .location_more_box {display: none;}
#extendBoard.active .extend_panel .location_more_box{min-width: 400px; height: 135px;background: #EDF1F4;position: absolute;top:50px;right: 30px;border: 1px solid #666666;color: #444;-webkit-box-shadow: 2px 5px 10px #dedede;box-shadow: 2px 5px 10px #dedede;display: none; z-index: 101;}
#extendBoard.active .extend_panel .location_more_box .heading{ background: #fff;height: 45px; padding: 0 20px;font-size: 16px;}
#extendBoard.active .extend_panel .location_more_box .heading button{float: right;height: 100%;padding:0 12px;background: url("/images/lvs/icon/icon-close.png") center center no-repeat;}
#extendBoard.active .extend_panel .location_more_box .content{ padding: 0 20px; color: #666;}
#extendBoard.active .extend_panel .location_more_box .content strong{width: 60px;height: 14px; padding-left: 15px;background: url("/images/lvs/icon/icon-location.png")center left no-repeat;}
#extendBoard.active .extend_panel .location_more_box .content div{display: -webkit-box;display: -ms-flexbox;display: flex;height: 30px;-webkit-box-pack: justify;-ms-flex-pack: justify;justify-content: space-between;-webkit-box-align:center;-ms-flex-align:center;align-items:center;}
#extendBoard.active .extend_panel .location_more_box .content select{width: 140px;height: 30px;}
#extendBoard.active .extend_panel .location_more_box .content button{padding: 0 20px;line-height: 30px;background: #2A62FF;color:#fff; font-weight: bold;border-radius: 2px;}
#extendBoard.active .extend_panel .location_more.active{color:#2A62FF;}




#extendBoard .extend_data_01 .main {display: none;}
#extendBoard.active .extend_data_01 .main {display: block;}

#extendBoard .extend_data_01 .sliderWrap {overflow-y: inherit; padding:0; width:100%}
#extendBoard.active .extend_data_01 .sliderWrap {height: calc(100% - 218px);overflow-y: scroll;padding: 0 30px;}

#extendBoard .extend_data_01,
#extendBoard .extend_data_02 {width: 100%; padding: 15px 0px 0 0;}
#extendBoard.active .extend_data_01 {height:calc(100% - 44px) !important; padding: 22px 0px 15px 0px;}


#extendBoard .arrows {display: none;}
#extendBoard.active .arrows {display: block;} 
#extendBoard.active .arrows > div{position: absolute;z-index: 999999; cursor: pointer;}
#extendBoard.active .arrows > .prev_arrow{top:50%;left: 8px;}
#extendBoard.active .arrows > .next_arrow{top:50%;right: 8px;}
#extendBoard.active .arrows > div > img {width:10px; height:auto}

.slider {font-size: 0;}
.slider .slider_item .item_heading,
.slider .slider_item .item_content {font-size:14px}

#extendBoard .slick_slider .slider .slider_item {display: block; display: -webkit-box; display: -ms-flexbox; display: flex; -webkit-box-align: center; -ms-flex-align: center; align-items: center; -webkit-box-pack: justify; -ms-flex-pack: justify; justify-content: space-between; padding:10px 20px; background: #fff; 
    border-bottom:1px solid #ededed; }
#extendBoard.active .slick_slider .slider .slider_item{display: inline-block; width:50%; padding: 15px;position: relative;background: #fff; border:1px solid rgb(204, 204, 204,0.3); min-height: 185px;}

#extendBoard .sliderWrap .slider_item {cursor: pointer;}
#extendBoard .sliderWrap .slider_item:hover,
#extendBoard .sliderWrap .slider_item.active {background: rgba(42, 98, 255,0.1)}

#extendBoard.active .sliderWrap .slider_item:hover,

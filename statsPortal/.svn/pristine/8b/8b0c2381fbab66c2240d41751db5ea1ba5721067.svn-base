/*quizBox*/
.quizBox {
    padding: 30px 50px;
    box-sizing: border-box;
}

.quizList {
    margin-bottom: 40px;
    /*    border: 1px solid red;*/
}

.quizList .quiz {
    margin-bottom: 12px;
    font-size: 14px;
}

.quiz .no {
    color: #0065b0;
    font-weight: 600;
    margin-right: 5px;
}


/* .p-i */
.p-i {color:#fff; width:50px; margin:0 auto; font-size:11px; line-height:150%; margin-bottom:20px;}
.p-i .p1 {font-size:14px; font-weight:600; margin-bottom:14px;}
.p-i .p2 {margin-bottom:24px;}
.p-i .p3 {margin-bottom:28px;}
.p-i .p3 label {margin-left:5px; margin-right:10px;}
.p-i .p4 {line-height:200%; margin-bottom:10px;}
.p-i .p4 input { background-color:#3bb485; border:1px solid #3bb485;	border-bottom:2px solid rgba(255,255,255,0.2); width:130px; margin-left:5px;}


.quizBtn{width:100%;height:auto;overflow:hidden;text-align:center;margin-top: 20px;}
.quizBtn a{margin:0 10px;}

.btnType01{width:110px;height:32px;line-height:32px;display:inline-block;overflow:hidden;text-align:center;color:#fff;border-radius:5px;border:1px solid #f9a93c;background:#ffb657;box-shadow:1px 1px 0 #777;font-size:15px;font-family:'NanumGothicBold';}
.btnType01:hover{background:#e09734;border:1px solid #e09734; cursor:pointer;}

.btnType02{width:110px;height:32px;line-height:32px;display:inline-block;overflow:hidden;text-align:center;color:#fff;border-radius:5px;border:1px solid #55c2bf;background:#60c9c6;box-shadow:1px 1px 0 #777;font-size:15px;font-family:'NanumGothicBold';}

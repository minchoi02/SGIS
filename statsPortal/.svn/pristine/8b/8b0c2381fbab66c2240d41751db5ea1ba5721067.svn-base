.lh24 {line-height: 24px;}
/* text-indent */
.ti10 {text-indent: 10px;}
.ti15 {text-indent: 15px;}
/* paragraph */
p.para {line-height: 1.7;letter-spacing: -0.6px;}
/* skip */
#skip {position: relative;width: 100%;z-index: 10}
#skip a {position: absolute;left: 0;top: -30px;width: 100%;background-color: #000;color: #fff;font-weight: bold;text-align: center;padding: 5px 0}
#skip a:active,
#skip a:focus {top: 0}

/* 버튼 */

#gnb li a {
  display: block;
  height: 110px;
  width: 100%;
  position: relative;
  background-color: transparent;
  transition: all 0.25s;
  text-align: center;
  padding-top: 55px;
  font-size: 15px;
  color: rgba(255, 255, 255, 0.4);
  font-weight: 700; }

#gnb li a:before {
  content: "";
  display: block;
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  text-align: center;
  height: 55px;
  background-repeat: no-repeat;
  opacity: 0.4;
  transition: 0.25s; }

#gnb li a:after {
  content: "";
  display: block;
  position: absolute;
  left: 10px;
  right: 10px;
  bottom: 0;
  height: 1px;
  background-color: #fff;
  opacity: 0.1; }

#gnb li.current a,
#gnb li a:hover,
#gnb li a:focus {
  color: #fff;
  background: -webkit-gradient(linear, left top, right top, color-stop(0, #196aac), color-stop(0.8, #1287a1));
  background: -moz-linear-gradient(left, #196aac 0%, #1287a1 80%);
  background: -webkit-linear-gradient(left, #196aac 0%, #1287a1 80%);
  background: -o-linear-gradient(left, #196aac 0%, #1287a1 80%);
  background: -ms-linear-gradient(left, #196aac 0%, #1287a1 80%);
  background: linear-gradient(left, #196aac 0%, #1287a1 80%); }

#gnb li a:hover:after,
#gnb li a:focus:after {
  opacity: 0; }

#gnb li.current a:before,
#gnb li a:hover:before,
#gnb li a:focus:before {
  opacity: 1; }

#gnb li.mn1 a:before {
  background-image: url("/images/administStats/renew/mn1_icon.png");
  background-position: 50% 9px; }

#gnb li.mn2 a:before {
  background-image: url("/images/administStats/renew/mn2_icon.png");
  background-position: 50% 17px; }

#gnb li.mn3 a:before {
  background-image: url("/images/administStats/renew/mn3_icon.png");
  background-position: 50% 9px; }

#gnb li.mn4 a:before {
  background-image: url("/images/administStats/renew/mn4_icon.png");
  background-position: 50% 9px; }

#gnb li.mn5 a:before {
  background-image: url("/images/administStats/renew/mn5_icon.png");
  background-position: 50% 19px; }


.list-panel.bg-gray .searchbox .search .searchLayer { display:none; width:309px; /* height:320px; */ box-sizing:border-box; position:absolute; z-index:100; left:20px; top:60px; border-radius:10px; background-color:#fff;  border:1px solid #E8E8E8; filter:drop-shadow(2px 2px 2px  #E2E2E2); padding:15px;}

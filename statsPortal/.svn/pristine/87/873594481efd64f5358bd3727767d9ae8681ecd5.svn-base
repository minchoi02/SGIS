.list-panel.bg-gray .searchbox .search .searchLayer table th span { display:inline; box-sizing:border-box; padding:3px 10px;  font-size:13px; font-weight:bold; background-color:#E9ECFF; border-radius:10px; margin-left:5px; }
.list-panel.bg-gray .searchbox .search .searchLayer .searchClose { cursor:pointer; float:right;}
.list-panel.bg-gray .searchbox .search .searchLayer table td { font-size:14px; color:#333; text-indent:22px; padding:5px; font-weight:500; }
.list-panel.bg-gray .searchbox .search .searchLayer table td img { vertical-align:middle; }
.list-panel.bg-gray .searchbox .search .searchLayer table td #img1 {width:15px; margin-left:2px;}
.list-panel.bg-gray .searchbox .search .searchLayer table td #img2 {width:15px; margin-left:2px;}
.list-panel.bg-gray .searchbox .search .searchLayer .scrollBox { width:100%; height:150px; overflow-y:auto;}
.list-panel.bg-gray .searchbox .search .searchLayer .scrollBox::-webkit-scrollbar { width: 15px; }
.list-panel.bg-gray .searchbox .search .searchLayer .scrollBox::-webkit-scrollbar-thumb { background-color: #E9ECFF; border-radius:30px; }
.list-panel.bg-gray .searchbox .search .searchLayer .scrollBox::-webkit-scrollbar-track { background-color: #F8F8F8; border-radius:30px;}

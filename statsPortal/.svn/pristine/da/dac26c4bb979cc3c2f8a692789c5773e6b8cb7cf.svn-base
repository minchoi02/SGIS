/* @import url(https://cdn.jsdelivr.net/gh/moonspam/NanumSquare@1.0/nanumsquare.css); */
body { padding:0; margin:0; font-family: 'NanumSquare', sans-serif; letter-spacing:-1px; }
a { outline:none; text-decoration:none; cursor:pointer; }
img { max-width:100%; }
ul { padding:0; margin:0; }
li { list-style:none;}

select { font-family: 'NanumSquare', sans-serif;}
#wrapper22 { width:100%; margin:0 auto; position:relative; overflow:hidden; }
#wrapper22 .container { width:100%; height:calc(100vh - 70px); margin:0 auto; position:relative; overflow:hidden; background-color:#F6F8FC; padding:15px; }

#wrapper22 .container .searchlist { width:350px; height:100%; position:relative; float:left; box-sizing:border-box; border-right:1px solid #E3E3EA; background-color:#fff;  }
#wrapper22 .container .searchlist .searchbox { position:relative; width:100%; float:left; box-sizing:border-box; border-bottom:1px solid #E3E3EA; padding:15px 20px;}
#wrapper22 .container .searchlist .searchbox .search { width:100%; margin:0 auto; box-sizing:border-box; background-color:#F8F8F8;  border-radius:30px; padding:5px 20px; border:1px solid #1F295A;}
#wrapper22 .container .searchlist .searchbox .search .searchLayer { display:none; width:309px; height:320px; box-sizing:border-box; position:absolute; z-index:100; left:20px; top:60px; border-radius:10px; background-color:#fff;  border:1px solid #E8E8E8; filter:drop-shadow(2px 2px 2px  #E2E2E2); padding:15px;}


#wrapper22 .container .searchlist .searchbox .search .searchLayer table th { text-align:left; font-size:14px; color:#1F295A; padding-bottom:5px; }
#wrapper22 .container .searchlist .searchbox .search .searchLayer table th > img { width:15px; margin-top:-3px; vertical-align:middle; margin-right:5px; }
#wrapper22 .container .searchlist .searchbox .search .searchLayer table th span { display:inline; box-sizing:border-box; padding:3px 10px;  font-size:12px; font-weight:bold; background-color:#E9ECFF; border-radius:10px; margin-left:10px; }
#wrapper22 .container .searchlist .searchbox .search .searchLayer .searchClose { cursor:pointer; float:right;}
#wrapper22 .container .searchlist .searchbox .search .searchLayer table td { font-size:14px; color:#494949; text-indent:22px; padding-bottom:5px;}
#wrapper22 .container .searchlist .searchbox .search .searchLayer table td img { vertical-align:middle; }
#wrapper22 .container .searchlist .searchbox .search .searchLayer .scrollBox { width:100%; height:150px; overflow-y:auto;}
#wrapper22 .container .searchlist .searchbox .search .searchLayer .scrollBox::-webkit-scrollbar { width: 15px; }
#wrapper22 .container .searchlist .searchbox .search .searchLayer .scrollBox::-webkit-scrollbar-thumb { background-color: #E9ECFF; border-radius:30px; }
#wrapper22 .container .searchlist .searchbox .search .searchLayer .scrollBox::-webkit-scrollbar-track { background-color: #F8F8F8; border-radius:30px;}

.btnTagHidden { cursor:pointer;}



#wrapper22 .container .searchlist .searchbox .search input[type='text'] { border:none; background-color:#F8F8F8; font-size:14px; color:#616161; text-indent:10px; width:80%; height:25px; }
#wrapper22 .container .searchlist .searchbox .search input[type='text']:focus { outline:none; }
#wrapper22 .container .searchlist .searchbox .search button { border:none; cursor:pointer; background-color:#F8F8F8; font-size:19px; float:right;}

#wrapper22 .container .searchlist .listbox { width:96%; height:500px; margin:15px 2%; float:left;  box-sizing:border-box; padding:20px; overflow-y:auto;}
#wrapper22 .container .searchlist .listbox li { width:100%; float:left; padding:8px 10px; box-sizing:border-box; border-radius:5px;}
#wrapper22 .container .searchlist .listbox li:nth-child(even) { background-color:#FBFBFB;}
#wrapper22 .container .searchlist .listbox li a { font-size:13px; color:#494949; }
#wrapper22 .container .searchlist .listbox li a:last-child { display:inline; vertical-align:middle; float:right;}

#wrapper22 .container .searchlist .listbox::-webkit-scrollbar { width: 15px; }
#wrapper22 .container .searchlist .listbox::-webkit-scrollbar-thumb { background-color: #E9ECFF; border-radius:30px; }
#wrapper22 .container .searchlist .listbox::-webkit-scrollbar-track { background-color: #F8F8F8; border-radius:30px;}

#wrapper22 .container .searchlist .searchbox2 { width:100%; position:absolute; z-index:10; bottom:28px; left:0px;  border-top:1px solid #E3E3EA; box-sizing:border-box; padding:20px; background-color:#fff;}
#wrapper22 .container .searchlist .searchbox2 table { width:100%; }
#wrapper22 .container .searchlist .searchbox2 table th { font-size:14px; text-align:right; font-weight:600; color:#505050;}
#wrapper22 .container .searchlist .searchbox2 table td { text-align:left; padding:5px 0; text-indent:10px; }
#wrapper22 .container .searchlist .searchbox2 button {  margin-top:10px; background: linear-gradient(to right, #006FC7, #00C7F3); width:100%; height:40px; font-size:16px; font-weight:800; color:#fff; border:none; cursor:pointer; border-radius:10px; filter:drop-shadow(2px 2px 2px  #BDBDBD);border:1px solid #0465AB; }
#wrapper22 .container .searchlist .searchbox2 button:hover { background: linear-gradient(to left, #006FC7, #00C7F3); }


#wrapper22 .container .contentbox { width:calc(100% - 470px); height:100%; float:left; }
#wrapper22 .container .contentbox .nav { width:100%; float:left; box-sizing:border-box; padding:17px 20px; font-size:24px; font-weight:600; }
#wrapper22 .container .contentbox .nav i { font-size:30px; margin:auto 10px;}
#wrapper22 .container .contentbox .dashboardbox-type { width:1450px; float:left; box-sizing:border-box; padding:0 10px; display:flex; flex-wrap: nowrap; }
#wrapper22 .container .contentbox .dashboardbox-type .item {  box-sizing:border-box; background-color:#ddd; background-color:#fff; border:1px solid #E8E8E8; border-radius:20px; overflow:hidden; }



#wrapper22 .container .contentbox .dashboardbox-type .item .title { width:100%; float:left; font-size:17px; font-weight:600; padding:0 20px; border-left:3px solid #1F295A; margin-top:15px; color:#505050; box-sizing:border-box; line-height:27px;}

#wrapper22 .container .contentbox .dashboardbox-type .item .title button { float:right; border:none; background-color:#fff; cursor:pointer;}
#wrapper22 .container .contentbox .dashboardbox-type .item .title button img { width:25px; }


#wrapper22 .container .contentbox .dashboardbox-type .item .title2 {  float:left; font-size:17px; font-weight:600; padding:0 20px; border-left:3px solid #1F295A; color:#505050; box-sizing:border-box; line-height:27px;}




#wrapper22 .container .contentbox .dashboardbox-type .item .chartbox { width:100%; float:left; height:calc(100% - 75px);  }
#wrapper22 .container .contentbox .dashboardbox-type .item .division {
	width:100%; float:left; height:30px; box-sizing:border-box;
	border-top:1px solid #E3E3EA;  font-size:12px; color:#494949; text-align:center; line-height:30px; background-color:#FCFCFC;
	vertical-align:middle;
	overflow-x:auto;overflow-y:hidden;
	white-space:nowrap;
}

#wrapper22 .container .contentbox .dashboardbox-type .item .division::-webkit-scrollbar {
display: none;
}




select.customSelect {
  /* styling */
  background-color: #FCFCFC;
  border: 1px solid #E2E2E2;
  border-radius: 30px;
  display: inline-block;
  padding: 12px 100px 12px 30px;
  font-size:16px;
  font-weight:600;
	filter:drop-shadow(2px 2px 2px  #E2E2E2);
  /* reset */
  margin: 0;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  -webkit-appearance: none;
  -moz-appearance: none;
}

select.customSelect {
  background-image:
    linear-gradient(45deg, transparent 50%, #BFBFBF 50%),
    linear-gradient(135deg, #BFBFBF 50%, transparent 50%),
    linear-gradient(to right, #ccc, #ccc);
  background-position:
    calc(100% - 20px) calc(1em + 2px),
    calc(100% - 15px) calc(1em + 2px),
    calc(100% - 2.5em) 0.5em;
  background-size:
    5px 5px,
    5px 5px,
    1px 1.5em;
  background-repeat: no-repeat;
}

select.customSelect:focus {
  border-color: #007CCD;
  outline: 0;
}

select.customSelect:-moz-focusring {
  color: transparent;
  text-shadow: 0 0 0 #000;
}



select.customSelect2 {
  /* styling */
  background-color: #FCFCFC;
  border: 1px solid #E2E2E2;
  border-radius: 10px;
  display: inline-block;
  font-size:14px;
  font-weight:600;
	filter:drop-shadow(2px 2px 2px  #E2E2E2);
  /* reset */
  margin: 0;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;

  -webkit-appearance: none;
  -moz-appearance: none;
  appearance:none;

   background-image:
    linear-gradient(45deg, transparent 50%, #BFBFBF 50%),
    linear-gradient(135deg, #BFBFBF 50%, transparent 50%),
    linear-gradient(to right, #ccc, #ccc);
  background-position:
    calc(100% - 20px) calc(1em + 2px),
    calc(100% - 15px) calc(1em + 2px),
    calc(100% - 2.5em) 0.5em;
  background-size:
    5px 5px,
    5px 5px,
    1px 1.5em;
  background-repeat: no-repeat;
}


select.customSelect2:focus {

  border-color: #272F4B;
  outline: 0;
}

select.customSelect2:-moz-focusring {
  color: transparent;
  text-shadow: 0 0 0 #000;
}



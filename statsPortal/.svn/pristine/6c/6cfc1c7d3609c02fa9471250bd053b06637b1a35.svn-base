#ui-header-picker > div:nth-child(1){text-align:right;padding:0 15px 0 0;}
#ui-header-picker > div:nth-child(2){text-align:left;padding:0 0 0 15px;}

.button-group{vertical-align:middle;line-height:100%;display:inline-block;position:relative;min-width:140px;/* height: 25px; */}
.button-group>:last-child{right:0;}
.button-group > div{width:60%;text-align:center;position:absolute;left:20%;line-height:24px;}

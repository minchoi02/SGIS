.dscList dt{width:100%;height:26px;overflow:hidden;margin:5px auto;}
.dscList dt.mt15{margin-top:15px;}
.dscList dt>a{font-weight:bold;color:#595757;background:url(./images/ico/ico_up03.png) no-repeat 505px center;}

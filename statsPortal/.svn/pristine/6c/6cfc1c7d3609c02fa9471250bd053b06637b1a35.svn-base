/*  map */
#container.column-map{position:relative;width:100%;padding:0;margin:0 auto;overflow-y: scroll;height:  100%;}
.inner-wrap{width:70%;max-width:1278px;margin:0 auto;height: auto;padding-bottom: 55px;}

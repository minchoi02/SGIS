.button-group > div  h2 > span{font-family:'Nanum Square Bold';font-size:24px;color:#555;}
.button-group button{float:left;margin:0;padding:0;white-space:nowrap;cursor:pointer;}
.button-group button:after{top:2px;content:'';height:22px;width:22px;display:inline-block;position:relative;}
.button-group button.left-arrow:after{background:url(./images/btn_mapleft.png) no-repeat  center;}
.button-group button.right-arrow:after{background:url(./images/btn_mapright.png) no-repeat;}
.button-group button.left-arrow:hover:after{background:url(./images/btn_mapleft_on.png) no-repeat  center;}
.button-group button.right-arrow:hover:after{background:url(./images/btn_mapright_on.png) no-repeat;}

.tabs-content{position:relative;width:100%;padding-bottom:35px;}
.pip-slider-box{width:auto;margin:50px auto;text-align:center;font-size:14px;padding:3em 0 1em;transition:-webkit-transform .3s ease,width .3s ease,padding .3s ease;transition:transform .3s ease,width .3s ease,padding .3s ease;}
.pip-slider-box{position:relative;box-sizing:border-box;border:1px solid #e7e7e7;width:100%;height:118px;background:#f5f5f5;padding:15px 30px;}
.ui-slider-horizontal.green .ui-slider-handle{background:#afe32c;}
.ui-slider-horizontal.ui-slider-pips{margin-bottom:2.8em;}
.ui-slider-pips .ui-slider-pip-hide{display:none;}

.ui-slider-pips .ui-slider-pip-number .ui-slider-number{top:17px;position:absolute;margin-left:8px;display:block;color:#555;font-family:'Nanum Gothic Bold';font-size:14px;}
.ui-slider-pips .ui-slider-pip{width:2em;height:1em;line-height:1em;position:absolute;font-size:0.8em;color:#999;overflow:visible;text-align:center;left:20px;margin-left:-1em;cursor:pointer;}
.slider-pip.slider-pip-number{/* position:  relative; */}
.ui-slider.ui-slider-horizontal >  .slider-line{width:2px;top:0;position:absolute;left:2%;background:#f5f5f5;transition:all .4s ease;height:5px;margin-left:0;display:block;}
.slider-pip.slider-pip-number> span > span.slider-number{position:absolute;top:15px;color:#555;width:2em;margin-left:2em;}
.ui-slider-vertical.ui-slider-pips{margin-bottom:0;margin-right:2em;}
.ui-slider-float .ui-slider-tip,.ui-slider-float .ui-slider-tip-number{position:absolute;visibility:hidden;top:-40px;display:block;width:34px;margin-left:-17px;left:50%;height:20px;line-height:20px;background:white;border-radius:3px;box-shadow:0 0 3px rgba(0,0,0,0.4);text-align:center;font-size:12px;opacity:-1;transition:all 0.4s ease;color:#333;}
.ui-slider-float .ui-slider-handle:hover .ui-slider-tip,.ui-slider-float .ui-slider-handle:focus .ui-slider-tip,.ui-slider-float .ui-slider-pip:hover .ui-slider-tip-number{opacity:0.9;top:-30px;color:#333;visibility:visible;}
.ui-slider.ui-slider-horizontal .ui-slider-handle{width:20px;height:20px;cursor:pointer;border:1px solid #1778cc;background:#1778cc;-webkit-transform:rotateZ(45deg);transform:rotateZ(45deg);top:-30px;margin-left:-10px;}
.ui-slider.ui-slider-horizontal .ui-slider-handle:before{position:absolute;content:'';width:9px;height:9px;background-color:#fff;border-radius:50%;top:5px;margin-left:5px;}
.ui-slider-horizontal .ui-slider-handle{border-radius:50% 50% 0;transition:border .4s ease;}
/* .ui-slider.ui-slider-horizontal{width:100%;margin:30px 0;background:#00bcd4;border:none;border-radius:0;height:8px;} */
.ui-slider.ui-slider-vertical .ui-slider-handle{-webkit-transform:rotateZ(-45deg);transform:rotateZ(-45deg);left:-25px;margin-bottom:-10px}
.ui-slider.ui-slider-horizontal .ui-slider-pip .ui-slider-pips:not(.ui-slider-disabled) .ui-slider-pip:hover .ui-slider-label{color:#000;font-weight:700;}
.ui-slider.ui-slider-horizontal .ui-slider-pip span.ui-slider-label{position:absolute;top:5px;left:50%;margin-left:-1em;width:2em;font-family:'Nanum Gothic Bold';}
.ui-slider.ui-slider-horizontal .ui-slider-pip span.ui-slider-label{display:block;}
.ui-slider.ui-slider-horizontal .ui-slider-pip span.ui-slider-line{background:#fff;width:2px;height:5px;position:absolute;left:50%;top:-17px;}
.ui-slider .ui-slider-pip{top:8px;}
.ui-slider.ui-slider-horizontal .ui-slider-pip{width:2em;height:1em;line-height:1em;position:absolute;font-size:14px;color:#555;overflow:visible;text-align:center;top:17px;left:20px;margin-left:-1em;cursor:pointer;-webkit-touch-callout:none;-webkit-user-select:none;-moz-user-select:none;-ms-user-select:none;user-select:none;}
.ui-slider-pip.ui-slider-pip-label.ui-slider-pip:last-of-type span:first-child{left:12px;}



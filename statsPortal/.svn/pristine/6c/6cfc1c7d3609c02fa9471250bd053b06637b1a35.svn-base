.archive-list{padding:0;min-height:512px;overflow:hidden;margin-bottom:25px;}
.archive-list > li{margin:0px 1.5% 0 0;width:32.33333%;float:left;padding:0;}
.archive-list > li:last-child{margin:0;}
.archive-list > li .area{border:1px solid #d9dadf;height:510px;display:block;overflow:hidden}
.archive-list > li .area > .title{display:table;padding:10px 10px 10px 15px;text-align:left;background:#eeeeee;color:#333;font-family:'Nanum Gothic Bold';font-size:18px;box-sizing:border-box;height:46px;line-height:130%;border-bottom:1px solid #d9dadf;width:100%}
.archive-list > li .title > dt,
.archive-list > li .title > dd{width:auto;display:table-cell;}
.archive-list > li .title > dd{text-align:right}
.archive-list > li .title > dd dl{display:inline-block;font-family:'Nanum Gothic Bold';font-size:14px;line-height:20px;color:#555}
.archive-list > li .title > dd dl:nth-child(1){color:#1778cc;margin-right:5px;}
.tooltip{background:url(./images/ico/ico_tooltip01.png) no-repeat left center;display:inline-block;width:16px;height:16px;vertical-align:middle;margin-left:5px;}

#ui-header-picker{display:table;max-width:1078px;width:100%;margin:0 auto;margin-bottom:45px;line-height:0;overflow:hidden;}

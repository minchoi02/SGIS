@importÂ url(https://cdn.jsdelivr.net/gh/moonspam/NanumSquare@1.0/nanumsquare.css);

#wrapperMore { width:100%; margin:0 auto; position:relative; overflow:auto; }
#wrapperMore .container { width:100%; height:calc(100vh - 70px); margin:0 auto; position:relative; overflow:hidden; }
.contentbox-typeB { width:100%; margin:0 auto; position:relative; overflow:hidden; height:570px; }
.contentbox-typeC { 
	border: 1px solid #FF748E;
    display: flex;
    justify-content: space-between;
    height:270px;
    
    border-radius: 15px;
    padding: 15px 20px;
    overflow: hidden;
	
	
}

.contentbox-typeB .left-col { 
    width: 445px;
    height: 100%;
    float: left;
    
    border-radius: 15px;
    box-sizing: border-box;
    
    position: relative;
    padding:15px;
    background:url(/images/administStats/more1/people_back.png) no-repeat left bottom;
    background-size:135px;
    background-color: #1F60DE;
}

.contentbox-typeB .left-col .title { 
	width: 100%;
    margin-bottom: 6px;
    text-align: left;
    font-size: 17px;
    font-weight: normal;
    color: #fff;
}

.contentbox-typeB .left-col .subtitle {
	width: 100%;
    margin: 0 auto;
    text-align: left;
    font-size: 26px;
    font-weight: bold;
    color: #FEF527;
    margin-bottom: 10px;
}

.contentbox-typeB .left-col .mainCardfot { width:250px; float:left; margin-top:20px; margin-left:120px;}
.contentbox-typeB .left-col .mainCardfot .tit { 
	margin-bottom: 5px;
    width: 100%;
    border-left: 3px solid #fff;
    padding-left: 5px;
    font-size: 17px;
    color: #FEF527;
    font-weight:bold; 
}
#more1Chart6 .highcharts-background { fill:#1F60DE !important; }
#tmsrChart .highcharts-background { fill:#1F60DE !important; }
#more1Chart11 .highcharts-point { stroke-width:1px; stroke:#181818;}



.contentbox-typeB .right-col { 
    width: calc(100% - 445px);
    height: 100%;
    float: left;
    padding-left: 10px;
    box-sizing: border-box;
}
.contentbox-typeB .selection { width:100%; margin:0 auto; position:relative;  }
.contentbox-typeB .selection.top { display:flex; justify-content:space-between;}

.contentbox-typeB .selection .rcon_box { 
	position: relative;
    background: #FFFFFF;
    border: 1px solid #BEBEBE;
    
    border-radius: 15px;
    padding: 15px 20px;
    overflow:hidden; 
}
.contentbox-typeB .selection .rcon_box.type2 { 
	border:1px solid #0078D5;
	display:flex; 
	justify-content:space-between; 
}


.contentbox-typeB .selection .rcon_box .rcon_tit {
	
	width: 100%;
    border-left: 3px solid #1f60de;
    padding-left: 5px;
    font-size: 15px;
    display:flex; justify-content:space-between; align-items:center; 
}

.contentbox-typeC .rcon_tit {
	
	width: 100%;
    border-left: 3px solid #1f60de;
    padding-left: 5px;
    font-size: 15px;
    display:flex; justify-content:space-between; align-items:center; 
}
.contentbox-typeC .conbody {
    display:flex; justify-content:space-between; padding-top:10px;  
}
.contentbox-typeC .conbody .chartImgBox {
    width:75px; display:flex; flex-direction:column; position:relative; text-align:center; font-weight:bold; color:#fff; 
}
.contentbox-typeC .conbody .chartImgBox img { width:100%; }
.contentbox-typeC .conbody .chartImgBox > span { position:relative; } 
.contentbox-typeC .conbody .chartImgBox span > span { position:absolute; z-index:100; bottom:13px; width:100%; left:0px; width:100%; }
.contentbox-typeC .conbody .chartImgBox span:last-child { margin-top:10px;}

.contentbox-typeB .selection .rcon_box .chartbox { width:100%; }
.contentbox-typeB .selection .rcon_box .division { 
	position: absolute;
    left: 0px;
    bottom: 0px;
    width: 100%;
    text-align: center;
    box-sizing: border-box;
    padding: 15px 0px;
}
.division { margin:0 auto; position:relative; overflow:hidden;  width:100%; text-align:center; box-sizing:border-box; padding:15px 0px;  }
.division_right { box-sizing:border-box; padding:15px 0px; color:#fff; font-size:12px; font-weight:600; position:absolute; right:5px; top: 40px; right: 10px;}

.flex_wrapper {display:flex; margin-bottom: 10px; width:150px; }
.flex_wrapper .i_building_01 {width:22%; margin:0px 10px 0px 0px;}
.flex_wrapper .i_building_02 {width:18%; margin:0px 15px 0px 2px;}
.flex_wrapper .i_building_03 {width:20%; margin:0px 14px 0px 0px;}
.flex_wrapper .i_building_04 {width:16%; margin:0px 18px 0px 2px;}
.flex_wrapper .flex_text div:nth-child(2) {font-size:14px; margin-top:2px;}



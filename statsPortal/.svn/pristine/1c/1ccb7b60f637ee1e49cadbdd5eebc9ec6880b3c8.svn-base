.contentbox-typeB .selection .rcon_box .rcon_tit span:last-child { display:flex; align-items:center; }
.contentbox-typeB .selection .rcon_box .rcon_tit span:last-child a { display:inline-block; height:25px; width:25px; }
.contentbox-typeB .selection .rcon_box .rcon_tit span:last-child a:nth-child(2) { width:auto; font-size:12px;font-size: 12px;
    background: #F7F5FF;
    border: 0.6px solid #C2C2C2;
    border-radius: 3px;
    padding: 4px 6px;
    margin: 0px 5px;}
.contentbox-typeB .selection .rcon_box .rcon_tit span:last-child a img { max-width:100%;}

.contentbox-typeC .rcon_tit span:last-child { display:flex; align-items:center; }
.contentbox-typeC .rcon_tit span:last-child a { display:inline-block; height:25px; width:25px; }
.contentbox-typeC .rcon_tit span:last-child a:nth-child(2) { width:auto; font-size:12px;font-size: 12px;
    background: #F7F5FF;
    border: 0.6px solid #C2C2C2;
    border-radius: 3px;
    padding: 4px 6px;
    margin: 0px 5px;}
.contentbox-typeC .rcon_tit span:last-child a img { max-width:100%;}





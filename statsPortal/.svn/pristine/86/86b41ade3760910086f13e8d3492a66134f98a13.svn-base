*{margin:0;padding:0;box-sizing:border-box;}
body{}
table{width:100%;border-collapse: collapse;}
img{vertical-align:bottom;}
textarea{resize:none;border:1px solid #ddd;}
a{text-decoration:none;}
.wrap{width:700px;margin:0 auto;}
.cont_wrap{padding:20px;background:url('./img/cont_bg.png');}
.cont{background:#fff;border-radius:30px;padding:20px;}
.cont h2{font-size:20px;background:#0070bb;border-radius:50px;padding:6px 20px;color:#fff;margin-bottom:20px;}
.cont h5{font-size:16px;margin-bottom:10px;}
.answer_wrap{margin-left:20px;margin-bottom:40px;}
table .answer_wrap{margin-bottom:0;}
.answer_wrap.layout_style01{display:flex;flex-wrap:wrap;}
.answer_wrap.layout_style01 p{width:calc(50% - 20px);}
.answer_wrap.layout_style02{display:flex;flex-wrap:wrap;}
.answer_wrap.layout_style02 p{width:calc(33.333% - 10px);}
.answer_wrap.layout_style03{display:flex;flex-wrap:wrap;}
.answer_wrap.layout_style03 p{width:calc(25% - 10px);}
.answer{display:flex;align-items:center;margin-bottom:6px;}
span.num{display:block;width:16px;height:16px;border-radius:50%;border:1px solid #555;font-size:12px;display:flex;align-items:center;justify-content:center;margin-right:6px;}
span.num.select{background: #0398fb;color:#fff;border: 1px solid #1fb9f9;}
.answer .sub_txt{font-size:12px;color:#ff0000;margin-left:10px;font-weight:600;}
.agree_wrap{margin:20px;padding:20px;background:#f1f1f1;}
.agree_txt{text-align:left;font-size:15px;padding:20px 0;}
.agree_chk_wrap{text-align:center;}
.user_info_wrap{margin:40px 20px 20px;}
.user_info_tit{color:#0070bb;font-size:18px;text-align:center;}
.btn_wrap{display:flex;align-items:center;justify-content: center;margin:40px 0;}
.btn_wrap .btn_submit{width:120px;height:40px;background:#0070bb;color:#fff;display:flex;align-items:center;justify-content:center;border-radius:20px;}
.btn_wrap .btn_submit:hover{background:#103f5f;}
.btn_wrap .btn_cancel{width:120px;height:40px;background:#ccc;margin-left:20px;color:#555;display:flex;align-items:center;justify-content:center;border-radius:20px;}
.btn_wrap .btn_cancel:hover{background:#bbb;}
.line{height:2px;background:#ddd;margin:30px 0;}

.table_style01{margin:20px 0;}   
.table_style01 th, .table_style01 td{text-align: -webkit-center;border:1px solid #ddd;padding:4px;}
.table_style01 th{background:#f1f1f1;}

button.num{width:16px;height:16px;border-radius:50%;border:1px solid #555;font-size:12px;align-items:center;justify-content:center;margin-right:6px;background-color:#FFF;cursor: pointer;}
button.num.select{background: #0398fb;color:#fff;border: 1px solid #1fb9f9;cursor: pointer;}
label{cursor: pointer;}
.f15{font-size:15px;}

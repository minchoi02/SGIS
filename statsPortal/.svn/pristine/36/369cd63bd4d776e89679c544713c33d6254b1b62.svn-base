
/* 상세 */
main>.btnS01{position: absolute; top:20px; right:20px;}
.imgTi{display: block; margin-bottom: 15px;}
.imgTi::before{content:""; display:inline-block; vertical-align: middle; margin-right:10px; width:15px; height:15px;}

main>.slideWrap{width: calc(100% - 125px); height: 100%;}
main>.slideWrap .btnPage02.prev{left:-35px;}
main>.slideWrap .btnPage02.next{right:-35px;}
main .slideObj{position: relative; width:100%; overflow: hidden;}
main .naviSlide{margin-top: 10px;}

.imgWrap{display: flex; /*background: #eeeeee54;*/ justify-content: center; align-items: center; height: 53.5vh; padding: 0 20px;}
.imgWrap figure{height: 100%; display: flex; flex-direction: column; justify-content: center;}
.imgWrap figure~figure{margin-left: 15px;}
.imgWrap figure img{width: 100%; max-height: calc(100% - 50px); border:1px solid #efefef;}
.imgWrap figure figcaption{font-size: 1.2rem; /*margin-top: 3px;*/ margin-bottom:4px;  text-align:center; color: #163064;}

.tip{ text-align: center; margin-bottom: 5px; font-size:1.4rem; /*text-decoration: #f9dd07 wavy underline;*/}
.tip::before{content:"Tip"; font-size:1.4rem; font-weight: 600;line-height: 25px; margin-right: 10px; background: #f9dd07;}

.classExp{margin-top: 25px; padding: 10px; border-radius: 10px;}
.classExp li{display: flex; font-family: 'notoSansKR', sans-serif; font-size: 1.5rem; counter-increment: number; letter-spacing:0.1pt;}
.classExp li~li{margin-top: 3px;}
.classExp li::before{content: counter(number);flex-shrink: 0; display: inline-block; text-align: center; width:15px; height:15px; line-height: 15px; font-weight: 600; font-size: 1.3rem; border-radius: 50px; margin-top: 3px; margin-right: 10px;}

.moreCont{position: absolute; width:100%; bottom:0px; left:0; transition-duration: .3s;}
.moreCont .btnFold{position: absolute; right:50px; top:0; border-radius: 15px 15px 0 0; padding:10px 20px 5px 20px; color:#fff; font-size: 1.4rem;}
.moreCont .btnFold.btnArr::after{background-image: url(../images/icon_btn_arrow02.png); transform: rotate(-90deg);}
.moreCont.on .btnFold.btnArr::after{background-image: url(../images/icon_btn_arrow02.png); transform: rotate(90deg);}
.moreCont article{height:0; overflow: hidden; background: #fff; margin-top: 35px; box-shadow: 0 0 18px 0 rgba(0, 0, 0, 0.2); transition-duration: .2s;}
.moreCont.on article{height: 198px;}
.moreCont article>div{margin: 25px auto;}
.moreCont .card li{border:1px solid #c2c2c2;}

.btnPage01.next{padding:20px 20px 20px 30px;}

.view .lnb, .write .lnb{padding-top:50px;}
/* 팝업 */
.popup h3{position: absolute; top:-20px; left:-25px; padding:10px 20px 6px 20px; font-size:1.8rem; color:#fff; border-radius:50px 50px 0 50px;}
.popup section{position: relative;}

/* 자세한 설명보기 / SGIS 활용하기 */
.detailExp .slideBox, .sgisExp .slideBox{width:1032px; height:600px; /*background: #efefef;*/}
.detailExp .btnPage02.prev, .sgisExp .btnPage02.prev{left:-40px;}
.detailExp .btnPage02.next, .sgisExp .btnPage02.next{right:-40px;}


/* .slideObj { padding: 15px;} */
.bodyExp .slideObj {padding:0px !important; } 
.slideObj .cn {font-size:1.5rem; padding-bottom:10px; font-family: 'notoSansKR', sans-serif;}
.slideObj .imgCn {text-align:center;padding:10px 0px;  background-color: #eeeeee59;}
.slideObj .imgs {margin:auto; max-width: 100%; vertical-align: middle; /*border: 1px solid #c3c3c3;*/ padding:3px;}
.slideObj .arrows {margin:auto; max-width: 97%; vertical-align: middle; padding:3px;} 
.slideObj .objNm {font-size:1.3rem; color: #0a1146;}

/* 20211210 퀴즈 레이아웃 변경 */
.quiz .incrt {display:none;}
.quiz .incrt.on {display:block;}
.quiz .wrong {display:none;}
.quiz .wrong.on {display:block;} 

/* 퀴즈 팝업 */
.quiz section{max-width: 950px; min-width: 950px;}
.quiz section .btnWrap {visibility:hidden; display:none;}
.quiz section .btnWrap.on {visibility:visible; display:flex;}
.quiz .quizTi{display: flex; font-size:2.8rem; width:78%; margin: 20px auto;}
.quiz .quizTi::before{content:"Q."; font-weight: 700; margin-right: 10px;}
.quiz .answer{ justify-content: center; margin-top: 50px;}
/* .quiz .answer button{display: inline-flex; justify-content: center; align-items: center; width:130px; height:130px; background: #dedede; border-radius: 20px; font-size:6.5rem; font-weight: 700; text-align: center; transition-duration: .2s;} */
/* .quiz .answer button{width: 95%; font-size: 1.8rem; border: 1px solid #aaa; border-radius: 40px; padding: 6px; text-align: left; padding-left: 91px;} */

.quiz .answer{margin-top:50px;}
.quiz .answer li{display: flex; counter-increment: num; border-radius: 50px; padding: 10px 15px; cursor: pointer; font-size: 2rem; word-break: break-word; background: #eaeaea; transition-duration: .3s;}
.quiz .answer li~li{margin-top: 7px;}
.quiz .answer li::before{content:counter(num); flex-basis:30px; flex-shrink: 0; width:30px; height:30px; margin-right: 15px; text-align: center; line-height: 35px; border-radius: 50px; background: #fff;}
.quiz .answer li:hover{box-shadow: 0 22px 18px -4px rgb(0 0 0 / 12%);}
.quiz .answer li:hover::before{background: #fff;}

/* .quiz .answer button:hover{color:#fff; box-shadow: 0 22px 18px -4px rgb(0 0 0 / 12%);} */
.quiz .exp{display: flex; height:100px;}
.quiz .exp img{}
.quiz .exp ul{width:calc(100%); background:rgb(2,191,160,0.1); border-radius: 20px; padding: 5px 20px; margin-left: 10px; height: 100px; overflow-y: scroll;}
.quiz .exp ul::before{content:"에듀 Tip"; font-size:1.4rem; font-weight: 600; width:100%; line-height: 35px;}
.quiz .exp.imgTrue img{max-width:512px; max-height:353px;}
.quiz .exp.imgTrue ul{width:calc(100% - 522px); background:rgb(2,191,160,0.1); border-radius: 20px; padding: 20px; margin-left: 10px;}
.quiz .exp.imgTrue ul::before{content:"에듀 Tip"; font-size:1.4rem; font-weight: 600; width:100%; line-height: 35px;}
/* .quiz .btnWrap .nextContents {display:none;} */
/* .quiz .btnWrap .nextContents.on {display:block;} */

/* .quiz .exp img{max-width:512px; max-height:353px;} */
/* .quiz .exp ul{width:calc(100% - 522px); background:rgb(2,191,160,0.1); border-radius: 20px; padding: 20px; margin-left: 10px;} */
/* .quiz .exp ul::before{content:"에듀 Tip"; font-size:1.4rem; font-weight: 600; width:100%; line-height: 35px;} */
.quiz .exp li{display: flex; font-family: 'notoSansKR', sans-serif;}
.quiz .exp li~li{margin-top: 7px;}
.quiz .exp li::before{content:"-"; color:rgba(24,24,24,0.5); margin-right: 5px;}
/* 정답,오답 */
.incrt .quizTi,.wrong .quizTi{justify-content: center; align-items: center; font-size:3.5rem;}
.incrt .quizTi::before,.wrong .quizTi::before{content:""; display:block; width:48px; height:48px; margin-top: 0px;}
.incrt .quizTi::before{background: url(/sgis_edu/resource/images/icon_quiz01.png)no-repeat 50%; animation: quiz01 2 .4s ease-out;}
.wrong .quizTi::before{background: url(/sgis_edu/resource/images/icon_quiz02.png)no-repeat 50%; animation: quiz02 2 .4s ease-out;}

@keyframes quiz01 {
    0% { transform: translateY(0)}
    50% { transform: translateY(-7px);}
    100% { transform: translateY(0)}
  }
@keyframes quiz02 {
    0% { transform: translateX(0)}
    50% { transform: translateX(-7px);}
    100% { transform: translateX(0)}
  }


.overEvent {position: relative; }
.overEvent img{cursor : pointer; }
.tooltip.on {visibility: visible;}
.tooltip-middle {visibility:hidden; text-align:center; position: absolute; top: 50%; right:50%; transform: translate(50%, -50%); margin:10px; transition-duration: .1s; box-shadow: 0 22px 18px -4px rgb(0 0 0 / 12%);}
.tooltip-topright {visibility:hidden; text-align:center; position: absolute; top: 40px; right:15px; margin:10px; transition-duration: .1s; box-shadow: 0 22px 18px -4px rgb(0 0 0 / 12%);}
.tooltip-bottomleft {visibility:hidden; text-align:center; position: absolute; bottom: 40px; left:10px; margin:10px; transition-duration: .1s; box-shadow: 0 22px 18px -4px rgb(0 0 0 / 12%);}

.tooltipBtn {position: absolute;right: 25px; bottom: 25px; width: 50px;height: 50px; border-radius: 50%; background: #0a1146d1; font-size: 33px; font-weight: 600; /*box-shadow: 0 22px 22px -4px rgb(0 0 0 / 15%);*/ border:1px solid #ffffff3b; z-index:10000; animation: tooltip01 1s 0.7s infinite alternate;}
.tooltipBtn:before {content: "+"; display: block; width: 100%; height: 100%; color:white;}
.tooltipBtn:hover {/*transform: rotate(90deg);*/ background:#0a1146; animation: tooltip02;}
.tooltipBtn.on {transform: rotate(45deg); background-color:#0d0808; animation: tooltip02;}


.clickEvent .clickImg {cursor:pointer; display:none;}
.clickEvent .clickImg.on {display:block; cursor:pointer;}



.privacy__con > div ul {
  padding-left: 10px; }

.btn-scroll {
  z-index: 999;
  position: fixed;
  bottom: 10%;
  right: 3%; }

.btn-scroll a {
  display: block; }

.btn-scroll a:first-child {
  margin-bottom: 15px; }

.btn-scroll img {
  width: 40px; }


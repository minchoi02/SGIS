  padding: 6px 10px;
  color: #515355;
  font-size: 14px; border-top:1px solid #b9c9e2}

.navi .sub-path {
  width: calc(100% - (41% +  41px)); }

.map__above.fixed {
  z-index: 1000;
  position: fixed;
  left: 0;
  top: 0;
  width: 100%; }

/* =====================
    footer
===================== */
.footer {
  padding: 20px 0 30px;
  background-color: #333333;
  text-align: center; }

.footer__link {
  display: flex; }

.footer__link li {
  width: 33.333%; }

.footer__link li:not(:first-child) a {
  border-left: 1px solid #cdcdcd; }

.footer__link a {
  display: block;
  padding: 9px 0;
  border-bottom: 1px solid #cdcdcd;
  background-color: #f0f1f2;
  color: #000;
  font-size: 15px;
  text-align: center; }

.footer .logo {
  margin-bottom: 10px; }

.footer p {
  margin-bottom: 13px;
  color: #adadad;
  font-size: 13px; }

.footer span {
  display: block;
  color: #727272;
  font-size: 13px; }

.footer__btn {
  display: flex;
  justify-content: center;
  margin-bottom: 15px; }

.footer__btn > * {

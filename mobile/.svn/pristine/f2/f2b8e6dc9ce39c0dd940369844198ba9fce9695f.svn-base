  font-size: 14px;
  color: #000; }

.state {
  padding-left: 5px;
  padding-right: 15px;
  background-position: right center;
  background-repeat: no-repeat;
  background-size: 13px 15px; }

.state-up {
  color: #d67c89;
  background-image: url(../img/dashboard/i_state-up.png); }

.state-down {
  color: #41a8b8;
  background-image: url(../img/dashboard/i_state-down.png); }

.state::before {

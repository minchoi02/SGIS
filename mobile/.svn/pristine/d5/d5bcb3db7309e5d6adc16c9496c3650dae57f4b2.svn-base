.sop-control.mapSetting .box .list ul li.sgis a.on{background:#21b69a;border:1px solid #1aa58b;}
.sop-control.mapSetting .box .close{right:0; float:right; display: block; width:30px; height:30px; background: url(../../images/common/icon_close.png) no-repeat center 0; background-size:20px; }


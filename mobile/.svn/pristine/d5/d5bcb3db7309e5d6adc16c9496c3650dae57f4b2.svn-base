/* POI 선택화면 시작*/
.POI_select {position:relative; box-sizing:border-box; left:0; z-index:600; width:100%; height:auto; background:#dae8f4;}
.POI_select ul {position:absolute;  left:3%; top:10px; width:94%; padding-bottom: 30px;}
.POI_select ul li{background:#eee; border:#a1a1a1 solid 1px; border-bottom:none; }
.POI_select ul li:last-child{border-bottom:1px solid #a1a1a1;}
.POI_select ul li a {display:block;  color:#000; padding:14px 0 14px 12px; width:25%; z-index:100 !important; }
.POI_select ul li.on a {position:relative; background:#fff; color:#457bc3;  border-right:none; font-size:14px; font-family:"NanumSquareB" }
.POI_select ul ul { position:absolute;  left:26%; top:0; box-sizing:border-box; height:420px; padding:6px 20px; overflow:auto;border:#a1a1a1 solid 1px; background:#fff; width:74%; z-index:1;display: none; }
.POI_select ul>li.on>ul { display:block; }
.POI_select ul ul li{border:none; border-bottom:none !important; background:#fff; }
.POI_select ul ul li a {font-size:12px; text-align:left; width:100%; padding:6px 0}
.POI_select ul li.on li a{color:#000; font-size:13px;}
.POI_select ul ul li:last-child a {border-bottom:none; }
.POI_select ul ul li a img {vertical-align:middle; margin-right:5px; width:30px;}
/* POI 선택화면 종료*/


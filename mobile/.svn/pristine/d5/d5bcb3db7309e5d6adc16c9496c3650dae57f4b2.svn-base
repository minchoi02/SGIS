/*통계목록,지도,범례,차트,표 상단 버튼 시작*/
.Btn_Top {position: relative;width: 100%;height: 42px; background: #ccc;}
.Btn_Top a {box-sizing: border-box;width: 25%;height: 42px;display: inline-block;text-align: center;font-size: 11px;color: #555;background-size: 25px 19px, 100% 42px;background-repeat: no-repeat, repeat-x;background-position: center 4px, left top;padding-top: 23px;border-left: #ccc solid 1px;}
.Btn_Top a.M_on {position: relative;box-shadow: 0px 2px 7px #999;font-family: "NanumSquareB";}
.Btn_Top a.Btn_Top1 {background-image: url(../../images/map/common/map1_menu1.png),url(../../images/map/common/btn_bottom_bg1.png);border-left: none;}
.Btn_Top a.Btn_Top2 {background-image: url(../../images/map/common/map1_menu2.png),url(../../images/map/common/btn_bottom_bg1.png);}
.Btn_Top a.Btn_Top3 {background-image: url(../../images/map/common/map1_menu3.png),url(../../images/map/common/btn_bottom_bg1.png);}
.Btn_Top a.Btn_Top4 {background-image: url(../../images/map/common/map1_menu4.png),url(../../images/map/common/btn_bottom_bg1.png);}
.Btn_Top a.Btn_Top5 {background-image: url(../../images/map/common/map1_menu5.png),url(../../images/map/common/btn_bottom_bg1.png);}
.Btn_Top a.Btn_Top1.M_on {background-image: url(../../images/map/common/map1_menu1.png),url(../../images/map/common/btn_bottom_bg2.png);border-left: none;}
.Btn_Top a.Btn_Top2.M_on {background-image: url(../../images/map/common/map1_menu2.png),url(../../images/map/common/btn_bottom_bg2.png);}
.Btn_Top a.Btn_Top3.M_on {background-image: url(../../images/map/common/map1_menu3.png),url(../../images/map/common/btn_bottom_bg2.png);}
.Btn_Top a.Btn_Top4.M_on {background-image: url(../../images/map/common/map1_menu4.png),url(../../images/map/common/btn_bottom_bg2.png);}
.Btn_Top a.Btn_Top5.M_on {background-image: url(../../images/map/common/map1_menu5.png),url(../../images/map/common/btn_bottom_bg2.png);}
.Btn_Top a.NoneAction {opacity: 0.4;cursor: not-allowed;}
/*통계목록,지도,범례,차트,표 상단 버튼 끝*/


/*지역 네비게이터 시작*/
.SelectArea {position:relative; top:0px; font-size:12px; height:30px; padding:6px 0 2px 13px; background:#457bc3; border-top:1px solid #38649e; box-shadow:inset 0px 1px 0px rgba(255, 255, 255, 0.1);}
.SelectArea select{border:1px solid #a1a1a1; padding: 3px 25px 3px 5px; height:26px; width: 24%; max-width: 200px; border-radius:0;}
.SelectArea select:first-child {}

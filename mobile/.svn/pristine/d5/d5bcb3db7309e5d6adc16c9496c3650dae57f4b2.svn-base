.sop-control.map-list>.button-box{z-index: 1; position: relative; width:58px; height: 28px; border:1px solid #6d6d6d; background-color:rgba(255, 255, 255, 0.7); font-size:12px; box-sizing:border-box; text-align:center; font-family: "NanumSquareB"; letter-spacing:-1px; padding-top:6px; bottom:80px;} 
.sop-control.map-list>.button-box>a{color:#333; cursor: pointer;}

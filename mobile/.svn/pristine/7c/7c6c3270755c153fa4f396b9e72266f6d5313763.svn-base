.point__list a:hover, .point__list a:active, .point__list a:focus, .point__list a:visited {
  background-color: #dde8fa; }

.point__select {
  top: 30%;
  display: none; }

.point__select::before {
  content: '';
  display: block;
  position: absolute;
  top: -8px;
  left: 50%;

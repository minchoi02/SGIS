.point__address dd {
  letter-spacing: -1px; }

.point__list {
  overflow-y: scroll;
  height: 70px;
  padding-left: 25px;
  font-size: 12px; }

.point__list li {
  border-top: 1px dashed #d5d5d5; }

.point__list a {

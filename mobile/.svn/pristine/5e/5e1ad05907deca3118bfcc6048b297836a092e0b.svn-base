@charset "utf-8";
body{}
/* Main */
.Main {}
.Main .Header {position:relative; height:auto;}
.Main .Header h1 {background:url(../images/main/main_logo.png) no-repeat center 32px; background-size:120px; padding:40px 0 25px; text-indent:-1000px; }

.Main .Content { padding:0; background: #dae8f4 }
/* 2019.09.06[한광희] 메인이미지 수정 START */
/* .Main .MainTitle{background:url(../images/main/main_bg.png) no-repeat center 0px; background-size:200px; text-align:center; box-sizing:border-box; padding:140px 0 15px;}
.Main .MainTitle .ti{display:block; color:#ea432f; font-size:16px; font-family:"NanumSquareB"; letter-spacing:-1px;}
.Main .MainTitle .txt{display:block; color:#4d4d4d; font-size:11px; margin-top:3px; line-height:16px;} */
.Main .MainTitle{ position:relative; background:url(../images/main/main_bg.png) no-repeat left calc(50% - 100px) top 10px; background-size:200px; text-align:left; box-sizing:border-box; height:155px;  padding:0; /*padding:140px 0 15px;*/}
.Main .MainTitle .ti{display:block; position:absolute; top:50px; left:55%; color:#ea432f; font-size:16px; font-family:"NanumSquareB"; letter-spacing:-1px;}
.Main .MainTitle .txt{display:block; position:absolute; top:70px; left:55%; color:#4d4d4d; font-size:11px; margin-top:3px; line-height:16px;}
/* 2019.09.06[한광희] 메인이미지 수정 END */
/* 2019.09.06[한광희] SGIS 일자리맵 추가 START */
.Main .MainWork{ display:block; position:relative; background:url(../images/main/main_work.png) no-repeat right top 12px; background-color:#fff07b;  background-size:190px; text-align:left; box-sizing:border-box; height:80px;}
.Main .MainWork a { display:block; width:100%; height:100%; }
.Main .MainWork .ti{display:block; position:absolute; top:18px; left:20px; color:#205a8d; font-size:16px; font-family:"NanumSquareB"; letter-spacing:-1px;}
.Main .MainWork .txt{display:block; position:absolute; top:38px; left:20px; color:#666; font-size:11px; margin-top:3px; letter-spacing:-1px; line-height:14px;}
/* 2019.09.06[한광희] SGIS 일자리맵 추가 END */
.Main .MainMenu a {float:left; display:block; width:50%; height:220px; box-sizing:border-box; padding:15px 0px 15px 20px; letter-spacing:-0.06em;}
.Main .MainMenu a.Box1 {background-color:#7c98ae; background-image:url(../images/main/main_go1.png), url(../images/main/btn_go.png); background-size:100px auto, 30px 30px; background-position:right bottom, 20px 170px; background-repeat:no-repeat;}
.Main .MainMenu a.Box2 {background-color:#a4cae7; background-image:url(../images/main/main_go2.png), url(../images/main/btn_go.png); background-size:120px auto, 30px 30px; background-position:right bottom, 20px 170px; background-repeat:no-repeat;}
.Main .MainMenu a span.ti{font-size:15px ; font-family:"NanumSquareB"; line-height:130%;}
.Main .MainMenu a.Box1 .ti{color:#fff071}
.Main .MainMenu a.Box2 .ti{color:#205a8d}
.Main .MainMenu a span.intro {display:block; font-size:11px; color:#FFF; margin-top:5px; line-height:150%;}

.Main .MainNotice {width:100%; overflow:hidden; background:#ceec95; padding:11px 20px 10px 22px; box-sizing:border-box;}
.Main .MainNotice h2 {float:left; color:#000; font-size:14px;}
.Main .MainNotice ul {width:auto; margin:3px 0 0 92px;}
.Main .MainNotice li{display:inline-block; width:100%;}
.Main .MainNotice li a{color:#424242; font-family:"NanumSquareB"}
.Main .MainNotice li em{float:right; font-size:11px;}

@media (max-width:320px) {
.Main .Header h1{padding:35px 0 37px;}
/* 2019.09.06[한광희] 메인이미지 수정 START */
/* .Main .MainTitle{background:url(../images/main/main_bg.png) no-repeat center 4px; background-size:180px; padding:127px 0 15px;} */
.Main .MainTitle{background:url(../images/main/main_bg.png) no-repeat left calc(50% - 95px) top 0px; background-size:180px; padding:0; height:130px;}
/* 2019.09.06[한광희] 메인이미지 수정 END */
/* 2019.09.06[한광희] SGIS 일자리맵 추가 START */
.Main .MainTitle .ti {top:30px; }
.Main .MainTitle .txt {top:50px; letter-spacing:-1px; }
.Main .MainWork{background:url(../images/main/main_work.png) no-repeat right top 20px;  background-color:#fff07b; background-size:140px;}
/* 2019.09.06[한광희] SGIS 일자리맵 추가 END */
.Main .MainMenu a {height:210px;}
.Main .MainMenu a.Box1 {background-color:#7c98ae; background-image:url(../images/main/main_go1.png), url(../images/main/btn_go.png); background-size:80px auto, 26px 26px; background-position:right bottom, 20px 165px; background-repeat:no-repeat;}
.Main .MainMenu a.Box2 {background-color:#a4cae7; background-image:url(../images/main/main_go2.png), url(../images/main/btn_go.png); background-size:100px auto, 26px 26px; background-position:right bottom, 20px 165px; background-repeat:no-repeat;}
.Main .MainMenu a span.ti{font-size:13px;}
}
.Service1 .Header h1, .Service2 .Header h1 {background:url(../images/main/sub_logo.png) no-repeat center center; background-size:100px; text-indent:-1000px; padding:7px 0 8px;;  }
.Service1 .Content {background:#7c98ae url(../images/main/service1_img.png) no-repeat 104% 24px; background-size:187px auto;}
.Service1 .title{font-size:17px; letter-spacing:-0.09em; padding:8% 0 0 7%; line-height:28px;}
.Service1 .ti{display:block; color:#fff071; font-family:"NanumSquareB";}
.Service1 .txt{display:block; color:#fff;}
.Service1 .menu{margin-top:50px;}
.Service1 .menu a {display:inline-block; float:left;  box-sizing:border-box; padding:17px 0px; letter-spacing:-0.06em; margin:0 3% 0 0;}
.Service1 .menu a.Box1 {width:45.5%; height:160px; background-color:#fff; background-image:url(../images/main/service1_img_current.png); background-size:105px auto; background-position:center 82px; background-repeat:no-repeat; margin:0 3%;  text-align:center;}
.Service1 .menu a.Box2 {width:45.5%; height:160px;  background-color:#ffda84; background-image:url(../images/main/service1_img_community.png); background-size:125px auto; background-position:center 74px; background-repeat:no-repeat;  text-align:center;}
.Service1 .menu a.Box3 {width:94%; height:90px; background-color:#ceec95; background-image:url(../images/main/service1_img_thematic.png); background-size:125px auto; background-position:94% center; background-repeat:no-repeat; margin:3%; padding-left:5%;}
.Service1 .menu a span.ti{font-size:15px ; font-family:"NanumSquareB"; line-height:130%;}
.Service1 .menu a .ti{color:#000; font-size:20px;}
.Service1 .menu a .intro{display:block; color:#444; font-size:11px; margin-top:5px; line-height:140%;} 

.Service2 .Content {background:#a4cae7 url(../images/main/service2_img.png) no-repeat 104% 24px; background-size:197px auto;}
.Service2 .title{font-size:17px; letter-spacing:-0.09em; padding:8% 0 0 7%; line-height:28px;}
.Service2 .ti{display:block; color:#205a8d; font-family:"NanumSquareB";}
.Service2 .txt{display:block; color:#fff;}
.Service2 .menu{margin-top:50px;}
.Service2 .menu a {display:inline-block; float:left;  box-sizing:border-box; padding:17px 0px; letter-spacing:-0.06em; margin:0 3% 0 0;}
.Service2 .menu a.Box1 {width:45.5%; height:160px; background-color:#fff; background-image:url(../images/main/service2_img_interactive.png); background-size:109px auto; background-position:center 78px; background-repeat:no-repeat; margin:0 3%;  text-align:center;}
.Service2 .menu a.Box2 {width:45.5%; height:160px;  background-color:#fecd96; background-image:url(../images/main/service2_img_house.png); background-size:120px auto; background-position:center 76px; background-repeat:no-repeat;  text-align:center;}
.Service2 .menu a.Box3 {width:94%; height:90px; background-color:#9eddc7; background-image:url(../images/main/service2_img_biz.png); background-size:125px auto; background-position:94% center; background-repeat:no-repeat; margin:3%; padding-left:5%;}
.Service2 .menu a span.ti{font-size:15px ; font-family:"NanumSquareB"; line-height:130%;}
.Service2 .menu a .ti{color:#000; font-size:20px;}
.Service2 .menu a .intro{display:block; color:#444; font-size:11px; margin-top:5px; line-height:140%;} 

.Service1 .Footer, .Service2 .Footer{z-index:1}
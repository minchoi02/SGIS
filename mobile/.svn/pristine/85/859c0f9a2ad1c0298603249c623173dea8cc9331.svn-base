<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<%@include file="/WEB-INF/jsp/includes/taglib.jsp" %>
<!DOCTYPE html>
<html>
<head>
	<title>검색</title>
	<meta name="title" content="검색">
	<link rel="stylesheet" href="${ctx }/resources/css/search.css" />
	<script src="${ctx }/resources/js/etc/search.js"></script>
</head>
<body>
	<div class="SearchBar">
		<form id="search-form" action="#" method="get">
			<input type="text" name="searchKeyword" id="searchKeyword" autofocus="autofocus" onfocus="this.value=''">
			<button type="submit" class="SearchBtnSearch">검색</button>
		</form>
	</div>
	<div id="article-wrap">
		<div id="related-word" class="RelatedSearch" style="display:none;"></div>
		<div id="search_result" class="SearchCount"></div>
		<dl id="sopListTable" class="Result"></dl>
		<dl id="themeListTable" class="Result"></dl>
		<dl id="wordTable" class="Result"></dl>
		<dl id="communityTable" class="Result"></dl>
	</div>
</body>
</html>

/*reset*/
* {
  margin: 0;
  padding: 0; }

li {
  list-style: none; }

table {
  border-collapse: collapse;
  border-spacing: 0;
  width: 100%; }

.info {
  margin-bottom: 0;
  text-align: left; }

/* =====================
    common
===================== */
.wrap {
  width: 1200px;
  margin: 0 auto; }

.inner {
  padding: 3% 5%; }

.line {
  padding-top: 6px;
  border-top: 3px solid #333333; }

.col-2 {
  display: flex;
  justify-content: space-between;
  margin-bottom: 35px; }

.col-2 > * {
  width: calc((100% - 20px) / 2); }

table {
  border-top: 1px solid #999999; }

table th, table td {
  padding: 5px 3px;
  border-bottom: 1px solid #999999;
  font-size: 13px;
  text-align: center;
  letter-spacing: -0.7px; }

table th:not(:first-child), table td:not(:first-child) {
  border-left: 1px solid #999999; }

table th {
  background-color: #f7f7f7;
  font-weight: 400; }

/* =====================
    header
===================== */
.header {
  position: relative;
  padding: 15px 10px;
  background-color: #133761; }

.header .logo {
  position: absolute;
  top: 50%;
  transform: translateY(-50%); }

.header .logo img {
  width: 70px; }

.header h1 {
  color: #fff;
  font-size: 18px;
  text-align: center; }

/* =====================
    wrap
===================== */
.map {
  margin-top: 10px;
  margin-bottom: 26px; }

.map h2 {
  padding: 8px 0;
  background-color: #575757;
  color: #fff;
  font-size: 16px;
  text-align: center; }

.map__con {
  border: 1px solid #dcdcdc;
  border-top: none; }

.chart__tit {
  margin-bottom: 3px;
  font-size: 18px; }

.chart__con {
  margin-bottom: 10px; }

.chart__table tr.on td {
  background-color: #ecf2fc; }

.chart__table th, .chart__table td {
  line-height: 1; }

.chart__table td {
  padding: 5px 15px;
  text-align: right; }

.chart__table td:first-child {
  text-align: center; }

.chart h4 {
  margin-bottom: 5px;
  padding: 3px 0;
  background-color: #f7f7f7;
  color: #707070;
  font-size: 15px;
  font-weight: 400;
  text-align: center; }

.info {
  background-color: #f7f7f7; }

.info h4 {
  height: 28px;
  line-height: 28px;
  padding-left: 35px;
  background-image: url(../img/map/img_info.png);
  background-repeat: no-repeat;
  background-size: 28px 26px;
  font-size: 15px; }

.info ul {
  padding-left: 35px; }

.info li {
  position: relative;
  padding-left: 5px;
  font-size: 13px; }

.info li::before {
  content: '';
  position: absolute;
  top: 6px;
  left: 0;
  width: 2px;
  height: 2px;
  border-radius: 50%;
  background-color: #000; }

/* =====================
    footer
===================== */
.footer {
  padding: 0;
  background-color: #333333; }

.footer > div {
  display: flex;
  align-items: center;
  padding: 2%; }

.footer .logo {
  width: 100px;
  margin-right: 30px; }

.footer .logo img {
  width: 100%; }

.footer p {
  color: #fff;
  font-size: 14px;
  opacity: 0.5; }

.footer a {
  color: #fff;
  opacity: 0.5;
  text-decoration: none; }


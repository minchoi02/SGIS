/*토글버튼관련*/
label[data-form-control=toggle] {
	display: inline-block;
	position: relative;
	width: 50px;
	height: 25px;
	margin: 0;
	padding: 0;
	border-radius: 15px;
	background-color: #ddd;
	line-height: 16px;
	cursor: pointer;
	-webkit-transform: translate3d(0, 0, 0);
	transform: translate3d(0px, 0px, 0px);
}
.pull-right {
	float: right !important;
}
label[data-form-control=toggle] input[type=checkbox] {
	margin-left: -9999px;
	cursor: pointer;
}
input[type=checkbox]:focus, input[type=file]:focus, input[type=radio]:focus {
	outline: -webkit-focus-ring-color auto 5px;
	outline-offset: -2px;
}
input[type=checkbox], input[type=radio] {
	margin: 4px 0 0;
	line-height: normal;
}
label[data-form-control=toggle] input[type=checkbox]:checked + [data-form-decorator=before] {
 background-color: #2b69b3;
 cursor: pointer;
}
 label[data-form-control=toggle] input[type=checkbox] + [data-form-decorator=before] {
 position: absolute;
 top: 0px;
 left: 0px;
 right: 0px;
 bottom: 0px;
 border-radius: 15px;
 background-color: #ddd;
 -webkit-transition: background-color .35s;
 transition: background-color .35s;
 cursor: pointer;
}
 *, ::after, ::before {
 -webkit-box-sizing: border-box;
 -moz-box-sizing: border-box;
 box-sizing: border-box;
}
 label[data-form-control=toggle] input[type=checkbox]:checked + [data-form-decorator=before] [data-form-decorator=after] {
 left: 50%;
 right: 0px;
 cursor: pointer;
}
 label[data-form-control=toggle] input[type=checkbox] + [data-form-decorator=before] [data-form-decorator=after] {
 position: absolute;
 top: 0px;
 left: 0px;
 right: 50%;
 bottom: 0px;
 border-radius: 13px;
 margin: 2px;
 box-shadow: 0px 3px 5px rgba(0, 0, 0, 0.25);
 background: #fff;
 -webkit-transition: all .35s;
 transition: all .35s;
 cursor: pointer;
}

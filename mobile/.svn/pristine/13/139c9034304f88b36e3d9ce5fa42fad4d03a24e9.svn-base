/*페이징 영역 시작*/
.Pasing {
	text-align:center;
	padding-top:10px;
}
.Pasing a {
	border-top:#999 solid 1px;
	border-bottom:#999 solid 1px;
	border-left:#ccc solid 1px;
	display:inline-block;
	height:34px;
	width:34px;
	padding-top:11px;
	box-sizing:border-box;
	letter-spacing:-3px;
	font-size:11px;
}
.Pasing span {
	border-top:#999 solid 1px;
	border-bottom:#999 solid 1px;
	border-left:#ccc solid 1px;
	display:inline-block;
	height:34px;
	padding-top:9px;
	width:34px;
	box-sizing:border-box;
	font-size:13px;
}
.Pasing a:first-child {
	border-left:#999 solid 1px;
}
.Pasing a:last-child {
	border-right:#999 solid 1px;
}
.Pasing a.PasingOff {
	color:#CCC;
}

	background-color:#E5E7EB;
	border-top-right-radius: 5px;
	border-bottom-right-radius: 5px;
}

.arrowCol::after {
	content: "";
    display: block; 
    position: absolute;
    left: 34.5%;
    border-style: solid;
    border-width: 10px; 
    border-color: transparent transparent transparent #E5E7EB; 
}


/* table */
table.TableSt { width:100%; margin-bottom:20px; font-size:13px; border-collapse:collapse;}
table.TableSt caption { text-align:left;font-family:"NanumSquareB"; color:#333; padding-bottom:5px;}
table.TableSt th { background:#f4f4f4; padding:5px 0; border:#ddd solid 1px; width:50%;}
table.TableSt td { padding:5px 10px; border:#ddd solid 1px;font-family: 'tahoma', 'NanumSquare';}
table.TableSt td:first-child { text-align:center;}
table.TableSt td:last-child { text-align:right;}

/* 설명 */
.desc_txt{ font-size: 11px;text-align: right;margin-right: 20px;padding-bottom: 10px;}

/* 출처 */
.origin_txt{ color: #999;font-size: 11px;text-align: right;margin-right: 20px;bottom: 15px; }

.tab {display: inline-block; width:100%; font-family:"NanumSquareB"; font-size:12px;}
.tab li{display:inline-block; float:left; height:30px; box-sizing:border-box; text-align:center; padding:8px 15px 0;  border-bottom:1px solid #ccc; border-left:#ccc solid 1px; border-top:1px solid #ccc; background:#f4f4f4; }
.tab li:first-child{border-left:none;}
.tab li.on{background:#fff; border-bottom:none 0; font-family:"NanumSquareB";}
.tab li.on a{color:#457bc3;}

.tab_new {display: inline-block; float: left; height:30px; box-sizing:border-box; text-align: center; padding:8px 15px 0; border-bottom:1px solid #ccc; border-right:#ccc solid 1px; border-top:1px solid #ccc; background: #f4f4f4; }
.tab_new.M_on {background:#fff; border-bottom:none 0; font-family:"NanumSquareB"; color:#457bc3;}
.tab_new a:first-child{border-left:none;}
.tab_new a {vertical-align: middle;}

/* 인구 성별 */
#gender-pie-chart1-male { top: 106px;}
#gender-pie-chart1-female { top: 106px;}
#gender-pie-chart2-male { top: 300px;}
#gender-pie-chart2-female { top: 300px;}
#gender-pie-chart3-male { top: 486px;}
#gender-pie-chart3-female { top: 486px;}

/* 사업체 */
.swiper_m_on {}
.swiper_m_on span { font-weight:bold; color:#000;}
.swiper_m_on img { border:#36c solid 2px !important; border-radius:10px;}
.swiper-container{box-sizing: border-box;padding: 0 50px 0 50px;}
.swiper-slide{width: 33% !important;margin-right: 10px !important;}
.swiper-button-next,.swiper-button-prev{margin-top:-32px !important;}
.theme_icon{float: left;width: 100%;line-height: 11px;font-size: 11px;color: #999999;text-align: center;display: block;}
.theme_icon em{padding-bottom: 5px;display: block;width: 100%;text-align: center;}
.theme_icon em img { width:23px; height:23px; padding:3px; border:#fff solid 2px;}
.Btn_viewchange { position:fixed; bottom:32px; right:10px; opacity:0.7;z-index: 1001;}
.Btn_viewchange img { width:26px; height:26px;}
#areainfo-4-incdec a.M_on { background-color: #052a67; background-repeat: no-repeat; background-position: center 7px, 2px 2px, left top; background-size: 21px, auto, auto;}
#areainfo-4-incdec a { width: calc(100%/4); height: 40px; line-height: 40px; padding: 0;}
#areainfo-4-incdec a { background: #666 no-repeat center 7px; background-size: 35px; border-left: #828282 solid 1px; display: inline-block; height: 40px; width: 24%; color: #ccc; font-family: "NanumSquareB"; font-size: 11px; text-align: center; letter-spacing: -1px; overflow: hidden;}



/* 커뮤니티 상세정보 */
.DetailInfo { position:absolute; left:0; top:42px; width:100%; height:120%; z-index:10000; background:#fff; box-sizing:border-box;}

.DetailInfo .cm_newwin {z-index:10001;}
.DetailInfo .cm_newwin h3 {padding:9px 0 9px 16px; font-family: "NanumSquareB"; font-size: 13px; color:#fff; background:#457bc3;}
.DetailInfo .cm_newwin .btnclose {position: absolute; right:15px; top:4px; overflow: hidden; background: url(../../images/common/icon_close.png) no-repeat center center; width:24px; height:24px; background-size:22px; border: none 0; text-indent: -1000px; }

.cm_newwin_cont .InfoBox { position:relative; min-height:150px; padding:15px; box-sizing:border-box;}
.cm_newwin_cont .InfoBox dt { font-size:16px; font-weight:600; color:#36c; margin-bottom:10px;}
.cm_newwin_cont .InfoBox .SubInfo{border-top:2px solid #d1d1d1; border-bottom:1px solid #d1d1d1; padding:10px 0 15px}
.cm_newwin_cont .InfoBox dd { font-size:14px; color:#666;}
.cm_newwin_cont .InfoBox dd>span { display:block;}
.cm_newwin_cont .InfoBox dd .date { font-size:12px; color:#999;margin-bottom:5px;text-align: right;}
.cm_newwin_cont .InfoBox dd .add { margin-bottom:5px; padding-left:40px; position:relative;clear:both;}
.cm_newwin_cont .InfoBox dd .add strong {position:absolute; left:0; top:0; display:inline-block; width:35px; background:#3366cc; color:#fff;  text-align:center; border-radius:3px; padding:3px 0; font-size:14px;}
.cm_newwin_cont .InfoBox dd .comment {padding-left:40px; position:relative;clear:both; margin-top:12px;}
.cm_newwin_cont .InfoBox dd .comment strong {position:absolute; left:0; top:0; display:inline-block; width:35px; background:#3faa04; color:#fff; text-align:center; border-radius:3px; padding:3px 0; font-size:14px;}
.cm_newwin_cont .InfoBox dd img { max-width:300px; margin:0 10px 10px 0;}
.cm_newwin_cont .InfoBox dd button.report { height:25px; box-sizing:border-box; padding:0 10px 0 19px; border:#ccc solid 1px; background:url(../../images/community/icon_report.png) no-repeat 5px 5px; background-size:12px;  font-size:12px; color:#333; font-weight:600; border-radius:3px;}
.cm_newwin_cont .InfoBox dd button span { display:inline-block; font-size:12px; color:#C00; vertical-align:middle;}
.cm_newwin_cont ol{background:#f1f1f1; border-radius:3px; box-sizing:border-box; padding:5px 10px; margin:0 15px;}
.cm_newwin_cont ol li{display:block; border-bottom:1px dotted #d1d1d1; padding:5px 0; }
.cm_newwin_cont ol li label{display:inline-block; width:70px; color:#000; font-family: "NanumSquareB"; background:url(../../images/common/list_bullet.png) no-repeat 0 center; background-size:2px; padding-left:7px;}
.cm_newwin_cont ol li input[type=text], .cm_newwin_cont ol li input[type=password]{text-align:left; width:60%; height:22px;}
.cm_newwin_cont ol li button{width:18%; background: #7799b8; color:#fff; padding:6px 10px;}
.cm_newwin_cont ol li:last-child{border-bottom:none;} 

.CommentBox1 {margin:0 15px;}
.CommentBox1 h5 {background:#4f6280; font-size:14px; font-weight:600; color:#fff; padding:8px 15px;}
.CommentBox1 h5 span { font-size:12px;}
.CommentBox1 .comment_write {background:rgba(0, 0, 0, 0.8); border-top:none; padding:7px 79px 7px 20px; position:fixed; width:100%; box-sizing:border-box; left:0; bottom:0; margin-bottom:0; z-index:100000}

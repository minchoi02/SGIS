.CommentBox1 ul li button { background: no-repeat center center; width:15px; height:15px; text-indent:-1000px; overflow:hidden; vertical-align:middle; border:none;}
.CommentBox1 ul li button.modify { background-image:url(../../images/community/comment_modify.png);}
.CommentBox1 ul li button.delete { background-image:url(../../images/community/comment_delete.png);}

@charset "euc-kr";
body.full .Community_Insert>.Community_cont,body.full .Community_Insert>.Community_map{display: none;}
/*커뮤니티맵*/
.ContArea.Community_intro, .ContArea.Community_map, .ContArea.Community_Insert { padding-left:0; padding-right:0;}
.ContArea.Community_Insert{margin-top:-15px;}
.ContArea.Community_intro div, .ContArea.Community_intro ul, .ContArea.Community_intro li { box-sizing:border-box;}


.HotCommunity { min-height:165px; background:#e8f3f7;  position:relative; border-bottom:#003366 solid 2px;}
.HotCommunity h2 { background:url(../../images/community/icon_hot.png) no-repeat left top; width:45px; background-size:100%; text-indent:-1000px; overflow:hidden; position:absolute; left:25px; top:42px;z-index:2;}
.HotCommunity .Image { position:absolute; left:25px; top:30px; width:120px; height:90px; background: url(../../images/community/img_no.png) no-repeat center center; background-size:100% 100%; border-radius:5px; z-index:1; text-indent:-1000px; overflow:hidden;border:1px #ccc solid;background-color: #fff;}
.HotCommunity .List { padding:25px 25px 0 160px; }
.HotCommunity .List .Info { margin-bottom:10px;}
.HotCommunity .List dt a { font-size:18px; color:#333; font-weight:bold; margin-bottom:3px; display:inline-block;}
.HotCommunity .List .subtitle { font-size:14px; color:#666;margin-bottom:2px;}
.HotCommunity .List .InfoBasic { font-size:11px; color:#999; display:block; margin-bottom:5px;}
.HotCommunity .List .Member { font-size:12px; color:#333; display:inline-block; margin-bottom:5px; font-weight:600; margin-right:5px;height:18px;}
.HotCommunity .List .Member strong { display:inline-block; font-weight:normal; background:#36c; box-sizing:border-box; height:18px; text-align:center; color:#fff; margin-right:1px; padding:0 7px 0 7px; border-radius:12px;}
.HotCommunity .List .Count { font-size:12px; color:#333; display:inline-block; text-align:right; font-weight:600; height:18px;}
.HotCommunity .List .Count strong { display:inline-block; font-weight:normal; background:#3faa04; box-sizing:border-box; height:18px; text-align:center; color:#fff;  margin-right:1px; padding:0 7px 0 7px; border-radius:12px; }

.HotCommunity .btn_forward { width:30px; height:42px; background:url(../../images/community/community_arrow_left.png) no-repeat left top; background-size:100%; border:none; text-indent:-1000px; overflow:hidden; position:absolute; left:0; top:55px; opacity:0.5; z-index:3;}
.HotCommunity .btn_next { width:30px; height:42px; background:url(../../images/community/community_arrow_right.png) no-repeat left top; background-size:100%; border:none; text-indent:-1000px; overflow:hidden; position:absolute; right:0; top:55px; opacity:0.5; z-index:3;}

.HotCommunity .Number { text-align:center; margin-bottom:10px;}
.HotCommunity .Number a { text-indent:-1000px; overflow:hidden; display:inline-block; margin:0 8px; width:10px; height:10px; background:#b6d1e7; border-radius:6px; box-shadow:inset 0px 2px 3px #709cbf;}
.HotCommunity .Number a.M_on {background:#063981;box-shadow:0px 2px 3px #709cbf;}

.CommunityList .Tab {position:fixed; left:0; bottom:0; height:35px; background:rgba(0, 0, 0, 0.8); width:100%; padding-left:15px; box-sizing:border-box; overflow:auto; z-index:101;}
.CommunityList .Tab a {display:inline-block; float:left; position:relative; width:auto; padding:3px 0 0 22px; line-height:35px; box-sizing:border-box;  font-size:14px; font-weight:600;}
.CommunityList .Tab a span {color:#ffda84; padding:0 0 0 8px; }
.CommunityList .Tab a:first-child {border-left:none;}
.CommunityList .Tab a.M_on { background:url(../../images/icon/icon_check.png) no-repeat left center; background-size:18px; color:#fff;}

.CommunityList .List { padding:0px 20px 0 20px;  overflow:auto; }
.CommunityList .List dl {width:100%; background:#fff; padding-left:130px; position:relative; margin-bottom:5px; padding:10px 0 15px 0; height:95px; border-bottom:none; box-sizing:border-box;}
.CommunityList .List dl:after { width:100%; height:100%; content:''; display:block; position:absolute; left:0; top:0; border:#000 solid 1px; opacity:0.15; box-sizing:border-box;}
.CommunityList .List dl.Hot:before { content:''; background:url(../../images/community/icon_hot.png) no-repeat left top;background-size:100%; display:block; position:absolute; left:0; top:0; z-index:100; width:40px; height:40px;}
.CommunityList .List dl.lock:before {content: '';display: block;width: 31px;height: 31px;background:url(../../images/community/icon_lock.png) no-repeat left top;position: absolute;left:5px;top:5px;z-index: 100;}
.CommunityList .List dt a {width:100%; line-height:18px; padding:0 15px 0 145px; box-sizing:border-box; max-height:36px; font-family: "NanumSquareB"; font-size:15px; color:#222; font-weight:600; margin-bottom:4px; display:inline-block;}
.CommunityList .List dt img { vertical-align:middle; margin-left:10px; width: 44px;}
.CommunityList .List dd {padding:0 15px; box-sizing:border-box;}
.CommunityList .List .subtitle { position:absolute; width:0; height:0; overflow:hidden; font-size:12px; color:#666;margin-bottom:3px; }
.CommunityList .List .InfoBasic { position:absolute; margin:-20px 0 0 0; font-size:11px; color:#999; display:block; }
.CommunityList .List .Member { font-size:12px; color:#333; display:inline-block; font-weight:600; height:18px; padding-top:5px;}
.CommunityList .List .Info {position:absolute; width:calc(100% - 130px); right:0; bottom:0; background:#f4f4f4; height:25px; padding-left:15px; box-sizing:border-box;  margin-bottom:0px;}
.CommunityList .List .Member strong {background:none; color:#333; padding:0; height:auto; font-family: "NanumSquareB"; display:inline-block; font-weight:normal; box-sizing:border-box; text-align:center; margin-right:1px; border-radius:12px;}
.CommunityList .List .Count {font-size:12px; color:#333; display:inline-block; text-align:right; height:18px; padding-top:5px; margin-left:10px;}
.CommunityList .List .Count strong {background:none; color:#333; padding:0; height:auto; font-family: "NanumSquareB";  display:inline-block; box-sizing:border-box; text-align:center;  margin-right:1px; border-radius:12px; }
.CommunityList .List .Image {width:130px; height:95px; position:absolute; left:0; top:0; border-radius:0; border:none;background:url(../../images/community/img_no.png) no-repeat center center; background-size:100% 100%; text-indent:-1000px; overflow:hidden; padding-left:0;}
.CommunityList .List .Image:after { content:''; width:1px; height:calc(100% - 2px); background:#000; opacity:0.15; position:absolute; right:0; top:1px;}

.CommunityList .List ul a.NoData { text-align:center; background:url(../../images/community/icon_nodata.png) no-repeat center 15px; padding:45px 0 10px 0; background-size:30px;}

.ListSearch_cm { font-size:16px; position:absolute; width:100%; left:0; bottom:0;  background:#3e5473; font-size:12px;  padding:10px 65px 5px 10px;color:#fff;font-family:"NanumSquare"; text-align:center; height:48px; z-index:101; }
.ListSearch_cm input[type=text] { border:#fff solid 1px; padding:6px; width:100%; vertical-align:middle;height:29px; box-sizing:border-box;}
.ListSearch_cm label { width:0; height:0; overflow:hidden; display:inline-block;}
.ListSearch_cm button { height:29px; width:50px; vertical-align:middle; color:#fff; background:#000;font-family:"NanumSquareB"; position:absolute; right:10px; top:10px; border:#999 solid 1px;}

.CommunityList .List .Pasing { padding-top:20px;}

/* 커뮤니티 간략정보 */
.MapInfo { padding:18px 20px 15px 140px; position:relative; box-sizing:border-box; min-height:145px; background:#e8f3f7;}
.MapInfo .Image { display:block; width:110px; height:80px;  position:absolute; left:20px; top:20px; background:url(../../images/community/img_no.png) no-repeat center center;background-size:100% 100%; border-radius:5px; text-indent:-1000px; overflow:hidden;border:1px #ccc solid;}
.MapInfo h3 { font-size:18px; font-weight:600; color:#333; }
.MapInfo .subtitle { font-size:12px; color:#666; margin-bottom:2px;}
.MapInfo .detail { font-size:11px; color:#999; line-height:1.6; margin-bottom:8px;}
.MapInfo .detail img { vertical-align:middle;}
.MapInfo button {background:#7799b8; width:auto; border:none; border-radius:3px; box-sizing:border-box; height:34px; width:100%;  color:#fff; font-size:14px; font-weight:600; }
.MapInfo .btn_insert {}
.MapInfo .btn_register {}
.MapInfo .Social { position:absolute; left:20px; top:110px;}
.MapInfo .Social a { display:inline-block; width:25px; height:25px; vertical-align:top; margin-right:2px; background:url(../../images/icon/sns_fb.png) no-repeat left top; background-size:100% 100%; text-indent:-1000px; overflow:hidden; float:left; }
.MapInfo .Social a.sns_tw { background-image:url(../../images/icon/sns_tw.png);}
.MapInfo .Social a.sns_bl { background-image:url(../../images/icon/sns_bl.png);}

.Community_map .ListSearch { font-size:16px; background:#3e5473; font-size:12px;  padding:9px 65px 5px 10px;color:#fff;font-family:"NanumSquare"; text-align:center; height:48px; position:relative; box-sizing:border-box;}
.Community_map .ListSearch input[type=text] { border:#fff solid 1px; padding:6px; width:100%; vertical-align:middle;height:29px; box-sizing:border-box; margin-bottom:0;}
.Community_map .ListSearch label { width:0; height:0; overflow:hidden; display:inline-block;}
.Community_map .ListSearch button { height:29px; width:50px; vertical-align:middle; color:#fff; background:#000;font-family:"NanumSquareB"; position:absolute; right:10px; top:9px; border:#999 solid 1px;}

.CommunityList .List ul { padding:0 0px; margin-bottom:15px; }
.CommunityList .List ul a { border-bottom:#eaeaea solid 1px; background:no-repeat left center; display:block; box-sizing:border-box; position:relative; padding:10px 0 10px 30px;}
.CommunityList .List ul a .title {font-size:14px; color:#457bc3; font-weight:600; display:block; margin-bottom:3px; font-family:"NanumSquareB"; letter-spacing:-1px;}
.CommunityList .List ul a .position {font-size:12px; color:#555; display:block;}
.CommunityList .List ul a .date { font-size:11px; color:#999; top:12px; right:5px; position:absolute;}
.CommunityList .List ul a .comment { font-size:11px; color:#999;}

.Btn_Group { text-align:center; /* position:fixed; */ left:0; bottom:0; width:100%; background:#999; padding:10px 0;}
.Btn_Group button { box-sizing:border-box; width:30%; border:#999 solid 1px; background:#fff; height:29px; font-family:"NanumSquareB"; box-shadow:0px 3px 5px #888;}

.Community_Insert h1 {padding:9px 0 9px 16px; font-family: "NanumSquareB"; font-size: 13px; color:#fff; background:#457bc3;}
.Community_Insert .btn_back {position:absolute; right:12px; top:50px; font-size:0}
.Community_Insert .btn_back:before {content:''; display:block; width:12px; height:12px; border:#fff solid 1px; border-top:none; border-left:none; transform:rotate(135deg); position:absolute; right:5px; top:2px;}

.Community_maparea { background:#999;}

.InsertForm {background:#dae8f4; padding:15px; margin-top:-5px;}
.InsertForm h2 {font-size:14px; color:#000; font-family:'NanumSquareB'; display:block;}
.InsertForm .Insert_Position {}
.InsertForm .Insert_Position button { width:32%; height:40px; border:none; background:#7799b8;font-size:14px; color:#fff; ont-family:'NanumSquare'; float:left; margin-left:2%; margin-bottom:10px;}
.InsertForm .Insert_Position button.M_on {background: #457bc3;}
.InsertForm .Insert_Position button:first-child { margin-left:0;}
.InsertForm input[type=text],.InsertForm input[type=password] { height:36px; width:100%; border:#ccc solid 1px; box-sizing:border-box; margin-bottom:15px;}
.InsertForm label.label {font-family:'NanumSquareB'; color:#333; font-size:14px; margin-bottom:3px; display:block;}

.InsertForm .Insert_File div { position:relative; padding:0 94px 0 0;}
.InsertForm .Insert_File div button { height:36px; width:95px; background:#999; border:#807e7e solid 1px; position:absolute; right:0; top:0;font-family:'NanumSquare'; color:#fff; font-size:14px;}
.InsertForm .Insert_File ul { margin-bottom:15px;}
.InsertForm .Insert_File li { background:url(../../images/community/icon_warning.png) no-repeat left 6px; background-size:8px; padding:0 0 0 12px;}
.InsertForm .file-list { height: 145px;border: 1px solid #ccc;padding: 10px;margin-bottom: 50px;}
.InsertForm .file-list li{ clear:both;height:30px;}
.InsertForm .file-list li>div{ overflow: hidden;white-space: nowrap;text-overflow: ellipsis;width: 80%;float: left;}
.InsertForm .file-list li>button{ position: absolute;right: 30px;}

.btn_finish {width:155px; height:40px; font-size:15px; color:#fff; padding:7px 25px; font-family:"NanumSquareB"; background:#457bc3; margin:0 auto;}



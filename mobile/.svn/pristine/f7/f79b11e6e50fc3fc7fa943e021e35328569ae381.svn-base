/* 커뮤니티 기본정보 */
.BasicInfo { position:absolute; border:#333 solid 1px; border-radius:8px; background:#fff; box-shadow:#000 0 3px 10px -4px; width:200px;  box-sizing:border-box; padding:10px;}

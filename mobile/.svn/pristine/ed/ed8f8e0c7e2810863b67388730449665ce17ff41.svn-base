.spbox.type05 {background: #f6c34a;}
.spbox.type06 {background: #7cc576;}
.spbox.type07 {background: #f26d7d;}
.spbox.type08 {background: #8781bd;}
.spbox.type09 {background: #c29561;}
.spbox.type10 {background: #9e9e9e;}


@media screen and (max-width: 360px) {
  .default-select__circle li {
    width: 38px;
    height: 38px;
    line-height: 38px;
  }

  .life-industry dl button {
    width: 50px;
  }
  .life-industry .point__radio p {
    font-size: 11px;
    letter-spacing: -1px;
  }
  .life-industry__table th, .life-industry__table td {
    padding: 8px 5px;
  }
  .life-industry__detail p {
    font-size: 13px;
  }
}
